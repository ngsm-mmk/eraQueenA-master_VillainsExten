@CHARATITLE_SETTING

PRINTL 请选择称号的显示方式。
;PRINT 现在的显示方式是【
;	IF (FLAG:2 & 4) == 0
;		PRINT 不
;	ENDIF
;PRINTL 显示】。
PRINTL [  0] - 不显示
PRINTL [  1] - 显示
PRINTL [  2] - 将获得的称号初始化
PRINTL [100] - 返回

$INPUT_LOOP
INPUT
IF RESULT == 0
	PRINTFORMW 不显示称号
	SIF FLAG:2 & 4
		FLAG:2 -= 4
ELSEIF RESULT == 1
	PRINTFORMW 显示称号
	FLAG:2 |= 4
ELSEIF RESULT == 2
	PRINTFORML 要将谁的称号初始化？
	REPEAT CHARANUM
			PRINTFORML [{COUNT}] - %NAME:COUNT%
	REND
	
	PRINTL [100] - 全员
	PRINTL 
	PRINTL [101] - 返回

	$INPUT_LOOP2
	INPUT
	IF RESULT <= CHARANUM
		;ループでTALENTを0にする
		X = RESULT
		CALL CHARATITLE_RESET
		PRINTFORMW %NAME:X%的称号已初始化。
	ELSEIF RESULT == 100
		REPEAT CHARANUM
			X = COUNT
			CALL CHARATITLE_RESET
			COUNT = X
		REND
	PRINTW 全员称号已初始化。
	ELSEIF RESULT == 101
		RETURN 0
	ELSE
		GOTO INPUT_LOOP2
	ENDIF


ELSEIF RESULT == 100
	RETURN 0

ELSE
	GOTO INPUT_LOOP
ENDIF
