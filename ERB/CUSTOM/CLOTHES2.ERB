SIF T:3 == 0
	T:3 = 1
;男はブラをしない
SIF TALENT:137 || TALENT:140
	T:3 = 0

;全身外衣をつけていない場合は上衣をつける
	;上身外衣枠
	;上身外衣无の場合、ただの上衣を
	SIF T:5 == 0
		T:5 = 1
;----------------------------------------------------
	;下身外衣枠
	;女には水手裙をつける。
	IF TALENT:137 || TALENT:140
	ELSE
		T:4 = 1
	ENDIF
