		ENDIF
		T:10 = 0
		CALL ACCESSORY_SET
		;领结と领带と帽子取下
		T:10 -= T:10 & 16
		T:10 -= T:10 & 8
		T:10 -= T:10 & 2
	;かっちり军装
	ELSEIF Z == 8
