		CALL CLOTHES_SET3
		FLAG:1 = C:2
	;連合ポリス系
	ELSEIF Z == 9
		T:0 = 0
		T:1 = 1
		T:2 = 1
		SIF TALENT:137 || TALENT:140
			T:2 = 2
		T:3 = 4
		T:4 = 2
		T:5 = 9
		T:6 = 0
		T:7 = 0
		T:9 = 1
		IF TEQUIP:8 == 1
			T:8 = 1
		ELSE
			T:8 = 0
		ENDIF
		T:10 = 0
		CALL ACCESSORY_SET
		;领结と帽子取下
		T:10 |= 8
		T:10 -= T:10 & 16
		T:10 -= T:10 & 2
	;ミニスカポリス
	ELSEIF Z == 10
		T:0 = 0
		T:1 = 4
		T:2 = 1
		SIF TALENT:137 || TALENT:140
			T:2 = 2
		T:3 = 0
		SIF TALENT:137 == 0 && TALENT:140 == 0
			T:3 = 1
		SIF TALENT:TARGET:109 || TALENT:TARGET:110
			T:3 = 2
		T:4 = 1
		T:5 = 9
		T:6 = 0
		T:7 = 0
		T:9 = 9
		IF TEQUIP:8 == 1
			T:8 = 1
		ELSE
			T:8 = 0
		ENDIF
		T:10 = 0
		CALL ACCESSORY_SET
		;领结と帽子取下
		T:10 |= 8
		T:10 -= T:10 & 16
		T:10 -= T:10 & 2
	;海賊服：女の子は男装で
	ELSEIF Z == 11
		T:0 = 0
		T:1 = 1
		T:2 = 2
		T:3 = 4
		T:4 = 2
		T:5 = 17
		T:6 = 0
		T:7 = 0
		T:9 = 4
		T:10 = 0
		CALL ACCESSORY_SET
		;领带を外して领结と帽子を付ける
		T:10 -= T:10 & 8
		T:10 |= 2
		T:10 |= 16
	ENDIF
	RETURN
ELSEIF Z == 100
	RETURN 1
ELSE
	GOTO INPUT_LOOP_2
ENDIF




