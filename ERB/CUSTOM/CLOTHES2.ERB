		T:10 -= T:10 & 16
		T:10 -= T:10 & 8
		T:10 -= T:10 & 2
	;きっちり和装
	ELSEIF Z == 7
		T:0 = 0
		T:1 = 2
		T:2 = 1
		SIF TALENT:137 || TALENT:140
			T:2 = 2
		T:3 = 8
		T:4 = 0
		T:5 = 0
		T:6 = 1
		T:7 = 0
		T:9 = 5
		IF TEQUIP:8 == 1
			T:8 = 1
		ELSE
