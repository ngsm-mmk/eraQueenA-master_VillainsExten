	TALENT:不易湿 = 1
ELSEIF D == 3 || D == 5
	TALENT:容易湿 = 1
ENDIF
;包茎
D = (A + A + B + B - C) % 6
SIF TALENT:矮小体型 || TALENT:伪娘
	D = (B + C) % 3
SIF TALENT:幼稚
	D = C % 2
SIF D == 1
	TALENT:包茎 = 1
;容易自慰　５分の一
D = (A*B*2) % 5
SIF D == 2
	TALENT:容易自慰 = 1
;シャッフルしてみる
IF A > B
	D = A
	A = B
	B = D
ENDIF
;サイズ決め
D = ((B + C) * A) % 5
IF D == 2
	TALENT:短小 = 1
ELSEIF D == 0
	TALENT:巨根 = 1
	IF (A % 4) == 3
		TALENT:巨根 = 0
		TALENT:尺寸傲人 = 1
	ENDIF
ENDIF
;Ｃ感度
D = ((A + C) * B ) % 7
IF D == 1
	TALENT:Ｃ钝感 = 1
ELSEIF D == 2 || D == 4
	TALENT:Ｃ敏感 = 1
ENDIF
;Ａ感度
D = ((B + C) * A) % 4
IF D == 2
	TALENT:Ａ钝感 = 1
ELSEIF D == 3
	TALENT:Ａ敏感 = 1
ENDIF
;Ｂ感度
D = (A + B - C) % 5
IF D == 1
	TALENT:Ｂ钝感 = 1
ELSEIF D == 4
	TALENT:Ｂ敏感 = 1
ENDIF
