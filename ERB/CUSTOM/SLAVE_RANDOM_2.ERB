K = TARGET

SIF !TALENT:精灵 && !TALENT:类人
    TALENT:化形 = 1
SIF TALENT:吸血鬼
    TALENT:化形 = 1

LOCAL:2 = 0
LOCAL:1 = RAND:8
LOCAL:1 += 5
WHILE 1
	$INPUT_LOOP_SHORI2
	LOCAL:2 += 1
	;随机输入
	$INPUT_AUTO_RESULT
	LOCAL = RAND:312
	;その素質が输入不可の場合は输入し直す
	SIF LOCAL < 54 || (LOCAL > 54 && LOCAL < 90) || (LOCAL > 90 && LOCAL < 142) || (LOCAL > 143 && LOCAL < 201) || (LOCAL > 238 && LOCAL < 307) || (LOCAL > 307 && LOCAL < 312)
		GOTO INPUT_AUTO_RESULT

	RESULT = LOCAL

	SIF LOCAL:2 >= LOCAL:1
		RESULT = 999

	IF RESULT == 54 || RESULT == 90 || RESULT == 143 || RESULT == 142 || (RESULT >= 201 && RESULT <= 238) || RESULT == 307 || RESULT == 312
		TALENT:K:RESULT = !TALENT:K:RESULT
        IF !TALENT:K:异形
            ;不是异形的话只能有一种羽翼
            IF RESULT == 201
                TALENT:K:202 = 0
                TALENT:K:203 = 0
                TALENT:K:204 = 0
            ELSEIF RESULT == 202
                TALENT:K:201 = 0
                TALENT:K:203 = 0
                TALENT:K:204 = 0
            ELSEIF RESULT == 203
                TALENT:K:201 = 0
                TALENT:K:202 = 0
                TALENT:K:204 = 0
            ELSEIF RESULT == 204
                TALENT:K:201 = 0
                TALENT:K:202 = 0
                TALENT:K:203 = 0
            ENDIF
            ;只能有一种角
            IF RESULT == 205
                TALENT:K:206 = 0
            ELSEIF RESULT == 206
                TALENT:K:205 = 0
            ENDIF
            ;只能有一种尾巴
            IF RESULT == 207
                TALENT:K:208 = 0
                TALENT:K:209 = 0
            ELSEIF RESULT == 208
                TALENT:K:207 = 0
                TALENT:K:209 = 0
            ELSEIF RESULT == 209
                TALENT:K:207 = 0
                TALENT:K:208 = 0
            ENDIF
            ;尖耳兽耳不共存
            IF RESULT == 143
                TALENT:K:215 = 0
            ELSEIF RESULT == 215
                TALENT:K:143 = 0
            ENDIF
		ENDIF
        IF TALENT:K:马身
            TALENT:K:鱼尾 = 0
            TALENT:K:蛇尾 = 0
        ENDIF
        SIF TALENT:K:鱼尾 || TALENT:K:蛇尾
            TALENT:K:马身 = 0
		;不是变色龙
		IF RESULT == 238
		    TALENT:K:307 = 0
		    TALENT:K:312 = 0
		ELSEIF RESULT == 307
		    TALENT:K:312 = 0
		    TALENT:K:238 = 0
		ELSEIF RESULT == 312
		    TALENT:K:307 = 0
		    TALENT:K:238 = 0
		ENDIF
		;体型不共存
		IF RESULT == 142
		    TALENT:K:224 = 0
		ELSEIF RESULT == 224
		    TALENT:K:142 = 0
		ENDIF
		;体质不共存
		IF RESULT == 226
		    TALENT:K:227 = 0
		    TALENT:K:228 = 0
		ELSEIF RESULT == 227
		    TALENT:K:226 = 0
		    TALENT:K:228 = 0
		ELSEIF RESULT == 228
		    TALENT:K:226 = 0
		    TALENT:K:227 = 0
		ENDIF
		;没有翅膀不存在Ｗ敏钝、不可以飞翔
		IF !TALENT:K:201 && !TALENT:K:202 && !TALENT:K:203 && !TALENT:K:204
		    TALENT:K:230 = 0
		    TALENT:K:231 = 0
		    TALENT:K:219 = 0
		ENDIF
		;没有兽耳和尖耳不存在Ｅ敏钝
		IF !TALENT:K:143 && !TALENT:K:215
		    TALENT:K:234 = 0
		    TALENT:K:235 = 0
		ENDIF
		;没有尾巴不存在Ｔ敏钝，不存在多尾
		IF !TALENT:K:207 && !TALENT:K:208 && !TALENT:K:209
		    TALENT:K:210 = 0
		    TALENT:K:232 = 0
		    TALENT:K:233 = 0
		ENDIF
		;敏感相悖
		IF RESULT == 230
		    TALENT:K:231 = 0
		ELSEIF RESULT == 231
		    TALENT:K:230 = 0
		ENDIF
		IF RESULT == 232
		    TALENT:K:233 = 0
		ELSEIF RESULT == 233
		    TALENT:K:232 = 0
		ENDIF
		IF RESULT == 234
		    TALENT:K:235 = 0
		ELSEIF RESULT == 235
		    TALENT:K:234 = 0
		ENDIF
		;非不死系、非异形不可以没有头
		SIF !TALENT:K:不死系 && !TALENT:K:异形
		    TALENT:K:237 = 0
		;类人无马身、多足
        IF TALENT:K:类人
		    TALENT:K:236 = 0
		    TALENT:K:多足 = 0
		ENDIF
		;非走兽无狐狸
        SIF !TALENT:K:走兽 && !TALENT:K:异形
		    TALENT:K:狐狸 = 0
	ELSEIF RESULT == 999
		BREAK
	ELSE
		GOTO INPUT_LOOP_SHORI2
	ENDIF
