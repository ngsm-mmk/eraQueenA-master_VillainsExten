	RETURN 0
;[漏尿癖]をもっていない
SIF !TALENT:A:漏尿癖
	RETURN 0
RETURN 1

;庭院でひなたぼっこ
@DAILY_LIFE_ABLE_7
;夜はダメ
SIF TIME
	RETURN 0
;未陷落は不可だけど共犯者はOK
SIF !KANRAKU(A) && CFLAG:A:1 != 3
	RETURN 0
;月光浴は不可
SIF TALENT:A:月光浴
	RETURN 0
;吸血鬼は不可
SIF TALENT:A:吸血鬼
	RETURN 0
RETURN 1

;屋根の上で笑っている
@DAILY_LIFE_ABLE_8
;昼はダメ
SIF !TIME
	RETURN 0
;未陷落は不可だけど共犯者はOK
SIF !KANRAKU(A) && CFLAG:A:1 != 3
	RETURN 0
;[妖狐][魅惑][谜之魅力][月光浴]をどれももっていない
SIF !TALENT:A:妖狐 && !TALENT:A:魅惑 && !TALENT:A:谜之魅力 && !TALENT:A:月光浴
	RETURN 0
RETURN 1

;兽耳/庭院で摇摇晃晃している
@DAILY_LIFE_ABLE_9
;未陷落は不可だけど共犯者はOK
SIF !KANRAKU(A) && CFLAG:A:1 != 3
	RETURN 0
;[失意]は不可
SIF TALENT:A:失意
	RETURN 0
;[兽耳]をもっていない
SIF !TALENT:A:兽耳
	RETURN 0
SIF TALENT:A:大叔 || TALENT:A:污臭敏感 || TALENT:A:兽耳 == 11 || TALENT:A:兽耳 == 12 || TALENT:A:兽耳 == 13
	RETURN 0
RETURN 1

;縫物
@DAILY_LIFE_ABLE_10
;擅长裁缝無いとだめ（奴隶は男がデフォで、普通は縫物しないと思うので）
SIF !TALENT:A:擅长裁缝
	RETURN 0
;未陷落は不可だけど共犯者はOK
SIF !KANRAKU(A) && CFLAG:A:1 != 3
	RETURN 0
;侍奉精神４以下かつ[自我奉献]が無いときは不可
SIF ABL:A:侍奉精神 <= 4 && !TALENT:A:自我奉献
	RETURN 0
RETURN 1

;扫除
@DAILY_LIFE_ABLE_11
;未陷落は不可だけど共犯者はOK
SIF !KANRAKU(A) && CFLAG:A:1 != 3
	RETURN 0
;侍奉精神４以下かつ[自我奉献]でないとき不可、喜欢扫除持ちならOK
IF TALENT:A:喜欢扫除
ELSEIF ABL:A:侍奉精神 <= 4 && !TALENT:A:自我奉献
	RETURN 0
ENDIF
RETURN 1

;洗濯
@DAILY_LIFE_ABLE_12
;未陷落は不可だけど共犯者はOK
SIF !KANRAKU(A) && CFLAG:A:1 != 3
	RETURN 0
;侍奉精神４以下かつ[自我奉献]でないとき不可、喜欢扫除持ちならOK
IF TALENT:A:喜欢扫除
ELSEIF ABL:A:侍奉精神 <= 4 && !TALENT:A:自我奉献
	RETURN 0
ENDIF
;夜は駄目
SIF TIME
	RETURN 0
RETURN 1

;お祈り
@DAILY_LIFE_ABLE_13
;共犯者は不可
;SIF CFLAG:A:1 == 3
;	RETURN 0
;信仰之心ないとだめ
SIF !TALENT:A:信仰虔诚
	RETURN 0
SIF TALENT:A:疯癫
	RETURN 0
RETURN 1

;ぼー
@DAILY_LIFE_ABLE_14
;[缺乏感情][无动于衷]をもっていない
SIF !TALENT:A:缺乏感情 && !TALENT:A:无动于衷
	RETURN 0
RETURN 1

;ダンシング
@DAILY_LIFE_ABLE_15
;未陷落は不可だけど共犯者はOK
SIF !KANRAKU(A) && CFLAG:A:1 != 3
	RETURN 0
;[失意]は不可
SIF TALENT:A:失意 || TALENT:A:嫉妒
	RETURN 0
;[喜欢引人注目]、ナルシーが無い＋踊りが得意ではないと不可
SIF !TALENT:A:喜欢引人注目 && CSTR:A:11 != "舞蹈" && CSTR:A:11 != "踊り" && CFLAG:A:600 != 12
	RETURN 0
;堅物、冷静は不可
SIF CFLAG:A:600 == 3 || CFLAG:A:600 == 8
	RETURN 0
RETURN 1

;夜にトイレに
@DAILY_LIFE_ABLE_16
;未陷落は不可だけど共犯者はOK
SIF !KANRAKU(A) && CFLAG:A:1 != 3
	RETURN 0
;[失意]は不可
SIF TALENT:A:失意
	RETURN 0
;[幼稚][幼儿／幼儿退行]のどちらも持っていないとき不可
SIF !TALENT:A:幼稚 && !TALENT:A:幼儿／幼儿退行
	RETURN 0
;主人が[胆怯][幼稚][幼儿／幼儿退行]のいずれかを持っているとき不可
SIF TALENT:MASTER:胆怯 || TALENT:MASTER:幼稚 || TALENT:MASTER:幼儿／幼儿退行
	RETURN 0
;昼間は不可
SIF !TIME
	RETURN 0
RETURN 1

;木に登っている
@DAILY_LIFE_ABLE_17
;性格タイプ：元気、甘えん坊、ショタ、不思議子限定
SIF CFLAG:A:600 != 1 && CFLAG:A:600 != 10 && CFLAG:A:600 != 11 && CFLAG:A:600 != 13
	RETURN 0
;未陷落は不可だけど共犯者はOK
SIF !KANRAKU(A) && CFLAG:A:1 != 3
	RETURN 0
;[失意]は不可
SIF TALENT:A:失意
	RETURN 0
;反抗刻印３あると不可
SIF MARK:A:反抗刻印 >= 3
	RETURN 0
;夜は駄目
SIF TIME
	RETURN 0
;喜欢运动OR性好奇がないと駄目
SIF !TALENT:A:喜欢运动 || !TALENT:A:性好奇
	RETURN 0
;大人はだめ
SIF TALENT:A:大人
	RETURN 0
RETURN 1

;手伝い
@DAILY_LIFE_ABLE_18
;未陷落は不可だけど共犯者はOK
SIF !KANRAKU(A) && CFLAG:A:1 != 3
	RETURN 0
;[失意]は不可
SIF TALENT:A:失意 || TALENT:A:疯癫
	RETURN 0
;反抗刻印３あると不可
SIF MARK:A:反抗刻印 >= 3
	RETURN 0
;[直率][反抗]自大、自尊心高、堅物のいずれかを持っていなければ不可
SIF !TALENT:A:直率 && !TALENT:A:反抗 && !TALENT:A:自大 && !TALENT:A:高傲 && CFLAG:A:600 == 3
	RETURN 0
RETURN 1

