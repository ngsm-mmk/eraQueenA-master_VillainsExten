	RETURN 0
;[化学属性を持っていない]
SIF !TALENT:A:化学属性
	RETURN 0
RETURN 1

;读书
@DAILY_LIFE_ABLE_5
;共犯者は不可
;SIF CFLAG:A:1 == 3
;	RETURN 0
SIF KANRAKU(A) && !TALENT:A:喜欢读书 && !(TALENT:A:幼稚 || TALENT:A:幼儿／幼儿退行)
	RETURN 0
RETURN 1

;おもらし
@DAILY_LIFE_ABLE_6
