;-------------------------------------------------
;日常イベント実行判定
;-------------------------------------------------
;部屋にいる
@DAILY_LIFE_ABLE_0
;共犯者は不可
SIF CFLAG:A:1 == 3
	RETURN 0
RETURN 1

;歌唱练习
@DAILY_LIFE_ABLE_1
;歌唱技能がない
SIF !ABL:A:歌唱技能
	RETURN 0
;暗い・無口・冷静タイプは歌好きじゃないと駄目
SIF CFLAG:A:600 == 8 && !TALENT:A:擅长歌唱
	RETURN 0
RETURN 1

;壁にブツブツ言う
@DAILY_LIFE_ABLE_2
;[疯癫]を持っていない
SIF !TALENT:A:疯癫
	RETURN 0
RETURN 1

;魔法っぽいことをしている
@DAILY_LIFE_ABLE_3
;未陷落は不可だけど共犯者はOK
SIF !KANRAKU(A) && CFLAG:A:1 != 3
	RETURN 0
;[失意]は不可
SIF TALENT:A:失意
	RETURN 0
;[魔法・占卜属性を持っていない]
SIF !TALENT:A:魔法／占卜属性
	RETURN 0
RETURN 1

;実験っぽいことをしている
@DAILY_LIFE_ABLE_4
