@CHARA_ROOM(ARG)
;跳出催眠返回
SIF 催眠返回
GOTO INPUT_HUDONG2
CALL KAIGYO
;每次进来时清空行动数
CFLAG:ARG:2415 = 0
DRAWLINE
A = 0
B = 0
C = 0
D = 0
E = 0
F = 0
G = 0
H = 0
IF CFLAG:ARG:2404 == 1
    PRINTW 门上锁了，换个时间再来看吧
    RETURN 0
ENDIF
;B是可能偷情的对象，D是相性好的对象，E是关系差的对象（特指嫉妒），F是其他恋慕角色，G是子女/父母
;决定B
REPEAT CHARANUM
    ;主人不可
    SIF COUNT == MASTER
        CONTINUE
    ;娼馆、出租劳役中
    SIF CFLAG:COUNT:12 > 0
        CONTINUE
    SIF CFLAG:COUNT:2404 == 1
        CONTINUE
    SIF CFLAG:COUNT:2414 == 1
        CONTINUE
    ;育儿室
    SIF  (TALENT:COUNT:怀孕 && CFLAG:COUNT:110 - 3 <= DAY) || TALENT:COUNT:育儿中
        CONTINUE
    SIF BASE:COUNT:0 < 300
        CONTINUE
    ;排除恋慕
    SIF TALENT:COUNT:3 || TALENT:COUNT:6
        CONTINUE
    SIF CFLAG:ARG:(NO:COUNT + 2000) == 29 && CFLAG:COUNT:(NO:ARG + 2000) == 29
        B = COUNT
REND
;决定D
REPEAT CHARANUM
    ;主人不可
    SIF COUNT == MASTER || COUNT == B
        CONTINUE
    ;娼馆、出租劳役中
    SIF CFLAG:COUNT:12 > 0
        CONTINUE
    SIF CFLAG:COUNT:2404 == 1
        CONTINUE
    SIF CFLAG:COUNT:2414 == 1
        CONTINUE
    ;育儿室
    SIF  (TALENT:COUNT:怀孕 && CFLAG:COUNT:110 - 3 <= DAY) || TALENT:COUNT:育儿中
        CONTINUE
    SIF BASE:COUNT:0 < 100
        CONTINUE
    SIF RELATION:ARG:(NO:COUNT) >= 120 && RELATION:COUNT:(NO:ARG) >= 120
        D = COUNT
REND
;决定E E嫉妒ARG
REPEAT CHARANUM
    ;主人不可
    SIF COUNT == MASTER || COUNT == B || COUNT == D
        CONTINUE
    ;娼馆、出租劳役中
    SIF CFLAG:COUNT:12 > 0
        CONTINUE
    SIF CFLAG:COUNT:2404 == 1
        CONTINUE
    SIF CFLAG:COUNT:2414 == 1
        CONTINUE
    ;育儿室
    SIF  (TALENT:COUNT:怀孕 && CFLAG:COUNT:110 - 3 <= DAY) || TALENT:COUNT:育儿中
        CONTINUE
    SIF BASE:COUNT:0 < 200
        CONTINUE
    SIF RELATION:COUNT:(NO:ARG) <= 90 && TALENT:COUNT:嫉妒
        E = COUNT
REND
;决定F
REPEAT CHARANUM
    ;主人不可
    SIF COUNT == MASTER || COUNT == B || COUNT == D || COUNT == E || COUNT == ARG
        CONTINUE
    ;娼馆、出租劳役中
    SIF CFLAG:COUNT:12 > 0
        CONTINUE
    SIF CFLAG:COUNT:2404 == 1
        CONTINUE
    SIF CFLAG:COUNT:2414 == 1
        CONTINUE
    ;育儿室
    SIF  (TALENT:COUNT:怀孕 && CFLAG:COUNT:110 - 3 <= DAY) || TALENT:COUNT:育儿中
        CONTINUE
    SIF BASE:COUNT:0 < 200
        CONTINUE
    SIF TALENT:COUNT:3 ||TALENT:COUNT:6
