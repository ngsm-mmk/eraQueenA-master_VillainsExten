REND
;决定G
REPEAT CHARANUM
    ;主人不可
    SIF COUNT == MASTER || COUNT == B || COUNT == D || COUNT == E || COUNT == F
        CONTINUE
    ;娼馆、出租劳役中
    SIF CFLAG:COUNT:12 > 0
        CONTINUE
    SIF CFLAG:COUNT:2404 == 1
        CONTINUE
    SIF CFLAG:COUNT:2414 == 1
        CONTINUE
