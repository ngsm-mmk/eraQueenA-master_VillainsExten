	催眠返回 = 1
	JUMP CHARA_ROOM(ARG)
ENDIF	
IF !CFLAG:ARG:7000
	PRINTFORMW %PLAYER_NAME()%%催眠暗示%之後，引起了%CALLNAME:ARG%的注意……
	;催眠成功率判定
	;困难度计算
	催眠成功率 = 催眠技术 * 30
		;困难度計算
		LOCAL = DEFFICULTY(ARG)
		IF LOCAL > 400
			催眠成功率 += 10
		ELSEIF LOCAL > 100
			催眠成功率 -= 20
		ELSEIF LOCAL > -200
			催眠成功率 -= 40
		ELSEIF LOCAL > -400
			催眠成功率 -= 70
		ELSEIF LOCAL > -800
			催眠成功率 -= 100
		ELSEIF LOCAL > -2000
			催眠成功率 -= 150
		ELSEIF LOCAL > -3000
			催眠成功率 -= 200
		ELSE
			催眠成功率 -= 3000
		ENDIF		
	IF CFLAG:ARG:2 < 0
		催眠成功率 += -1 * (CFLAG:ARG:2/100)
	ELSEIF CFLAG:ARG:2 == 0
		催眠成功率 += 0
	ELSE
		催眠成功率 += (CFLAG:ARG:2/100)
	ENDIF
	SIF 催眠成功率 > 100
		催眠成功率 = 100
	SIF 催眠成功率 < 0
		催眠成功率 = 0
	PRINTFORMW %CALLNAME:ARG%的催眠难度为%CSTR:ARG:10%，加上依赖度的计算之後…
	PRINTFORMW ……………
	PRINTFORMW ………
	PRINTFORMW …
	PRINTFORMW 目前催眠的成功率为{催眠成功率}％
	IF 催眠成功率 == 0
		PRINTFORMW 唉…看来没有催眠%CALLNAME:ARG%的可能啊！
		催眠返回 = 1
		JUMP CHARA_ROOM(ARG)
	ENDIF
	PRINTFORML 要实行催眠吗？（本日催眠的还有机会：{每日催眠可实行次数}次）
	PRINTBUTTON "[ 实行 ]", 01
	PRINTBUTTON "  [ 放弃 ]", 02
	INPUT
	IF RESULT == 1
		IF 每日催眠可实行次数 >= 1
			;催眠成功
			IF 催眠成功率 >= RAND:100
				PRINTFORMW 『催眠』成功了！
				CFLAG:ARG:7000 = 催眠技术
				每日催眠可实行次数 -= 1
				CALL 催眠点数计算(ARG)
				PRINTFORMW %PLAYER_NAME()%的催眠点数增加了{G:66}！
				CALL 催眠指令(ARG)
			;催眠失败
			ELSE
				PRINTFORM 神情有点恍惚的%CALLNAME:ARG%
				PRINTDATAL
					DATAFORM  甩了甩脑袋，恢复了意识…… 
					DATAFORM  眨了眨眼睛之後，又恢复了意识…… 
					DATAFORM  才刚把头低下，又马上抬起头来恢复了意识…… 
					DATAFORM  不知为何打了个喷嚏，然後恢复了意识…… 
					DATAFORM  皱了皱眉头之後，拍了自己的脑袋一下，恢复了意识…… 
				ENDDATA
				PRINTFORMW
				G:66 = 0
				G:66 = (RAND:80 + 20)
				催眠EXP += G:66
				PRINTFORMW 『催眠』失败了！
				PRINTFORMW %PLAYER_NAME()%的催眠点数增加了{G:66}！
				PRINTFORMW 对%CALLNAME:ARG%的精神造成了负担，压力增加了10！
				CFLAG:ARG:10 += 10
				每日催眠可实行次数 -= 1
				催眠返回 = 1
				JUMP CHARA_ROOM(ARG)
			ENDIF
		ELSE
			PRINTFORMW 可以催眠的次数已经用尽，看来只能等明天再来催眠了…… 
			催眠返回 = 1
			JUMP CHARA_ROOM(ARG)
		ENDIF
	ELSEIF RESULT == 2
			催眠返回 = 1
			JUMP CHARA_ROOM(ARG)
	ENDIF
ELSE
	CALL 催眠指令(ARG)
ENDIF
