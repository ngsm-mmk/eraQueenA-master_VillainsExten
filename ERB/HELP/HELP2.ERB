	P -= 1
	GOTO PRINT_LIST
ELSEIF RESULT == 1009 && P < 5
	P += 1
	GOTO PRINT_LIST
ELSE
	SIF Q:RESULT == 0
		GOTO INPUT_LOOP
	PRINTFORML 【%ITEMNAME:RESULT%】
	TRYCALLFORM DICTIONARY_ITEM_{RESULT}
