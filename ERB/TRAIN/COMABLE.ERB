SIF TALENT:无形体 || TALENT:无头
	RETURN 0
SIF TALENT:人偶 || TALENT:机器人
	RETURN 0
;调教者が男性、双性、扶她、もしくは捆绑式假阴茎がないとダメ
SIF TALENT:PLAYER:137 == 0 && TALENT:PLAYER:139 == 0 && TALENT:PLAYER:140 == 0 && ITEM:PBAND == 0
	RETURN 0
;后庭处子で、顺从4以下・百合属性or同性倾向4以下のASSIの場合はできない（施虐狂ならＯＫ）
IF TALENT:后庭处子 && ASSIPLAY
	SIF (ABL:ASSI:0 <= 4 || (DOUSEI(TARGET,ASSI) && MALE(TARGET) && ABL:ASSI:同性倾向 <= 4 && !TALENT:ASSI:男女通吃) || (DOUSEI(TARGET,ASSI) && !MALE(TARGET) && ABL:ASSI:百合属性 <= 4 && !TALENT:ASSI:男女通吃)) && !TALENT:ASSI:施虐狂
		RETURN 0
ENDIF
;润滑不足で、顺从3以下・百合属性or同性倾向3以下のASSIの場合はできない（施虐狂ならＯＫ）
IF PALAM:4 < PALAMLV:2 && ASSIPLAY
	SIF (ABL:ASSI:0 <= 3 || (DOUSEI(TARGET,ASSI) && MALE(TARGET) && ABL:ASSI:同性倾向 <= 3 && !TALENT:ASSI:男女通吃) || (DOUSEI(TARGET,ASSI) && !MALE(TARGET) && ABL:ASSI:百合属性 <= 3 && !TALENT:ASSI:男女通吃)) && !TALENT:ASSI:施虐狂
		RETURN 0
ENDIF
;下身内衣をつけている、布偶装を着ている、もしく水手裙以外の下身外衣を履いているとダメ
SIF TEQUIP:2 || TEQUIP:4 > 1 || TEQUIP:6 == 7
	RETURN 0
;触手调教中はダメ
SIF TEQUIP:25
	RETURN 0
;浴室PLAY中は塑料地垫が無いとダメ
IF TEQUIP:31
SIF ITEM:20 == 0 && NOITEM == 0
	RETURN 0
ENDIF
RETURN 1

