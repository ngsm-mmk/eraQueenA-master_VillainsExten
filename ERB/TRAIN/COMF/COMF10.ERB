;-------------------------------------------------
;胸自慰
;爱抚系指令、レベル3
;-------------------------------------------------
@COM10
;-------------------------------------------------
;特殊派生処理
;-------------------------------------------------
;前回と今回の调教者が同じ
IF (ASSIPLAY && TFLAG:50) || (ASSIPLAY == 0 && TFLAG:50 == 0)
	;前指令が骑乘位の場合は骑乘位揉乳自慰にジャンプ
	SIF PREVCOM == 66 && TFLAG:60
		JUMP COM505
	;前指令が骑乘位肛交の場合は骑乘位揉乳自慰にジャンプ
	SIF PREVCOM == 67 && TFLAG:60
		JUMP COM505
ENDIF
;口交时自慰
IF PREVCOM == 81
	CALL COM_ABLE530
	JUMP COM530
ENDIF
;舔陰时自慰
IF PREVCOM == 120 || PREVCOM == 79
	CALL COM_ABLE531
	JUMP COM531
ENDIF
;视频拍摄中なら公開
PRINT 命其
SIF TEQUIP:28
	PRINT 公开
PRINTL 胸部自慰

;実行者が奴隶であるフラグ
TFLAG:28 = 1


;-------------------------------------------------
;実行判定処理
;-------------------------------------------------
A = 0
S = 0

;すべての命令に共通の要素を考慮
;(顺从が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:欲望
IF ABL:1
	SIF S
		PRINT  + 
	A += ABL:1 * 3
	PRINTS ABLNAME:1
	PRINTV 'LV,ABL:1,'(,ABL:1 * 3,')
	S = 1
ENDIF
;ABL:露出癖
IF ABL:8
	SIF S
		PRINT  + 
	A += ABL:8 * 4
	PRINTS ABLNAME:8
	PRINTV 'LV,ABL:8,'(,ABL:8 * 4,')
	S = 1
ENDIF
;ABL:自慰成瘾
IF ABL:15
	SIF S
		PRINT  + 
	A += ABL:15 * 3
	PRINTS ABLNAME:15
	PRINTV 'LV,ABL:15,'(,ABL:15 * 3,')
	S = 1
ENDIF

IF MARK:1
	SIF S
		PRINT  + 
	A += MARK:1 * 3
	PRINTS MARKNAME:1
	PRINTV 'LV,MARK:1,'(,MARK:1 * 3,')
	S = 1
ENDIF


;PALAM:情欲
IF PALAM:6 < PALAMLV:1
	L = 0
ELSEIF PALAM:6 < PALAMLV:2
	L = 1
ELSEIF PALAM:6 < PALAMLV:3
	L = 2
ELSEIF PALAM:6 < PALAMLV:4
	L = 3
ELSEIF PALAM:6 < PALAMLV:5
	L = 4
ELSE
	L = 5
ENDIF
IF L
	SIF S
		PRINT  + 
	A += L * 3
	PRINTS PALAMNAME:6
	PRINTV 'LV,L,'(,L * 3,')
	S = 1
ENDIF

;'自制力
IF TALENT:20
	PRINT  - 
	A -= 5
	PRINTS TALENTNAME:20
	PRINTV '(,5,')
	S = 1
ENDIF
;害羞
IF TALENT:35
	PRINT  - 
	A -= 5
	PRINTS TALENTNAME:35
	PRINTV '(,5,')
	S = 1
ENDIF
;不知羞耻
IF TALENT:36
	SIF S
		PRINT  + 
	A += 2
	PRINTS TALENTNAME:36
	PRINTV '(,2,')
	S = 1
ENDIF
;容易自慰
IF TALENT:60
	SIF S
		PRINT  + 
	A += 5
	PRINTS TALENTNAME:60
	PRINTV '(,5,')
	S = 1
ENDIF
;厌恶身体接触
IF TALENT:66
	PRINT  + 
	A += 2
	PRINTS TALENTNAME:66
	PRINTV '(,2,')
	S = 1
ENDIF
;接受快感
IF TALENT:70
	SIF S
		PRINT  + 
	A += 5
	PRINTS TALENTNAME:70
	PRINTV '(,5,')
	S = 1
ENDIF
;否定快感（淫乱、娼妇で无效）
IF TALENT:71 && (TALENT:4 == 0 && TALENT:7 == 0)
	PRINT  - 
	A -= 5
	PRINTS TALENTNAME:71
	PRINTV '(,5,')
	S = 1
ENDIF

;媚薬
IF TEQUIP:26
	SIF S
		PRINT  + 
	A += 8
	PRINTS ITEMNAME:41
	PRINTV '(,8,')
	S = 1
ENDIF

;合計を表示(33以上で実行)
PRINT  = 
PRINTV A

;困难度上昇
;公開で+10
V = 33
SIF TEQUIP:28
	V += 10

SIF A < V
	PRINT  < 
SIF A == V
	PRINT  = 
SIF A > V
	PRINT  > 
PRINT 实行值
PRINTV V


;実行不可口上/実行判定操作
IF A < V
	CALL KOJO_DENIAL
	WAIT
	RETURN 0
ELSE
	PRINTL
ENDIF

STR:0 = 命其胸部自慰


;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、气力
LOSEBASE:0 += 5
LOSEBASE:1 += 50

SOURCE:3 = 50
SOURCE:5 = 40
SOURCE:8 = 100
SOURCE:9 = 100
SOURCE:13 = 200
SOURCE:14 = 400
SOURCE:17 = 150
SOURCE:18 = 150
IF TEQUIP:28
	SOURCE:10 = 50
	SOURCE:17 += 100
ENDIF
;奴隶男の場合
SIF TALENT:140
	SOURCE:17 += 50
;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;性压抑、抵抗持ちには、抑郁ソース追加
SIF TALENT:32 || TALENT:34
	SOURCE:16 += 150

;中毒による中毒充足のソースは先に処理しておく
;自慰成瘾を見る
SIF ABL:15 == 1
	TIMES SOURCE:9 , 0.00
SIF ABL:15 == 2
	TIMES SOURCE:9 , 0.30
SIF ABL:15 == 3
	TIMES SOURCE:9 , 0.60
SIF ABL:15 == 4
	TIMES SOURCE:9 , 0.90
SIF ABL:15 == 5
	TIMES SOURCE:9 , 1.20
SIF ABL:15 == 6
	TIMES SOURCE:9 , 1.50
SIF ABL:15 == 7
	TIMES SOURCE:9 , 1.80
SIF ABL:15 == 8
	TIMES SOURCE:9 , 2.30
SIF ABL:15 == 9
	TIMES SOURCE:9 , 3.00
SIF ABL:15 >= 10
	TIMES SOURCE:9 , 5.00


;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;手套をしているとき
IF TEQUIP:0 == 1
	;奴隶のＢ⇔奴隶の特殊１の汚れが移动
	STAIN:8 |= STAIN:5
	STAIN:5 |= STAIN:8
ELSE
	;奴隶の指⇔奴隶のＢの汚れが移动
	STAIN:1 |= STAIN:5
	STAIN:5 |= STAIN:1
ENDIF
;-------------------------------------------------
;经验の処理
;-------------------------------------------------
;视频拍摄時は经验にプラス
IF TEQUIP:28
	EXP:10 += 2
	PRINTL 自慰经验＋２
	EXP:11 += 2
	PRINTL 调教自慰经验＋２
ELSE
	EXP:10 += 1
	PRINTL 自慰经验＋１
	EXP:11 += 1
	PRINTL 调教自慰经验＋１
ENDIF
IF TEQUIP:28 && CFLAG:3 == 0
	EXP:50 += 1
	PRINTL 异常经验＋１
	CFLAG:3 = 1
ENDIF

;蔷薇经验とか加算
CALL COM_EXP(3)


;依赖度ベクトル
TFLAG:33 = 3

;依赖度補正値(服从で1、隶属で2）
IF TALENT:5 && ASSIPLAY == 0
	TFLAG:30 += 1
ELSEIF TALENT:8 && ASSIPLAY == 0
	TFLAG:30 += 2
ENDIF


RETURN 1

