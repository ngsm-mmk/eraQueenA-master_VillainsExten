;-------------------------------------------------
;首絞め
;SM系指令、レベル
;-------------------------------------------------
@COM112
;-------------------------------------------------
;特殊派生処理
;-------------------------------------------------
;前のプ雷が正常位か对面立位で调教者の技巧３以上の場合、首絞め做爱へ派生
IF (PREVCOM == 60 || PREVCOM == 74) && ABL:PLAYER:2 >= 3
	CALL COM_ABLE517
	SIF RESULT == 1
		JUMP COM517
;前のプ雷が正常位肛交か对面立位肛で调教者の技巧３以上の場合、首絞め肛へ派生
ELSEIF (PREVCOM == 62 || PREVCOM == 75) && ABL:PLAYER:2 >= 3
	CALL COM_ABLE518
	SIF RESULT == 1
		JUMP COM518
ENDIF

PRINTL 掐脖子
;-------------------------------------------------
;実行判定処理
;-------------------------------------------------
A = 0
S = 0

;すべての命令に共通の要素を考慮
;(顺从が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:欲望
IF ABL:1
	SIF S
		PRINT  + 
	A += ABL:1 * 1
	PRINTS ABLNAME:1
	PRINTV 'LV,ABL:1,'(,ABL:1 * 1,')
	S = 1
ENDIF
;ABL:侍奉精神
IF ABL:7
	SIF S
		PRINT  + 
	A += ABL:7 * 4
	PRINTS ABLNAME:7
	PRINTV 'LV,ABL:7,'(,ABL:7 * 4,')
	S = 1
ENDIF
;受虐属性
IF ABL:9
	SIF S
		PRINT  + 
	A += ABL:9 * 5
	PRINTS ABLNAME:9
	PRINTV 'LV,ABL:9,'(,ABL:9 * 5,')
	S = 1
ENDIF

IF MARK:1
	SIF S
		PRINT  + 
	A += MARK:1 * 1
	PRINTS MARKNAME:1
	PRINTV 'LV,MARK:1,'(,MARK:1 * 1,')
	S = 1
ENDIF


;PALAM:情欲
IF PALAM:6 < PALAMLV:1
	L = 0
ELSEIF PALAM:6 < PALAMLV:2
	L = 1
ELSEIF PALAM:6 < PALAMLV:3
	L = 2
ELSEIF PALAM:6 < PALAMLV:4
	L = 3
ELSEIF PALAM:6 < PALAMLV:5
	L = 4
ELSE
	L = 5
ENDIF
IF L
	SIF S
		PRINT  + 
	A += L * 1
	PRINTS PALAMNAME:6
	PRINTV 'LV,L,'(,L * 1,')
	S = 1
ENDIF

;自我奉献
IF TALENT:63
	SIF S
		PRINT  + 
	A += 6
	PRINTS TALENTNAME:63
	PRINTV '(,6,')
	S = 1
ENDIF
;否定快感（淫乱、娼妇で无效）
IF TALENT:71 && (TALENT:4 == 0 && TALENT:7 == 0)
	PRINT  - 
	A -= 1
	PRINTS TALENTNAME:71
	PRINTV '(,1,')
	S = 1
ENDIF
;受虐狂
IF TALENT:84
	SIF S
		PRINT  + 
	A += 8
	PRINTS TALENTNAME:84
	PRINTV '(,8,')
	S = 1
ENDIF
;恋慕
IF TALENT:3 && ASSIPLAY == 0
	SIF S
		PRINT  + 
	A += 5
	PRINTS TALENTNAME:3
	PRINTV '(,5,')
	S = 5
ENDIF
;相亲相爱
IF TALENT:6 && ASSIPLAY == 0
	SIF S
		PRINT  + 
	A += 10
	PRINTS TALENTNAME:6
	PRINTV '(,10,')
	S = 10
ENDIF

;合計を表示(30以上で実行)
PRINT  = 
PRINTV A

V = 30
SIF A < V
	PRINT  < 
SIF A == V
	PRINT  = 
SIF A > V
	PRINT  > 
PRINT 实行值
PRINTV V
;実行不可口上/実行判定操作
IF A < V
	CALL KOJO_DENIAL
	WAIT
	RETURN 0
ELSE
	PRINTL
ENDIF


;-------------------------------------------------
;実行決定
;-------------------------------------------------
STR:0 = 掐脖子


;実行者が奴隶であるフラグ
TFLAG:28 = 1

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、气力
LOSEBASE:0 += 80
LOSEBASE:1 += 50
;疼痛、恐惧、逸脱、反感
SOURCE:7 = 1000
SOURCE:8 = 2500
SOURCE:17 = 600
SOURCE:18 = 800

;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;痛苦のパラメータによる痛苦のソース増加は先に処理しておく
IF PALAM:10 < PALAMLV:1
	TIMES SOURCE:7 , 1.00
ELSEIF PALAM:10 < PALAMLV:2
	TIMES SOURCE:7 , 1.50
ELSEIF PALAM:10 < PALAMLV:3
	TIMES SOURCE:7 , 2.00
ELSEIF PALAM:10 < PALAMLV:4
	TIMES SOURCE:7 , 2.50
ELSEIF PALAM:10 >= PALAMLV:4
	TIMES SOURCE:7 , 3.00
ENDIF

;受虐属性を見る
IF ABL:9 == 0
	TIMES SOURCE:14 , 0.80
	SOURCE:9 += 0
	SOURCE:11 += 0
ELSEIF ABL:9 == 1
	TIMES SOURCE:14 , 1.00
	SOURCE:9 += 20
	SOURCE:11 += 30
ELSEIF ABL:9 == 2
	TIMES SOURCE:14 , 1.20
	SOURCE:9 += 40
	SOURCE:11 += 70
ELSEIF ABL:9 == 3
	TIMES SOURCE:14 , 1.40
	SOURCE:9 += 70
	SOURCE:11 += 120
ELSEIF ABL:9 == 4
	TIMES SOURCE:14 , 1.50
	SOURCE:9 += 110
	SOURCE:11 += 180
ELSEIF ABL:9 == 5
	TIMES SOURCE:14 , 1.70
	SOURCE:9 += 150
	SOURCE:11 += 250
ELSEIF ABL:9 == 6
	TIMES SOURCE:14 , 1.90
	SOURCE:9 += 200
	SOURCE:11 += 300
ELSEIF ABL:9 == 7
	TIMES SOURCE:14 , 2.10
	SOURCE:9 += 250
	SOURCE:11 += 350
ELSEIF ABL:9 == 8
	TIMES SOURCE:14 , 2.30
	SOURCE:9 += 300
	SOURCE:11 += 400
ELSEIF ABL:9 == 9
	TIMES SOURCE:14 , 2.50
	SOURCE:9 += 350
	SOURCE:11 += 450
ELSEIF ABL:9 >= 10
	TIMES SOURCE:14 , 3.00
	SOURCE:9 += 500
	SOURCE:11 += 500
ENDIF

;受虐狂を見る
IF TALENT:84 == 1
	TIMES SOURCE:11 , 1.50
	TIMES SOURCE:18 , 0.80
ENDIF

;体格差により恐怖のソース追加
S = BUILD2(PLAYER, TARGET)
IF S == 3
	TIMES SOURCE:8 , 2.00
ELSEIF S == 2
	TIMES SOURCE:8 , 1.50
ELSEIF S == 1
	TIMES SOURCE:8 , 1.20
ELSEIF S == 0
	TIMES SOURCE:8 , 1.00
ELSEIF S == -3
	TIMES SOURCE:8 , 0.75
ELSE
	TIMES SOURCE:8 , 0.90
ENDIF

;-------------------------------------------------
;经验の処理
;-------------------------------------------------
;蔷薇经验とか加算
CALL COM_EXP(2)



;依赖度ベクトル
TFLAG:33 = 3

;依赖度補正値(主人かつ恋慕、淫乱、服从で1。相亲相爱、娼妇、隶属で2）
IF (TALENT:3 || TALENT:4 || TALENT:5) && ASSIPLAY == 0
	TFLAG:30 += 1
ELSEIF (TALENT:6 || TALENT:7 || TALENT:8) && ASSIPLAY == 0
	TFLAG:30 += 2
ENDIF


RETURN 1
