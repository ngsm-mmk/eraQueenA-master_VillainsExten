;-------------------------------------------------
;顔騎圧迫
;SM系指令
;-------------------------------------------------
@COM113

PRINTL 颜骑圧迫
;-------------------------------------------------
;実行判定処理
;-------------------------------------------------
A = 0
S = 0

;すべての命令に共通の要素を考慮
;(顺从が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:欲望
IF ABL:1
	SIF S
		PRINT  + 
	A += ABL:1 * 1
	PRINTS ABLNAME:1
	PRINTV 'LV,ABL:1,'(,ABL:1 * 1,')
	S = 1
ENDIF
;ABL:侍奉精神
IF ABL:7
	SIF S
		PRINT  + 
	A += ABL:7 * 4
	PRINTS ABLNAME:7
	PRINTV 'LV,ABL:7,'(,ABL:7 * 4,')
	S = 1
ENDIF
;受虐属性
IF ABL:9
	SIF S
		PRINT  + 
	A += ABL:9 * 5
	PRINTS ABLNAME:9
	PRINTV 'LV,ABL:9,'(,ABL:9 * 5,')
	S = 1
ENDIF
;快乐刻印
IF MARK:1
	SIF S
		PRINT  + 
	A += MARK:1 * 1
	PRINTS MARKNAME:1
	PRINTV 'LV,MARK:1,'(,MARK:1 * 1,')
	S = 1
ENDIF

;PALAM:情欲
IF PALAM:6 < PALAMLV:1
	L = 0
ELSEIF PALAM:6 < PALAMLV:2
	L = 1
ELSEIF PALAM:6 < PALAMLV:3
	L = 2
ELSEIF PALAM:6 < PALAMLV:4
	L = 3
ELSEIF PALAM:6 < PALAMLV:5
	L = 4
ELSE
	L = 5
ENDIF
IF L
	SIF S
		PRINT  + 
	A += L * 1
	PRINTS PALAMNAME:6
	PRINTV 'LV,L,'(,L * 1,')
	S = 1
ENDIF

;害羞
IF TALENT:35
	PRINT  - 
	A -= 1
	PRINTS TALENTNAME:35
	PRINTV '(,1,')
	S = 1
ENDIF
;污臭钝感
IF TALENT:61
	SIF S
		PRINT  + 
	A += 1
	PRINTS TALENTNAME:61
	PRINTV '(,1,')
	S = 1
ENDIF
;污臭敏感
IF TALENT:62
	PRINT  - 
	A -= 3
	PRINTS TALENTNAME:62
	PRINTV '(,3,')
	S = 1
ENDIF
;自我奉献
IF TALENT:63
	SIF S
		PRINT  + 
	A += 6
	PRINTS TALENTNAME:63
	PRINTV '(,6,')
	S = 1
ENDIF
;否定快感（淫乱、娼妇で无效）
IF TALENT:71 && (TALENT:4 == 0 && TALENT:7 == 0)
	PRINT  - 
	A -= 1
	PRINTS TALENTNAME:71
	PRINTV '(,1,')
	S = 1
ENDIF



;受虐狂
IF TALENT:84
	SIF S
		PRINT  + 
	A += 8
	PRINTS TALENTNAME:84
	PRINTV '(,8,')
	S = 1
ENDIF
;恋慕
IF TALENT:3 && ASSIPLAY == 0
	SIF S
		PRINT  + 
	A += 5
	PRINTS TALENTNAME:3
	PRINTV '(,5,')
	S = 5
ENDIF
;相亲相爱
IF TALENT:6 && ASSIPLAY == 0
	SIF S
		PRINT  + 
	A += 10
	PRINTS TALENTNAME:6
	PRINTV '(,10,')
	S = 10
ENDIF

Y = 0
;愛液の汚れ
SIF STAIN:PLAYER:2 & 1
	Y += 1
;精液の汚れ
SIF STAIN:PLAYER:2 & 4
	Y += 3
;肛の汚れ
SIF STAIN:PLAYER:2 & 8
	Y += 7
SIF STAIN:PLAYER:2 & 16
	Y += 15

SIF TALENT:61
	Y /= 3
SIF TALENT:62
	Y *= 2

;汚れあり
IF Y
	PRINT  - 
	A -= Y
	;污臭钝感
	IF TALENT:61
		PRINT 有污物、
		PRINTS TALENTNAME:61
	ELSEIF TALENT:62
		PRINT 有污物、
		PRINTS TALENTNAME:62
	ELSE
		PRINT 有污物
	ENDIF
	PRINTV '(,Y,')
	S = 1
ENDIF

;合計を表示(36以上で実行)
PRINT  = 
PRINTV A

V = 36
SIF A < V
	PRINT  < 
SIF A == V
	PRINT  = 
SIF A > V
	PRINT  > 
PRINT 实行值
PRINTV V
;実行不可口上/実行判定操作
IF A < V
	CALL KOJO_DENIAL
	WAIT
	RETURN 0
ELSE
	PRINTL
ENDIF

;-------------------------------------------------
;実行決定
;-------------------------------------------------
STR:0 = 颜骑圧迫


;実行者が奴隶であるフラグ
TFLAG:28 = 1

;-------------------------------------------------
;射精槽チェック
;-------------------------------------------------
B = 1500

;ABL:技巧をみる
IF ABL:2 == 0
	TIMES B , 1.00
ELSEIF ABL:2 == 1
	TIMES B , 1.20
ELSEIF ABL:2 == 2
	TIMES B , 1.40
ELSE
	TIMES B , 1.60
ENDIF

;ABL:顺从をみる
IF ABL:0 == 0
	TIMES B , 0.80
ELSEIF ABL:0 == 1
	TIMES B , 0.90
ELSEIF ABL:0 == 2
	TIMES B , 1.00
ELSE
	TIMES B , 1.10
ENDIF

;ABL:侍奉精神をみる
IF ABL:7 == 0
	TIMES B , 0.50
ELSEIF ABL:7 == 1
	TIMES B , 0.80
ELSEIF ABL:7 == 2
	TIMES B , 1.20
ELSE
	TIMES B , 1.50
ENDIF

;TALENT:善用舌头、猫舌をみる
SIF TALENT:52 || TALENT:54
	TIMES B , 2.00

;プ雷ヤーのABL:Ｃ感觉をみる
IF ABL:PLAYER:3 == 0
	TIMES B , 1.00
ELSEIF ABL:PLAYER:3 == 1
	TIMES B , 1.50
ELSEIF ABL:PLAYER:3 == 2
	TIMES B , 2.00
ELSEIF ABL:PLAYER:3 == 3
	TIMES B , 2.50
ELSEIF ABL:PLAYER:3 == 4
	TIMES B , 3.50
ELSE
	TIMES B , 5.00
ENDIF

;再装填処理中は射精槽は20分の1に
;SIF CFLAG:PLAYER:9 > 0
;	B /= 20

B = B/4
BASE:PLAYER:2 += B

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、气力
LOSEBASE:0 += 100
LOSEBASE:1 += 200

SOURCE:5 = 500
SOURCE:7 = 300
SOURCE:11 = 100
SOURCE:14 = 1200
SOURCE:17 = 500
SOURCE:18 = 700

;上のほうで計算した汚れデータ
SOURCE:15 = Y*40 + 100


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;性压抑、抵抗持ちには、抑郁ソース追加
SIF TALENT:32 || TALENT:34
	SOURCE:16 += 700

;受虐属性を見る
IF ABL:9 == 0
	TIMES SOURCE:14 , 0.80
	SOURCE:9 += 0
	SOURCE:11 += 0
ELSEIF ABL:9 == 1
	TIMES SOURCE:14 , 1.00
	SOURCE:9 += 20
	SOURCE:11 += 30
ELSEIF ABL:9 == 2
	TIMES SOURCE:14 , 1.20
	SOURCE:9 += 40
	SOURCE:11 += 70
ELSEIF ABL:9 == 3
	TIMES SOURCE:14 , 1.40
	SOURCE:9 += 70
	SOURCE:11 += 120
ELSEIF ABL:9 == 4
	TIMES SOURCE:14 , 1.50
	SOURCE:9 += 110
	SOURCE:11 += 180
ELSEIF ABL:9 == 5
	TIMES SOURCE:14 , 1.70
	SOURCE:9 += 150
	SOURCE:11 += 250
ELSEIF ABL:9 == 6
	TIMES SOURCE:14 , 1.90
	SOURCE:9 += 200
	SOURCE:11 += 300
ELSEIF ABL:9 == 7
	TIMES SOURCE:14 , 2.10
	SOURCE:9 += 250
	SOURCE:11 += 350
ELSEIF ABL:9 == 8
	TIMES SOURCE:14 , 2.30
	SOURCE:9 += 300
	SOURCE:11 += 400
ELSEIF ABL:9 == 9
	TIMES SOURCE:14 , 2.50
	SOURCE:9 += 350
	SOURCE:11 += 450
ELSEIF ABL:9 >= 10
	TIMES SOURCE:14 , 3.00
	SOURCE:9 += 500
	SOURCE:11 += 500
ENDIF

;受虐狂を見る
IF TALENT:84 == 1
	TIMES SOURCE:11 , 1.50
	TIMES SOURCE:18 , 0.80
ENDIF

;体格差により恐怖のソース追加
S = BUILD2(PLAYER, TARGET)
IF S == 3
	TIMES SOURCE:8 , 2.00
ELSEIF S == 2
	TIMES SOURCE:8 , 1.50
ELSEIF S == 1
	TIMES SOURCE:8 , 1.20
ELSEIF S == 0
	TIMES SOURCE:8 , 1.00
ELSEIF S == -3
	TIMES SOURCE:8 , 0.75
ELSE
	TIMES SOURCE:8 , 0.90
ENDIF
;-------------------------------------------------
;绝顶チェック
;-------------------------------------------------
CALL SAMEN_CHECK

;射精時の処理
IF E >= 1
	;射精先タイプの设定
	TFLAG:703 = 7
	;射精先選択の可・不可
	TFLAG:704 = 1
	;汎用的な処理はこの関数で行う
	;（射精槽の再計算や射精经验の上昇と共通部分の表示）
	CALL SAMEN_SHOOT
ENDIF

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;奴隶の口⇔调教者のヴァギナの汚れが移动
STAIN:0 |= STAIN:PLAYER:3
STAIN:PLAYER:3 |= STAIN:0

;侍奉精神ＬＶ２以上、技巧ＬＶ２以上なら汚れをなめ取る（口塞球使用時は除く）
;IF ABL:7 >= 2 && ABL:2 >= 2 && TEQUIP:20 == 0
;	STAIN:PLAYER:3 = 2
;	SIF E >= 1
;		TFLAG:8 = 1
;ENDIF

;-------------------------------------------------
;经验の処理
;-------------------------------------------------
IF ABL:9 + ABL:7 >= 5
	;舔阴经验
	PRINTFORML %EXPNAME:25%＋１
	EXP:25 += 1
ENDIF

;蔷薇经验とか加算
CALL COM_EXP(5)



;爱情经验(受虐属性5以上限定）
IF (TALENT:3 == 1 || TALENT:6 == 1) && ASSIPLAY == 0 && ABL:9 >= 5
	PRINTS EXPNAME:23
	PRINTL +1
	EXP:23 += 1
ENDIF

;依赖度ベクトル
TFLAG:33 = 3

;依赖度補正値(主人かつ恋慕、淫乱、服从で1。相亲相爱、娼妇、隶属で2）
IF (TALENT:3 || TALENT:4 || TALENT:5) && ASSIPLAY == 0
	TFLAG:30 += 1
ELSEIF (TALENT:6 || TALENT:7 || TALENT:8) && ASSIPLAY == 0
	TFLAG:30 += 2
ENDIF

TFLAG:100 = 1


RETURN 1

