;-------------------------------------------------
;金蹴り
;SM系指令、レベル4
;-------------------------------------------------
@COM114
PRINTL 踢胯部
;-------------------------------------------------
;実行判定処理
;-------------------------------------------------
A = 0
S = 0

;すべての命令に共通の要素を考慮
;(顺从が高いと命令に従いやすいなど)
CALL COM_ORDER

;受虐属性
IF ABL:9
	SIF S
		PRINT  + 
	A += ABL:9 * 5
	PRINTS ABLNAME:9
	PRINTV 'LV,ABL:9,'(,ABL:9 * 5,')
	S = 1
ENDIF
;痛苦刻印
IF MARK:0
	SIF S
		PRINT  + 
	A += MARK:0 * 2
	PRINTS MARKNAME:0
	PRINTV 'LV,MARK:0,'(,MARK:0 * 2,')
	S = 1
ENDIF

;PALAM:情欲
IF PALAM:6 < PALAMLV:1
	L = 0
ELSEIF PALAM:6 < PALAMLV:2
	L = 1
ELSEIF PALAM:6 < PALAMLV:3
	L = 2
ELSEIF PALAM:6 < PALAMLV:4
	L = 3
ELSEIF PALAM:6 < PALAMLV:5
	L = 4
ELSE
	L = 5
ENDIF
IF L
	SIF S
		PRINT  + 
	A += L * 1
	PRINTS PALAMNAME:6
	PRINTV 'LV,L,'(,L * 1,')
	S = 1
ENDIF
;受虐狂
IF TALENT:84
	SIF S
		PRINT  + 
	A += 4
	PRINTS TALENTNAME:84
	PRINTV '(,4,')
	S = 1
ENDIF
;服从
IF TALENT:5 && ASSIPLAY == 0
	SIF S
		PRINT  + 
	A += 5
	PRINTS TALENTNAME:5
	PRINTV '(,5,')
	S = 1
ENDIF
;隶属
IF TALENT:8 && ASSIPLAY == 0
	SIF S
		PRINT  + 
	A += 10
	PRINTS TALENTNAME:8
	PRINTV '(,10,')
	S = 1
ENDIF
;蒙上眼睛しているとやりやすい
IF TEQUIP:眼罩
	SIF S
		PRINT  + 
	A += 10
	PRINT 蒙上眼睛
	PRINTV '(,10,')
	S = 1
ENDIF

;合計を表示(35以上で実行)
PRINT  = 
PRINTV A

V = 35
SIF A < V
	PRINT  < 
SIF A == V
	PRINT  = 
SIF A > V
	PRINT  > 
PRINT 实行值
PRINTV V
;実行不可口上/実行判定操作
IF A < V
	CALL KOJO_DENIAL
	WAIT
	RETURN 0
ELSE
	PRINTL
ENDIF
;-------------------------------------------------
;実行決定
;-------------------------------------------------
STR:0 = 踢胯部

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、气力
LOSEBASE:0 += 100
LOSEBASE:1 += 120

SOURCE:7 = 5500
SOURCE:8 = 4500
SOURCE:14 = 500
SOURCE:17 = 500
SOURCE:18 = 1500
;受虐属性がすごく高いと情欲が入る
IF ABL:受虐属性 < 4
ELSEIF ABL:受虐属性 < 6
ELSEIF ABL:受虐属性 < 9
	SOURCE:11 = 500
ELSE
	SOURCE:11 = 1000
ENDIF

;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;痛苦のパラメータによる痛苦のソース増加は先に処理しておく
IF PALAM:10 < PALAMLV:1
	TIMES SOURCE:7 , 1.00
ELSEIF PALAM:10 < PALAMLV:2
	TIMES SOURCE:7 , 1.50
ELSEIF PALAM:10 < PALAMLV:3
	TIMES SOURCE:7 , 2.00
ELSEIF PALAM:10 < PALAMLV:4
	TIMES SOURCE:7 , 2.50
ELSEIF PALAM:10 >= PALAMLV:4
	TIMES SOURCE:7 , 3.00
ENDIF
;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;射精に関係なく行われる処理はここから
;调教者の足⇔奴隶のＰの汚れが移动
STAIN:PLAYER:7 |= STAIN:2
STAIN:2 |= STAIN:PLAYER:7
;-------------------------------------------------
;经验の処理
;-------------------------------------------------
;蔷薇经验とか加算
CALL COM_EXP(2)


;依赖度ベクトル
TFLAG:33 = 5

;依赖度補正値(服从で1、隶属で2）
IF TALENT:5 && ASSIPLAY == 0
	TFLAG:30 += 1
ELSEIF TALENT:8 && ASSIPLAY == 0
	TFLAG:30 += 2
ENDIF


RETURN 1

