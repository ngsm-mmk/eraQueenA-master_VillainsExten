;-------------------------------------------------
;足交
;助手による指令、及び百合プ雷、レベル1
;-------------------------------------------------
@COM115
PRINTL 电气按摩

;-------------------------------------------------
;実行判定処理
;-------------------------------------------------
A = 0
S = 0

;すべての命令に共通の要素を考慮
;(顺从が高いと命令に従いやすいなど)
CALL COM_ORDER

;受虐属性
IF ABL:9
	SIF S
		PRINT  + 
	A += ABL:9 * 5
	PRINTS ABLNAME:9
	PRINTV 'LV,ABL:9,'(,ABL:9 * 5,')
	S = 1
ENDIF
;痛苦刻印
IF MARK:0
	SIF S
		PRINT  + 
	A += MARK:0 * 2
	PRINTS MARKNAME:0
	PRINTV 'LV,MARK:0,'(,MARK:0 * 2,')
	S = 1
ENDIF

;PALAM:情欲
IF PALAM:6 < PALAMLV:1
	L = 0
ELSEIF PALAM:6 < PALAMLV:2
	L = 1
ELSEIF PALAM:6 < PALAMLV:3
	L = 2
ELSEIF PALAM:6 < PALAMLV:4
	L = 3
ELSEIF PALAM:6 < PALAMLV:5
	L = 4
ELSE
	L = 5
ENDIF
IF L
	SIF S
		PRINT  + 
	A += L * 1
	PRINTS PALAMNAME:6
	PRINTV 'LV,L,'(,L * 1,')
	S = 1
ENDIF
;受虐狂
IF TALENT:84
	SIF S
		PRINT  + 
	A += 4
	PRINTS TALENTNAME:84
	PRINTV '(,4,')
	S = 1
ENDIF
;服从か恋慕
IF (TALENT:服从 || TALENT:恋慕) && ASSIPLAY == 0
	SIF S
		PRINT  + 
	A += 5
	PRINTS TALENTNAME:5
	PRINTV '(,5,')
	S = 1
ENDIF
;隶属か相亲相爱
IF (TALENT:隶属 || TALENT:相亲相爱) && ASSIPLAY == 0
	SIF S
		PRINT  + 
	A += 10
	PRINTS TALENTNAME:8
	PRINTV '(,10,')
	S = 1
ENDIF

;合計を表示(30以上で実行)
PRINT  = 
PRINTV A

V = 30
SIF A < V
	PRINT  < 
SIF A == V
	PRINT  = 
SIF A > V
	PRINT  > 
PRINT 实行值
PRINTV V
;実行不可口上/実行判定操作
IF A < V
	CALL KOJO_DENIAL
	WAIT
	RETURN 0
ELSE
	PRINTL
ENDIF


;-------------------------------------------------
;実行決定
;-------------------------------------------------

STR:0 = 电气按摩


;丝袜びりびりフラグ
IF TEQUIP:1 == 5 || TEQUIP:1 == 6
	TFLAG:55 = 1
ENDIF


;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、气力
LOSEBASE:0 += 50
LOSEBASE:1 += 120

SOURCE:0 = 350
SOURCE:6 = 200
SOURCE:13 = 300
SOURCE:14 = 400
SOURCE:17 = 350
SOURCE:18 = 400
IF PALAM:快Ｃ < 3000
	SOURCE:7 = 500
ELSEIF PALAM:快Ｃ < 6000
	SOURCE:7 = 400
ELSE
	SOURCE:7 = 300
ENDIF

;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;性压抑、抵抗持ちには、抑郁ソース追加
SIF TALENT:32 || TALENT:34
	SOURCE:16 += 400

;痛苦を伴わない受虐属性及び受虐狂の処理は先にしておく
;受虐属性を見る
IF ABL:9 == 0
	TIMES SOURCE:6 , 0.80
	TIMES SOURCE:14 , 0.80
	SOURCE:10 += 0
	SOURCE:11 += 0
ELSEIF ABL:9 == 1
	TIMES SOURCE:6 , 1.00
	TIMES SOURCE:14 , 1.00
	SOURCE:10 += 20
	SOURCE:11 += 30
ELSEIF ABL:9 == 2
	TIMES SOURCE:6 , 1.20
	TIMES SOURCE:14 , 1.20
	SOURCE:10 += 40
	SOURCE:11 += 70
ELSEIF ABL:9 == 3
	TIMES SOURCE:6 , 1.40
	TIMES SOURCE:14 , 1.40
	SOURCE:10 += 70
	SOURCE:11 += 120
ELSEIF ABL:9 == 4
	TIMES SOURCE:6 , 1.50
	TIMES SOURCE:14 , 1.50
	SOURCE:10 += 110
	SOURCE:11 += 180
ELSEIF ABL:9 == 5
	TIMES SOURCE:6 , 1.70
	TIMES SOURCE:14 , 1.70
	SOURCE:10 += 150
	SOURCE:11 += 250
ELSEIF ABL:9 == 6
	TIMES SOURCE:6 , 1.90
	TIMES SOURCE:14 , 1.90
	SOURCE:10 += 200
	SOURCE:11 += 300
ELSEIF ABL:9 == 7
	TIMES SOURCE:6 , 2.10
	TIMES SOURCE:14 , 2.10
	SOURCE:10 += 250
	SOURCE:11 += 350
ELSEIF ABL:9 == 8
	TIMES SOURCE:6 , 2.30
	TIMES SOURCE:14 , 2.30
	SOURCE:10 += 300
	SOURCE:11 += 400
ELSEIF ABL:9 == 9
	TIMES SOURCE:6 , 2.50
	TIMES SOURCE:14 , 2.50
	SOURCE:10 += 350
	SOURCE:11 += 450
ELSEIF ABL:9 >= 10
	TIMES SOURCE:6 , 3.00
	TIMES SOURCE:14 , 3.00
	SOURCE:10 += 500
	SOURCE:11 += 500
ENDIF

;受虐狂を見る
IF TALENT:84 == 1
	TIMES SOURCE:6 , 2.50
	TIMES SOURCE:14 , 2.50
	TIMES SOURCE:10 , 1.20
	TIMES SOURCE:11 , 1.20
ENDIF

;性倒错
SIF TALENT:80
	TIMES SOURCE:6 , 1.80
;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;射精に関係なく行われる処理はここから
;调教者の足⇔奴隶のＰの汚れが移动
STAIN:PLAYER:7 |= STAIN:2
STAIN:2 |= STAIN:PLAYER:7


;-------------------------------------------------
;经验の処理
;-------------------------------------------------
;蔷薇经验とか加算
CALL COM_EXP(4)



;依赖度ベクトル
P = NO:PLAYER
IF RELATION:P > 100
	TFLAG:33 = 2
ELSEIF RELATION:P < 100
	TFLAG:33 = 3
ELSE
	TFLAG:33 = 1
ENDIF

;依赖度補正値(相性100↑かつ淫乱で1、娼妇で2）
P = NO:PLAYER
IF TALENT:4 && RELATION:P > 100
	TFLAG:30 += 1
ELSEIF TALENT:7 && RELATION:P > 100
	TFLAG:30 += 2
ENDIF

;调教者が施虐狂持ちならパラメータに応じた嗜虐快乐经验と痛苦快乐经验も入れる（地の文から判断）
IF TALENT:PLAYER:83
	E = 1
	L = 0
	;受虐属性で痛苦快乐经验
	IF ABL:PLAYER:9 <= 3
		E += 0
		L += 0
	ELSEIF ABL:PLAYER:9 >= 4
		E += 1
		L += 2
	ENDIF
	;调教者の嗜虐属性に応じて嗜虐快乐经验
	IF ABL:PLAYER:10 <= 4
		E += 0
		L += 0
	ELSEIF ABL:PLAYER:10 >= 5
		E += 2
		L += 1
	ENDIF
	IF L > 0
		PRINT 痛苦快乐经验+
		PRINTVL L
		EXP:30 += L
		TFLAG:27 = L
	ENDIF
	EXP:PLAYER:32 += E
	PRINTFORM %CALLNAME:PLAYER%的%EXPNAME:32%+
	PRINTVL E
ENDIF

RETURN 1

