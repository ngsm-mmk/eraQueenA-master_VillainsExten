;-------------------------------------------------
;アナニー
;爱抚系指令、レベル3
;-------------------------------------------------
@COM14
;-------------------------------------------------
;特殊派生処理
;-------------------------------------------------
;アナニー
;口交时自慰
IF PREVCOM == 81
	CALL COM_ABLE530
	JUMP COM530
ENDIF
;舔陰时自慰
IF PREVCOM == 120 || PREVCOM == 79
	CALL COM_ABLE531
	JUMP COM531
ENDIF
;视频拍摄、野外PLAYの時に内容変化
;视频拍摄中なら公開アナニー
;野外PLAY中なら野外アナニー
;淋浴使用中なら淋浴アナニー
;肛塞で自慰棒アナニー
PRINT 命其
SIF TEQUIP:摄像机
	PRINT 公开
SIF TEQUIP:野外PLAY
	PRINT 野外
SIF TEQUIP:淋浴
	PRINT 淋浴时
SIF TEQUIP:肛塞
	PRINT 用自慰棒

PRINTL 肛门自慰

;-------------------------------------------------
;実行できるかの判定
;-------------------------------------------------
A = 0
S = 0

;すべての命令に共通の要素を考慮
;(顺从が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:欲望
IF ABL:欲望
	SIF S
		PRINT  + 
	A += ABL:欲望 * 3
	PRINTS ABLNAME:1
	PRINTV 'LV,ABL:欲望,'(,ABL:欲望 * 3,')
	S = 1
ENDIF
;ABL:露出癖
IF ABL:露出癖
	SIF S
		PRINT  + 
	A += ABL:露出癖 * 4
	PRINTS ABLNAME:8
	PRINTV 'LV,ABL:露出癖,'(,ABL:露出癖 * 4,')
	S = 1
ENDIF
;ABL:自慰成瘾
IF ABL:自慰成瘾
	SIF S
		PRINT  + 
	A += ABL:自慰成瘾 * 3
	PRINTS ABLNAME:15
	PRINTV 'LV,ABL:自慰成瘾,'(,ABL:自慰成瘾 * 3,')
	S = 1
ENDIF

IF MARK:快乐刻印
	SIF S
		PRINT  + 
	A += MARK:快乐刻印 * 3
	PRINTS MARKNAME:1
	PRINTV 'LV,MARK:快乐刻印,'(,MARK:快乐刻印 * 3,')
	S = 1
ENDIF


;PALAM:情欲
IF PALAM:6 < PALAMLV:1
	L = 0
ELSEIF PALAM:6 < PALAMLV:2
	L = 1
ELSEIF PALAM:6 < PALAMLV:3
	L = 2
ELSEIF PALAM:6 < PALAMLV:4
	L = 3
ELSEIF PALAM:6 < PALAMLV:5
	L = 4
ELSE
	L = 5
ENDIF
IF L
	SIF S
		PRINT  + 
	A += L * 3
	PRINTS PALAMNAME:6
	PRINTV 'LV,L,'(,L * 3,')
	S = 1
ENDIF
;自制力
IF TALENT:自制力
	PRINT  - 
	A -= 5
	PRINTS TALENTNAME:20
	PRINTV '(,5,')
	S = 1
ENDIF
;害羞
IF TALENT:害羞
	PRINT  - 
	A -= 5
	PRINTS TALENTNAME:35
	PRINTV '(,5,')
	S = 1
ENDIF
;不知羞耻
IF TALENT:不知羞耻
	SIF S
		PRINT  + 
	A += 2
	PRINTS TALENTNAME:36
	PRINTV '(,2,')
	S = 1
ENDIF
;容易自慰
IF TALENT:容易自慰
	SIF S
		PRINT  + 
	A += 5
	PRINTS TALENTNAME:60
	PRINTV '(,5,')
	S = 1
ENDIF
;厌恶身体接触
IF TALENT:厌恶身体接触
	PRINT  + 
	A += 2
	PRINTS TALENTNAME:66
	PRINTV '(,2,')
	S = 1
ENDIF
;接受快感
IF TALENT:接受快感
	SIF S
		PRINT  + 
	A += 5
	PRINTS TALENTNAME:70
	PRINTV '(,5,')
	S = 1
ENDIF
;否定快感（淫乱、娼妇で无效）
IF TALENT:否定快感 && (TALENT:淫乱 == 0 && TALENT:娼妇 == 0)
	PRINT  - 
	A -= 5
	PRINTS TALENTNAME:71
	PRINTV '(,5,')
	S = 1
ENDIF

;媚药
IF TEQUIP:媚药
	SIF S
		PRINT  + 
	A += 8
	PRINTS ITEMNAME:41
	PRINTV '(,8,')
	S = 1
ENDIF

;実行判定操作
;CALL KOJO_DENIAL_PLUS

;合計を表示(35以上で実行)
PRINT  = 
PRINTV A

;困难度上昇
;公開で+10、野外で+10、肛塞などで+5、淋浴で＋3
V = 35
SIF TEQUIP:摄像机
	V += 10
SIF TEQUIP:野外PLAY
	V += 10
SIF TEQUIP:肛塞 || TEQUIP:后庭拉珠 || TEQUIP:灌肠器＋塞头 || TEQUIP:扩张气球
	V += 5
SIF TEQUIP:淋浴
	V += 3

SIF A < V
	PRINT  < 
SIF A == V
	PRINT  = 
SIF A > V
	PRINT  > 
PRINT 实行值
PRINTV V

;実行不可口上/実行判定操作
IF A < V
	CALL KOJO_DENIAL
	WAIT
	RETURN 0
ELSE
	PRINTL
ENDIF

STR:0 = 命其肛门自慰

;丝袜びりびりフラグ
IF TEQUIP:1 == 5 || TEQUIP:1 == 6
	TFLAG:55 = 1
ENDIF

;実行者が奴隶であるフラグ
TFLAG:28 = 1

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、气力
LOSEBASE:0 += 50
LOSEBASE:1 += 100

SOURCE:快Ａ += 80
SOURCE:疼痛 += 80
SOURCE:性行动 += 100
SOURCE:恐惧 += 250
SOURCE:中毒充足 += 250
SOURCE:露出 += 450
SOURCE:屈从 += 600
SOURCE:逸脱 += 350
SOURCE:反抗 += 200
;ビデオ
IF TEQUIP:摄像机
	SOURCE:露出 += 50
	SOURCE:液体 += 50
	SOURCE:情欲 += 50
	SOURCE:反抗 += 80
ENDIF
;野外PLAY
IF TEQUIP:野外PLAY
	SOURCE:露出 += 100
	SOURCE:屈从 += 100
	SOURCE:反抗 +=80
ENDIF
;肛塞
IF TEQUIP:肛塞
	SOURCE:屈从 += 150
	SOURCE:反抗 += 100
	SOURCE:快Ａ += 30
ENDIF
;淋浴
IF TEQUIP:淋浴
	SOURCE:快Ａ += 10
	SOURCE:露出 += 100
	SOURCE:屈从 += 50
	SOURCE:反抗 += 80
ENDIF
;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;性压抑、抵抗持ちには、抑郁ソース追加
SIF TALENT:性压抑 || TALENT:抵抗
	SOURCE:郁闷 += 450
IF TEQUIP:摄像机 || TEQUIP:野外PLAY
	SELECTCASE ABL:露出癖
		CASE 0
			TIMES SOURCE:反抗 , 2.00
		CASE 1
			SOURCE:情欲 += 10
			TIMES SOURCE:反抗 , 1.20
		CASE 2
			SOURCE:情欲 += 20
			TIMES SOURCE:反抗 , 1.00
		CASE 3
			SOURCE:情欲 += 50
			TIMES SOURCE:反抗 , 0.70
		CASE 4
			SOURCE:情欲 += 75
			TIMES SOURCE:反抗 , 0.50
		CASE 5
			SOURCE:情欲 += 80
			TIMES SOURCE:反抗 , 0.30
		CASE 6
			SOURCE:情欲 += 100
			TIMES SOURCE:反抗 , 0.10
		CASE 7
			SOURCE:情欲 += 120
			TIMES SOURCE:反抗 , 0.10
		CASE 8
			SOURCE:情欲 += 150
			TIMES SOURCE:反抗 , 0.10
		CASE 9
			SOURCE:情欲 += 180
			TIMES SOURCE:反抗 , 0.10
		CASEELSE
			SOURCE:情欲 += 200
			TIMES SOURCE:反抗 , 0.10
	ENDSELECT
ENDIF
;Aへの痛苦は先に処理しておく
;EXP:Ａ经验をみる
IF EXP:Ａ经验 < EXPLV:1
	TIMES SOURCE:疼痛 , 3.00
	TIMES SOURCE:反抗 , 1.50
ELSEIF EXP:Ａ经验 < EXPLV:2
	TIMES SOURCE:疼痛 , 1.20
	TIMES SOURCE:反抗 , 1.20
ELSEIF EXP:Ａ经验 < EXPLV:3
	TIMES SOURCE:疼痛 , 0.80
ELSEIF EXP:Ａ经验 < EXPLV:4
	TIMES SOURCE:疼痛 , 0.60
ELSEIF EXP:Ａ经验 < EXPLV:5
	TIMES SOURCE:疼痛 , 0.40
ELSEIF EXP:Ａ经验 >= EXPLV:5
	TIMES SOURCE:疼痛 , 0.20
ENDIF

;PALAM:润滑をみる
IF PALAM:4 < PALAMLV:1
	TIMES SOURCE:疼痛 , 1.20
	TIMES SOURCE:反抗 , 1.20
ELSEIF PALAM:4 < PALAMLV:2
	TIMES SOURCE:疼痛 , 1.00
ELSEIF PALAM:4 < PALAMLV:3
	TIMES SOURCE:疼痛 , 0.60
ELSEIF PALAM:4 < PALAMLV:4
	TIMES SOURCE:疼痛 , 0.30
ELSEIF PALAM:4 >= PALAMLV:4
	TIMES SOURCE:疼痛 , 0.20
ENDIF
;中毒による中毒充足のソースは先に処理しておく
;自慰成瘾を見る
IF ABL:自慰成瘾 == 1
	TIMES SOURCE:快Ａ , 1.00
	TIMES SOURCE:中毒充足 , 0.00
ELSEIF ABL:自慰成瘾 == 2
	TIMES SOURCE:快Ａ , 1.10
	TIMES SOURCE:中毒充足 , 0.30
ELSEIF ABL:自慰成瘾 == 3
	TIMES SOURCE:快Ａ , 1.20
	TIMES SOURCE:中毒充足 , 0.60
ELSEIF ABL:自慰成瘾 == 4
	TIMES SOURCE:快Ａ , 1.30
	TIMES SOURCE:中毒充足 , 0.90
ELSEIF ABL:自慰成瘾 == 5
	TIMES SOURCE:快Ａ , 1.50
	TIMES SOURCE:中毒充足 , 1.20
ELSEIF ABL:自慰成瘾 == 6
	TIMES SOURCE:快Ａ , 1.50
	TIMES SOURCE:中毒充足 , 1.50
ELSEIF ABL:自慰成瘾 == 7
	TIMES SOURCE:快Ａ , 1.70
	TIMES SOURCE:中毒充足 , 1.80
ELSEIF ABL:自慰成瘾 == 8
	TIMES SOURCE:快Ａ , 2.00
	TIMES SOURCE:中毒充足 , 2.30
ELSEIF ABL:自慰成瘾 == 9
	TIMES SOURCE:快Ａ , 2.50
	TIMES SOURCE:中毒充足 , 3.00
ELSEIF ABL:自慰成瘾 >= 10
	TIMES SOURCE:快Ａ , 3.50
	TIMES SOURCE:中毒充足 , 5.00
ENDIF

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;手套をしているとき
IF TEQUIP:特殊１ == 1
	IF TEQUIP:肛塞 || TEQUIP:后庭拉珠 || TEQUIP:灌肠器＋塞头 || TEQUIP:扩张气球
	ELSEIF TALENT:男性
		;奴隶の特殊１⇔奴隶のＡの汚れが移动
		STAIN:8 |= STAIN:4
		STAIN:4 |= STAIN:8
	ENDIF
ELSE
	IF TEQUIP:肛塞 || TEQUIP:后庭拉珠 || TEQUIP:灌肠器＋塞头 || TEQUIP:扩张气球
	ELSEIF TALENT:男性
		;奴隶の指⇔奴隶のＡの汚れが移动
		STAIN:1 |= STAIN:2
		STAIN:2 |= STAIN:1
	ENDIF
ENDIF

;淋浴アナニーの場合汚れを重置、润滑更に半分。
IF TEQUIP:32 == 1
	STAIN:1 = 0
	STAIN:2 = 2
	STAIN:3 = 1
	STAIN:4 = 8
	STAIN:5 = 0
	STAIN:7 = 0
	STAIN:8 = 0
	STAIN:9 = 0
	STAIN:12 = 0
	STAIN:13 = 0
	STAIN:14 = 0
	STAIN:15 = 0
	STAIN:16 = 0
	STAIN:17 = 0
	PALAM:4 /= 2
ENDIF



;-------------------------------------------------
;经验の処理
;-------------------------------------------------
EXP:Ａ经验 += 1
PRINTL Ａ经验＋１
;视频拍摄時は经验にプラス
IF TEQUIP:摄像机
	EXP:自慰经验 += 2
	PRINTL 自慰经验＋２
	EXP:调教自慰经验 += 2
	PRINTL 调教自慰经验＋２
ELSE
	EXP:自慰经验 += 1
	PRINTL 自慰经验＋１
	EXP:调教自慰经验 += 1
	PRINTL 调教自慰经验＋１
ENDIF
;公開アナニー時は异常经验付加
IF TEQUIP:摄像机 && CFLAG:3 == 0
	EXP:异常经验 += 1
	PRINTL 公开肛门自慰初体验的异常经验＋１
	CFLAG:3 = 1
ENDIF

TFLAG:90 = 0
;摄影時の调教内容チェック
IF TEQUIP:13 && TEQUIP:14
	TFLAG:90 = 993
ELSEIF TEQUIP:14
	TFLAG:90 = 14
ELSEIF TEQUIP:32
	TFLAG:90 = 994
ENDIF

;蔷薇经验とか加算
CALL COM_EXP(4)

;依赖度ベクトル
TFLAG:33 = 3

;依赖度補正値(服从で1、隶属で2）
IF TALENT:服从 && ASSIPLAY == 0
	TFLAG:30 += 1
ELSEIF TALENT:隶属 && ASSIPLAY == 0
	TFLAG:30 += 2
ENDIF

RETURN 1

