;-------------------------------------------------
;抚摸翅膀
;-------------------------------------------------
@COM15

PRINTL 抚摸翅膀
STR:0 = 抚摸翅膀

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、气力
LOSEBASE:0 += 5
LOSEBASE:1 += 50

SOURCE:0 = 80

IF TALENT:230
    SOURCE:22 = 120
ELSEIF TALENT:231
    SOURCE:22 = 80
ELSE
    SOURCE:22 = 100
ENDIF

SOURCE:4 = 50
SOURCE:5 = 60
SOURCE:13 = 20
SOURCE:15 = 30
SOURCE:17 = 20
SOURCE:18 = 20

;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;性压抑、抵抗持ちには、抑郁ソース追加
SIF TALENT:32 || TALENT:34
	SOURCE:16 += 20

;喜欢身体接触だと情爱のソース追加
IF TALENT:65
	SOURCE:4 += 50
ENDIF

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
IF (STAIN:0 >= 2 && (STAIN:0 != 16 && STAIN:0 != 17) && ASSIPLAY == 0 && TALENT:MASTER:64 == 0) || TEQUIP:20
;奴隶の口の汚れがあるとき接吻はない（愛液と母乳よごれはＯＫ、脏污无视か助手调教時は問題无）
;口塞球使用中も无
	SOURCE:15 = 0
	SOURCE:0 /= 2
	SOURCE:4 /= 4
	SOURCE:10 /= 2
ELSE
	;污臭钝感
	SIF TALENT:61
		SOURCE:15 /= 4
	;污臭敏感
	SIF TALENT:62
		SOURCE:15 *= 3
	;自尊心
	SIF TALENT:15
		SOURCE:15 *= 2
	IF STAIN:PLAYER:0
	;主人の口の汚れがあると不洁150%
		SOURCE:15 *= 3
		SOURCE:15 /= 2
	ENDIF
ENDIF

;-------------------------------------------------
;经验の処理
;-------------------------------------------------
;蔷薇经验とか加算
CALL COM_EXP(3)


;依赖度ベクトル
TFLAG:33 = 1

RETURN 1