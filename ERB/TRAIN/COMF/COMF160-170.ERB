;-------------------------------------------------
;触手召喚および派生指令
;触手系指令
;-------------------------------------------------
@COM160
PRINTL 召唤触手
STR:0 = 召唤触手


;丝袜びりびりフラグ
IF TEQUIP:1 == 5 || TEQUIP:1 == 6
	TFLAG:55 = 1
ENDIF


;着脱の処理
IF TEQUIP:25 == 0
	;触手系指令に影響する装备は取下（道具が触手扱いになるため）
	TEQUIP:11 = 0
	TEQUIP:12 = 0
	TEQUIP:13 = 0
	TEQUIP:14 = 0
	TEQUIP:16 = 0
	TEQUIP:17 = 0
	TEQUIP:19 = 0
	TEQUIP:21 = 0
	TEQUIP:38 = 0
;触手解除時はソース、经验の加算は行わず结束
ELSE
	CALL KOJO_MESSAGE_COM
	TEQUIP:25 = 0
	;触手系指令の装备も解除する
	TEQUIP:11 = 0
	TEQUIP:12 = 0
	TEQUIP:13 = 0
	TEQUIP:14 = 0
	TEQUIP:16 = 0
	TEQUIP:17 = 0
	TEQUIP:19 = 0
	TEQUIP:21 = 0
	TEQUIP:36 = 0
	TEQUIP:38 = 0
	
	RETURN 1
ENDIF


;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、气力
LOSEBASE:0 += 200
LOSEBASE:1 += 300

SOURCE:8 = 2500
SOURCE:17 = 2500
SOURCE:18 = 2500


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;触手经验は先に見ておく
IF EXP:55 < EXPLV:1
	TIMES LOSEBASE:0 , 1.20
	TIMES LOSEBASE:1 , 1.20
	TIMES SOURCE:8 , 1.20
	TIMES SOURCE:17 , 1.20
	TIMES SOURCE:18 , 1.20
ELSEIF EXP:55 < EXPLV:2
	TIMES LOSEBASE:0 , 0.90
	TIMES LOSEBASE:1 , 0.90
	TIMES SOURCE:8 , 0.90
	TIMES SOURCE:17 , 0.90
	TIMES SOURCE:18 , 0.90
ELSEIF EXP:55 < EXPLV:3
	TIMES LOSEBASE:0 , 0.80
	TIMES LOSEBASE:1 , 0.80
	TIMES SOURCE:8 , 0.80
	TIMES SOURCE:17 , 0.80
	TIMES SOURCE:18 , 0.80
ELSEIF EXP:55 < EXPLV:4
	TIMES LOSEBASE:0 , 0.70
	TIMES LOSEBASE:1 , 0.70
	TIMES SOURCE:8 , 0.70
	TIMES SOURCE:17 , 0.70
	TIMES SOURCE:18 , 0.70
ELSEIF EXP:55 < EXPLV:5
	TIMES LOSEBASE:0 , 0.60
	TIMES LOSEBASE:1 , 0.60
	TIMES SOURCE:8 , 0.60
	TIMES SOURCE:17 , 0.60
	TIMES SOURCE:18 , 0.60
ELSE
	TIMES LOSEBASE:0 , 0.50
	TIMES LOSEBASE:1 , 0.50
	TIMES SOURCE:8 , 0.50
	TIMES SOURCE:17 , 0.50
	TIMES SOURCE:18 , 0.50
ENDIF


;-------------------------------------------------
;经验の処理
;-------------------------------------------------
IF EXP:55 == 0
	EXP:50 += 1
	PRINTL 异常经验＋1
ENDIF
PRINT 触手经验＋1
EXP:55 += 1

CALL KOJO_MESSAGE_COM
TEQUIP:25 = 1
RETURN 1

;-------------------------------------------------
;装着継続の処理
;-------------------------------------------------
@EQUIP_COM160
;触手召喚中
PRINTL ＜触手召唤中＞

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、气力
LOSEBASE:0 += 150
LOSEBASE:1 += 200

SOURCE:8 += 1000
SOURCE:17 += 1000
SOURCE:18 += 1000


;-------------------------------------------------
;射精チェック
;-------------------------------------------------
IF MAXBASE:MASTER:4 != 0
	B = 1000
	;ABL:技巧をみる
	IF ABL:2 == 0
		TIMES B , 1.00
	ELSEIF ABL:2 == 1
		TIMES B , 1.10
	ELSEIF ABL:2 == 2
		TIMES B , 1.20
	ELSEIF ABL:2 == 3
		TIMES B , 1.30
	ELSEIF ABL:2 == 4
		TIMES B , 1.40
	ELSE
		TIMES B , 1.50
	ENDIF


	;ABL:顺从をみる
	IF ABL:0 == 0
		TIMES B , 0.80
	ELSEIF ABL:0 == 1
		TIMES B , 0.90
	ELSEIF ABL:0 == 2
		TIMES B , 1.00
	ELSEIF ABL:0 == 3
		TIMES B , 1.10
	ELSEIF ABL:0 == 4
		TIMES B , 1.20
	ELSE
		TIMES B , 1.30
	ENDIF

	;PALAM:情欲をみる
	IF PALAM:5 < PALAMLV:1
		TIMES B , 1.00
	ELSEIF PALAM:5 < PALAMLV:2
		TIMES B , 1.10
	ELSEIF PALAM:5 < PALAMLV:3
		TIMES B , 1.20
	ELSEIF PALAM:5 < PALAMLV:4
		TIMES B , 1.30
	ELSEIF PALAM:5 < PALAMLV:5
		TIMES B , 1.40
	ELSE
		TIMES B , 1.50
	ENDIF

	;触手插入
	SIF TEQUIP:13
		TIMES B , 1.50
	;触手插肛
	SIF TEQUIP:14
		TIMES B , 1.50
	;触手揉阴
	SIF TEQUIP:11
		TIMES B , 1.20
	;触手揉乳
	SIF TEQUIP:16
		TIMES B , 1.20
	;触手挤奶
	SIF TEQUIP:17
		TIMES B , 1.30
	;触手玩弄阴茎
	SIF TEQUIP:12
		TIMES B , 1.10
	;触手紧缚
	SIF TEQUIP:19
		TIMES B , 1.20
	;触手灌肠
	SIF TEQUIP:21
		TIMES B , 1.30
	;触手口辱
	SIF TEQUIP:36
		TIMES B , 1.50
	;触手尿道插入
	SIF TEQUIP:38
		TIMES B , 1.50

	BASE:MASTER:4 += B
ENDIF
;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;触手经验は先に見ておく
IF EXP:55 < EXPLV:1
	TIMES LOSEBASE:0 , 1.20
	TIMES LOSEBASE:1 , 1.20
	TIMES SOURCE:8 , 1.20
	TIMES SOURCE:17 , 1.20
	TIMES SOURCE:18 , 1.20
ELSEIF EXP:55 < EXPLV:2
	TIMES LOSEBASE:0 , 0.90
	TIMES LOSEBASE:1 , 0.90
	TIMES SOURCE:8 , 0.90
	TIMES SOURCE:17 , 0.90
	TIMES SOURCE:18 , 0.90
ELSEIF EXP:55 < EXPLV:3
	TIMES LOSEBASE:0 , 0.80
	TIMES LOSEBASE:1 , 0.80
	TIMES SOURCE:8 , 0.80
	TIMES SOURCE:17 , 0.80
	TIMES SOURCE:18 , 0.80
ELSEIF EXP:55 < EXPLV:4
	TIMES LOSEBASE:0 , 0.70
	TIMES LOSEBASE:1 , 0.70
	TIMES SOURCE:8 , 0.70
	TIMES SOURCE:17 , 0.70
	TIMES SOURCE:18 , 0.70
ELSEIF EXP:55 < EXPLV:5
	TIMES LOSEBASE:0 , 0.60
	TIMES LOSEBASE:1 , 0.60
	TIMES SOURCE:8 , 0.60
	TIMES SOURCE:17 , 0.60
	TIMES SOURCE:18 , 0.60
ELSE
	TIMES LOSEBASE:0 , 0.50
	TIMES LOSEBASE:1 , 0.50
	TIMES SOURCE:8 , 0.50
	TIMES SOURCE:17 , 0.50
	TIMES SOURCE:18 , 0.50
ENDIF


;-------------------------------------------------
;射精チェック
;-------------------------------------------------
S = BASE:MASTER:4
EJAC = MAXBASE:MASTER:4

IF  S > EJAC * 2
	E = 2
ELSEIF S > EJAC
	E = 1
ELSE
	E = 0
ENDIF

;射精時に中毒充足のソースを追加、なお触手は射精の再装填は无
;通常射精時
SIF E == 1
	A = 200
;大量射精時
SIF E == 2
	A = 500

IF E > 0
	;精液成瘾を見る
	IF ABL:16 == 1
		TIMES A , 0.00
	ELSEIF ABL:16 == 2
		TIMES A , 0.30
	ELSEIF ABL:16 == 3
		TIMES A , 0.60
	ELSEIF ABL:16 == 4
		TIMES A , 0.90
	ELSEIF ABL:16 == 5
		TIMES A , 1.20
	ELSEIF ABL:16 == 6
		TIMES A , 1.50
	ELSEIF ABL:16 == 7
		TIMES A , 1.80
	ELSEIF ABL:16 == 8
		TIMES A , 2.30
	ELSEIF ABL:16 == 9
		TIMES A , 3.00
	ELSEIF ABL:16 >= 10
		TIMES A , 5.00
	ENDIF
ENDIF

SOURCE:9 += A

;射精時の処理
;大量射精
IF E == 2
	EXP:20 += 3
	PRINTL 触手大量射精
	PRINTL 精液经验＋３
	觸手 += 1

	BASE:MASTER:4 -= EJAC*2
	SIF BASE:MASTER:4 >= EJAC
		BASE:MASTER:4 = EJAC-1

;通常の射精
ELSEIF E == 1
	EXP:20 += 1
	PRINTL 触手射精
	PRINTL 精液经验＋１
	觸手 += 1

	BASE:MASTER:4 -= EJAC
	SIF BASE:MASTER:4 >= EJAC
		BASE:MASTER:4 = EJAC-1
ENDIF

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;触手が射精した場合の粘液汚れ
IF E > 0
	;触手插入
	SIF TEQUIP:13
		STAIN:6 |= 32
	;触手插肛
	SIF TEQUIP:14
		STAIN:4 |= 32
	;触手揉阴
	SIF TEQUIP:11
		STAIN:3 |= 32
	;触手揉乳
	SIF TEQUIP:16
		STAIN:5 |= 32
	;触手挤奶
	SIF TEQUIP:17
		STAIN:5 |= 32
	;触手玩弄阴茎
	SIF TEQUIP:12
		STAIN:2 |= 32
	;触手紧缚（全身にぶっかけるイメージ）
	IF TEQUIP:19
		STAIN:0 |= 32
		STAIN:1 |= 32
		;阴茎があれば阴茎、なければヴァギナへ
		IF TALENT:PLAYER:137 || TALENT:PLAYER:139 || TALENT:PLAYER:140
			STAIN:2 |= 32
		ELSE
			STAIN:3 |= 32
		ENDIF
		STAIN:5 |= 32
	ENDIF
	;触手灌肠
	SIF TEQUIP:21
		STAIN:4 |= 32
	;触手口辱
	SIF TEQUIP:36
		STAIN:0 |= 32
	;触手尿道插入
	SIF TEQUIP:38
		STAIN:2 |= 32
	;服を着てたら全部汚しちゃいます
	SIF TEQUIP:0
		STAIN:8 |= 32
	SIF TEQUIP:1
		STAIN:9 |= 32
	SIF TEQUIP:2
		STAIN:10 |= 32
	SIF TEQUIP:3
		STAIN:11 |= 32
	SIF TEQUIP:4
		STAIN:12 |= 32
	SIF TEQUIP:5
		STAIN:13 |= 32
	SIF TEQUIP:6
		STAIN:14 |= 32
	SIF TEQUIP:7
		STAIN:15 |= 32
	SIF TEQUIP:8
		STAIN:16 |= 32
	SIF TEQUIP:9
		STAIN:17 |= 32
ENDIF


;-------------------------------------------------
;经验の処理
;-------------------------------------------------
;各触手派生指令から集めた触手经验を一括処理
觸手 += 1
PRINT 触手经验＋
PRINTVL 觸手
EXP:55 += 觸手
觸手 = 0

;触手装着中は依赖度ベクトル強制变更
;依赖度ベクトル
TFLAG:33 = 5

;依赖度補正値(服从で1、隶属で2　強制变更なので、これまでの数値を重置）
IF TALENT:5 && ASSIPLAY == 0
	TFLAG:30 = 2
ELSEIF TALENT:8 && ASSIPLAY == 0
	TFLAG:30 = 4
ENDIF


RETURN 1



