;-------------------------------------------------
;避孕套精飲(助手)
;特殊指令
;-------------------------------------------------
@COM193
PRINTL 避孕套精饮(助手)
STR:0 = 避孕套精饮(助手)



;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、气力
LOSEBASE:0 += 0
LOSEBASE:1 += 30

SOURCE:4 = 100
SOURCE:11 = 100
SOURCE:14 = 800
SOURCE:17 = 200
SOURCE:18 = 200


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;性压抑、抵抗持ちには、抑郁ソース追加
SIF TALENT:32 || TALENT:34
	SOURCE:16 += 50

;助手と奴隶の相性によって、全体のソースが変化
T = NO:TARGET
A = NO:ASSI
SOURCE:4 *= RELATION:A
SOURCE:11 *= RELATION:A
SOURCE:14 *= RELATION:A
SOURCE:16 *= RELATION:A
SOURCE:17 *= RELATION:A
SOURCE:18 *= RELATION:A
SOURCE:4 /= 100
SOURCE:11 /= 100
SOURCE:14 /= 100
SOURCE:16 /= 100
SOURCE:17 /= 100
SOURCE:18 /= 100

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;助手の口に精液汚れ
STAIN:ASSI:0 |= 4

;-------------------------------------------------
;经验の処理
;-------------------------------------------------
;避孕套の精液を初期化
CFLAG:107 = 0


RETURN 1

