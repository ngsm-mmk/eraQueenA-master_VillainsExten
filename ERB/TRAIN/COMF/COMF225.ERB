;-------------------------------------------------
;插入命令
;助手命令指令、レベル
;-------------------------------------------------
@COM225
;-------------------------------------------------
;特殊派生処理
;-------------------------------------------------
PRINTL 插入命令
STR:0 = 插入•助手命令


;丝袜びりびりフラグ
IF TEQUIP:1 == 5 || TEQUIP:1 == 6
	TFLAG:55 = 1
ENDIF

;V经验を伴う指令のフラグ(处女膜再生对応)
TFLAG:19 = 1

;助手が童贞だった場合の喪失フラグ
IF TALENT:ASSI:160
	PRINTFORMW %CALLNAME:ASSI%童贞丧失
	TALENT:ASSI:160 = 0
ENDIF

;着脱の処理
IF TEQUIP:47 == 0
	TEQUIP:47 = 1
ELSE
	TEQUIP:47 = 0
ENDIF
;-------------------------------------------------
;射精槽チェック（助手）
;-------------------------------------------------
B = 1200

;ABL:助手の技巧をみる
IF ABL:ASSI:2 == 0
	TIMES B , 1.00
ELSEIF ABL:ASSI:2 == 1
	TIMES B , 1.20
ELSEIF ABL:ASSI:2 == 2
	TIMES B , 1.40
ELSEIF ABL:ASSI:2 == 3
	TIMES B , 1.60
ELSEIF ABL:ASSI:2 == 4
	TIMES B , 1.80
ELSE
	TIMES B , 2.00
ENDIF

;ABL:顺从をみる
IF ABL:0 == 0
	TIMES B , 0.80
ELSEIF ABL:0 == 1
	TIMES B , 0.90
ELSEIF ABL:0 == 2
	TIMES B , 1.00
ELSEIF ABL:0 == 3
	TIMES B , 1.10
ELSEIF ABL:0 == 4
	TIMES B , 1.20
ELSE
	TIMES B , 1.30
ENDIF

;ABL:欲望をみる
IF ABL:1 == 0
	TIMES B , 1.00
ELSEIF ABL:1 == 1
	TIMES B , 1.10
ELSEIF ABL:1 == 2
	TIMES B , 1.20
ELSEIF ABL:1 == 3
	TIMES B , 1.30
ELSEIF ABL:1 == 4
	TIMES B , 1.40
ELSE
	TIMES B , 1.50
ENDIF

;PALAM:润滑をみる

IF PALAM:4 < PALAMLV:1
	TIMES B , 0.60
ELSEIF PALAM:4 < PALAMLV:2
	TIMES B , 0.80
ELSEIF PALAM:4 < PALAMLV:3
	TIMES B , 1.00
ELSEIF PALAM:4 < PALAMLV:4
	TIMES B , 1.20
ELSEIF PALAM:4 >= PALAMLV:4
	TIMES B , 1.40
ENDIF


;助手のABL:Ｃ感觉をみる
IF ABL:ASSI:3 == 0
	TIMES B , 1.00
ELSEIF ABL:ASSI:3 == 1
	TIMES B , 1.50
ELSEIF ABL:ASSI:3 == 2
	TIMES B , 2.00
ELSEIF ABL:ASSI:3 == 3
	TIMES B , 2.50
ELSEIF ABL:ASSI:3 == 4
	TIMES B , 3.50
ELSE
	TIMES B , 5.00
ENDIF

BASE:ASSI:2 += B

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、气力
LOSEBASE:0 += 30
LOSEBASE:1 += 100

SOURCE:1 = 100
SOURCE:7 = 50
SOURCE:13 = 20
SOURCE:17 = 40
SOURCE:18 = 40


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;性压抑、抵抗持ちには、抑郁ソース追加
SIF TALENT:32 || TALENT:34
	SOURCE:16 += 40

;处女だった場合は、痛苦のソースと反抗のソースを追加する
IF TALENT:0 == 1
	SOURCE:7 += 2000
	SOURCE:18 += 3000
ENDIF

;再生处女だった場合は、痛苦のソースと反抗のソースを追加する
IF TALENT:1 == 1
	SOURCE:7 += 500
	SOURCE:18 += 1000
ENDIF
;助手の技巧により快Ｖソース追加、下手すぎると疼痛のソース追加
IF ABL:ASSI:2 == 0
	SOURCE:7 += 100
ELSEIF ABL:ASSI:2 == 1
	SOURCE:7 += 50
ELSEIF ABL:ASSI:2 == 2
	SOURCE:0 += 50
ELSEIF ABL:ASSI:2 == 3
	SOURCE:0 += 100
ELSEIF ABL:ASSI:2 == 4
	SOURCE:0 += 150
ELSE
	SOURCE:0 += 200
ENDIF
;助手が小人体型で、调教对象が小人体型でなく性倒错でもないと逸脱のソース追加
SIF TALENT:142 == 0 && TALENT:ASSI:142 && TALENT:80
	SOURCE:17 += 50	
;Vへの痛苦は先に処理しておく
;EXP:Ｖ经验をみる（处女の反感は別途処理済みなので省略）
IF EXP:0 < EXPLV:1
	TIMES SOURCE:7 , 2.50
ELSEIF EXP:0 < EXPLV:2
	TIMES SOURCE:7 , 0.80
	TIMES SOURCE:18 , 1.20
ELSEIF EXP:0 < EXPLV:3
	TIMES SOURCE:7 , 0.40
ELSEIF EXP:0 < EXPLV:4
	TIMES SOURCE:7 , 0.10
ELSEIF EXP:0 < EXPLV:5
	TIMES SOURCE:7 , 0.00
ELSEIF EXP:0 >= EXPLV:5
	TIMES SOURCE:7 , 0.00
ENDIF

;PALAM:润滑をみる
IF PALAM:4 < PALAMLV:1
	TIMES SOURCE:7 , 1.20
	TIMES SOURCE:18 , 1.50
ELSEIF PALAM:4 < PALAMLV:2
	TIMES SOURCE:7 , 0.70
	TIMES SOURCE:18 , 1.20
ELSEIF PALAM:4 < PALAMLV:3
	TIMES SOURCE:7 , 0.40
ELSEIF PALAM:4 < PALAMLV:4
	TIMES SOURCE:7 , 0.10
ELSEIF PALAM:4 >= PALAMLV:4
	TIMES SOURCE:7 , 0.10
ENDIF

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;奴隶のＶ⇔助手の阴茎の汚れが移动
STAIN:3 |= STAIN:ASSI:2
STAIN:ASSI:2 |= STAIN:3
;奴隶の膣内⇔助手の阴茎の汚れが移动
STAIN:6 |= STAIN:ASSI:2
STAIN:ASSI:2 |= STAIN:6
;-------------------------------------------------
;经验の処理
;-------------------------------------------------
;V经验
EXP:0 += 1
PRINTL Ｖ经验＋１

;奴隶と助手に性交经验
EXP:4 += 1
EXP:ASSI:4 += 1
;助手に快Ｃ・学习・恭顺・否定之珠
JUEL:ASSI:0 += 100
JUEL:ASSI:5 += 100
JUEL:ASSI:8 += 100
JUEL:ASSI:100 += 100
PRINTFORML %CALLNAME:ASSI%的%PALAMNAME:0%之珠＋100
PRINTFORML %CALLNAME:ASSI%的%PALAMNAME:5%之珠＋100
PRINTFORMW %CALLNAME:ASSI%的%PALAMNAME:8%之珠＋100
PRINTFORMW %CALLNAME:ASSI%的%PALAMNAME:100%之珠＋100

;异常经验
IF TALENT:0 == 1 && TEQUIP:25 == 1
	EXP:50 += 2
	PRINTFORML %EXPNAME:50%＋２
ELSEIF TALENT:0 == 1
	EXP:50 += 1
	PRINTFORML %EXPNAME:50%＋１
ENDIF

;百合经验
A = 5
B = NO:ASSI + 1199
IF (TALENT:137 == 0 && TALENT:140 == 0) && (TALENT:ASSI:137 == 0 && TALENT:ASSI:140 == 0)
	PRINTFORML %EXPNAME:40%+{A}
	EXP:40 += A
ELSEIF (TALENT:137 || TALENT:140) && (TALENT:ASSI:137 || TALENT:ASSI:140)
	PRINTFORML %EXPNAME:41%+{A}
	EXP:41 += A
ENDIF
;调教者を記録
CFLAG:B += A


;依赖度ベクトル
TFLAG:33 = 1


RETURN 1


