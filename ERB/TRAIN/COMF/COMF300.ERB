;--------------------------------------------------
;触手乳腺插入
;--------------------------------------------------
@COM300
PRINTL 触手玩弄乳腺
STR:0 = 触手玩弄乳腺


IF TEQUIP:40
	LOSEBASE:0 += 450
	LOSEBASE:1 += 2500
ELSE
	LOSEBASE:0 += 500
	LOSEBASE:1 += 3000
ENDIF

IF TALENT:149
	LOSEBASE:0 /= 2
	LOSEBASE:1 /= 2
ENDIF

;ABL:Ｂ感觉をみる
IF ABL:6 == 0
	SOURCE:3 = 50
ELSEIF ABL:6 == 1
	SOURCE:3 = 100
ELSEIF ABL:6 == 2
	SOURCE:3 = 200
ELSEIF ABL:6 == 3
	SOURCE:3 = 800
ELSEIF ABL:6 == 4
	SOURCE:3 = 1600
ELSE
	SOURCE:3 = 2800
ENDIF

;EXP:触手经验をみる
IF EXP:55 < EXPLV:1
	SOURCE:7 = 10000
	SOURCE:15 = 10000
	SOURCE:17 = 10000
ELSEIF EXP:55 < EXPLV:2
	SOURCE:7 = 5000
	SOURCE:15 = 6500
	SOURCE:17 = 8000
ELSEIF EXP:55 < EXPLV:3
	SOURCE:7 = 3000
	SOURCE:15 = 4500
	SOURCE:17 = 6000
ELSEIF EXP:55 < EXPLV:4
	SOURCE:7 = 2000
	SOURCE:15 = 3000
	SOURCE:17 = 4000
	TIMES LOSEBASE:0 , 0.90
ELSEIF EXP:55 < EXPLV:5
	SOURCE:7 = 1000
	SOURCE:15 = 1500
	SOURCE:17 = 2000
	TIMES LOSEBASE:0 , 0.70
ELSE
	SOURCE:7 = 500
	SOURCE:15 = 750
	SOURCE:17 = 1000
	TIMES LOSEBASE:0 , 0.50
ENDIF

;EXP:触手经验をみる
IF EXP:55 < EXPLV:1
	SOURCE:11 = 0
	TIMES SOURCE:5 , 0.50
	TIMES SOURCE:6 , 0.50
	TIMES SOURCE:15 , 2.00
	TIMES SOURCE:17 , 4.00
ELSEIF EXP:55 < EXPLV:2
	SOURCE:11 = 60
	TIMES SOURCE:5 , 0.80
	TIMES SOURCE:6 , 0.80
	TIMES SOURCE:15 , 1.50
	TIMES SOURCE:17 , 1.80
	TIMES LOSEBASE:1 , 0.90
ELSEIF EXP:55 < EXPLV:3
	SOURCE:11 = 180
	TIMES SOURCE:5 , 1.00
	TIMES SOURCE:6 , 1.00
	TIMES SOURCE:15 , 1.00
	TIMES SOURCE:17 , 1.00
	TIMES LOSEBASE:1 , 0.80
ELSEIF EXP:55 < EXPLV:4
	SOURCE:11 = 360
	TIMES SOURCE:5 , 1.20
	TIMES SOURCE:6 , 1.20
	TIMES SOURCE:15 , 0.50
	TIMES SOURCE:17 , 0.70
	TIMES LOSEBASE:1 , 0.70
ELSEIF EXP:55 < EXPLV:5
	SOURCE:11 = 640
	TIMES SOURCE:5 , 1.50
	TIMES SOURCE:6 , 1.50
	TIMES SOURCE:15 , 0.10
	TIMES SOURCE:17 , 0.20
	TIMES LOSEBASE:1 , 0.60
ELSE
	SOURCE:11 = 900
	TIMES SOURCE:5 , 2.00
	TIMES SOURCE:6 , 2.00
	TIMES SOURCE:15 , 0.00
	TIMES SOURCE:17 , 0.10
	TIMES LOSEBASE:1 , 0.50
ENDIF

;Ｂ敏感・钝感
IF TALENT:108
	TIMES SOURCE:3 , 1.20
ELSEIF TALENT:107 || TALENT:男性
	TIMES SOURCE:3 , 0.70
ENDIF
;绝壁・贫乳・巨乳・爆乳
IF TALENT:110
	TIMES SOURCE:7 , 1.75
ELSEIF TALENT:109
	TIMES SOURCE:7 , 1.50
ELSEIF TALENT:112
	TIMES SOURCE:3 , 1.50
ELSEIF TALENT:111
	TIMES SOURCE:3 , 1.20
ENDIF
;淫乳
SIF TALENT:77
	TIMES SOURCE:3 , 2.50
EXP:7 += 1
PRINTL Ｍ经验＋1
IF TEQUIP:40
	SIF TALENT:149
		STAIN:5 |= 16
	CALL KOJO_MESSAGE_COM
	TEQUIP:40 = 0
	觸手 = 0
ELSE
	IF TALENT:149 == 0
		IF PALAM:6 > PALAMLV:5
			UP:11 += 3000
			TALENT:149 = 1
			MAXBASE:3 = 10000
		ENDIF
	ELSE
		TIMES SOURCE:7 , 0.60
	ENDIF
	;初体験のとき异常经验+1
	IF EXP:7 == 0
		EXP:50 += 1
		PRINTL 异常经验＋1
	ENDIF
	CALL KOJO_MESSAGE_COM
	TEQUIP:40 = 1
	觸手 = 1
ENDIF

RETURN 1

;--------------------------------------------------
;触手乳腺插入中
;--------------------------------------------------
@EQUIP_COM300
PRINTL ＜触手乳腺责罚中＞

LOSEBASE:0 += 75
LOSEBASE:1 += 75

;ABL:Ｂ感觉をみる
IF ABL:6 == 0
	A = 20
ELSEIF ABL:6 == 1
	A = 60
ELSEIF ABL:6 == 2
	A = 100
ELSEIF ABL:6 == 3
	A = 250
ELSEIF ABL:6 == 4
	A = 400
ELSE
	A = 600
ENDIF

;PALAM:情欲をみる
IF PALAM:6 < PALAMLV:1
	TIMES A , 0.80
ELSEIF PALAM:6 < PALAMLV:2
	TIMES A , 0.90
ELSEIF PALAM:6 < PALAMLV:3
	TIMES A , 1.00
ELSEIF PALAM:6 < PALAMLV:4
	TIMES A , 1.10
ELSE
	TIMES A , 1.20
ENDIF

;EXP:触手经验をみる
IF EXP:55 <= EXPLV:0
	TIMES A , 0.80
	SOURCE:9 += 10
	SOURCE:17 += 2500
ELSEIF EXP:55 <= EXPLV:1
	TIMES A , 0.90
	SOURCE:9 += 40
	SOURCE:17 += 1500
ELSEIF EXP:55 <= EXPLV:2
	TIMES A , 1.00
	SOURCE:9 += 100
	SOURCE:14 += 10
	SOURCE:17 += 500
ELSEIF EXP:55 <= EXPLV:3
	TIMES A , 1.10
	SOURCE:9 += 150
	SOURCE:14 += 40
	SOURCE:17 += 150
ELSEIF EXP:55 <= EXPLV:4
	TIMES A , 1.20
	SOURCE:9 += 300
	SOURCE:14 += 70
	SOURCE:17 += 10
ELSE
	TIMES A , 1.30
	SOURCE:9 += 450
	SOURCE:14 += 100
ENDIF

;EXP:触手经验をみる
IF EXP:55 <= EXPLV:0
	TIMES A , 0.80
	SOURCE:7 += 500
ELSEIF EXP:55 <= EXPLV:1
	TIMES A , 1.00
	SOURCE:7 += 100
ELSEIF EXP:55 <= EXPLV:2
	TIMES A , 1.30
	SOURCE:7 += 50
ELSEIF EXP:55 <= EXPLV:3
	TIMES A , 1.60
	SOURCE:7 += 10
ELSEIF EXP:55 <= EXPLV:4
	TIMES A , 2.00
ELSE
	TIMES A , 2.25
ENDIF

;EXP:墳乳经验をみる
IF EXP:54 <= EXPLV:0
	TIMES A , 0.80
ELSEIF EXP:54 <= EXPLV:1
	TIMES A , 1.00
ELSEIF EXP:54 <= EXPLV:2
	TIMES A , 1.30
ELSEIF EXP:54 <= EXPLV:3
	TIMES A , 1.60
ELSEIF EXP:54 <= EXPLV:4
	TIMES A , 2.00
ELSE
	TIMES A , 2.25
ENDIF

;Ｂ钝感とＢ敏感
IF TALENT:108
	TIMES A , 1.40
ELSEIF TALENT:107 || TALENT:男性
	TIMES A , 0.60
ENDIF

;巨乳と爆乳、贫乳と绝壁
IF TALENT:112
	TIMES A , 1.20
ELSEIF TALENT:111
	TIMES A , 1.50
ELSEIF TALENT:109
	TIMES SOURCE:7 , 1.50
ELSEIF TALENT:110
	TIMES SOURCE:7 , 1.75
ENDIF
SOURCE:3 += A

STAIN:5 |= 32
觸手 += 1

RETURN 1


