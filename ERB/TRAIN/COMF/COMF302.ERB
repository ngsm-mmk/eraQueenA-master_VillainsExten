IF PALAM:6 < PALAMLV:1
	TIMES SOURCE:1 , 0.10
	SOURCE:7 += 2000
	TIMES SOURCE:7 , 3.00
ELSEIF PALAM:6 < PALAMLV:2
	TIMES SOURCE:1 , 0.20
	SOURCE:7 += 800
	TIMES SOURCE:7 , 1.00
ELSEIF PALAM:6 < PALAMLV:3
	TIMES SOURCE:1 , 0.60
	TIMES SOURCE:7 , 0.80
ELSEIF PALAM:6 < PALAMLV:4
