	TIMES SOURCE:9 , 0.90
	TIMES SOURCE:10 , 0.90
ELSEIF PALAM:13 < PALAMLV:2
	TIMES SOURCE:1 , 1.80
	TIMES SOURCE:9 , 1.00
	TIMES SOURCE:10 , 1.00
ELSEIF PALAM:13 < PALAMLV:3
