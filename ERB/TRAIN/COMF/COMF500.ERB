	SIF PREVCOM == 81 || PREVCOM == 140
		B = 3
	SIF PREVCOM == 205 || PREVCOM == 206 || PREVCOM == 213 || PREVCOM == 214
		B = 9
	;前回指令が3Pでない
	IF TFLAG:42 == 0
		;今回调教者が助手=前回调教者が主人
		IF ASSIPLAY
			TFLAG:40 = B
			TFLAG:41 = A
		;今回调教者が主人=前回调教者が助手
		ELSE
			TFLAG:40 = A
			TFLAG:41 = B
		ENDIF
	;前回指令が3Pである
	ELSE
		;今回调教者が助手=前回调教者が主人
		IF ASSIPLAY
			;助手の使用部位が前回の主人と同じ場合
			SIF A == TFLAG:40
				TFLAG:40 = TFLAG:41
			TFLAG:41 = A
		ELSE
			;主人の使用部位が前回の助手と同じ場合
			SIF A == TFLAG:41
				TFLAG:41 = TFLAG:40
			TFLAG:40 = A
		ENDIF
	ENDIF
