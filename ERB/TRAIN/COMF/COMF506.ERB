;-------------------------------------------------
;插入Ｇ点玩弄
;做爱系派生指令
;-------------------------------------------------
@COM506
PRINTL 插入Ｇ点玩弄
STR:0 = 插入Ｇ点玩弄
SELECTCOM = 506


;V经验を伴う指令のフラグ(处女膜再生对応)
TFLAG:19 = 1
SIF (TALENT:3 || TALENT:6) && ASSIPLAY == 0 && EXP:0 == 0
	TFLAG:20 = 1

;-------------------------------------------------
;射精槽チェック
;-------------------------------------------------
B = 1800

;ABL:调教者の技巧をみる
IF ABL:PLAYER:2 == 0
	TIMES B , 1.00
ELSEIF ABL:PLAYER:2 == 1
	TIMES B , 1.20
ELSEIF ABL:PLAYER:2 == 2
	TIMES B , 1.40
ELSEIF ABL:PLAYER:2 == 3
	TIMES B , 1.60
ELSEIF ABL:PLAYER:2 == 4
	TIMES B , 1.80
ELSE
	TIMES B , 2.00
ENDIF

;ABL:顺从をみる
IF ABL:0 == 0
	TIMES B , 0.80
ELSEIF ABL:0 == 1
	TIMES B , 0.90
ELSEIF ABL:0 == 2
	TIMES B , 1.00
ELSEIF ABL:0 == 3
	TIMES B , 1.10
ELSEIF ABL:0 == 4
	TIMES B , 1.20
ELSE
	TIMES B , 1.30
ENDIF

;ABL:欲望をみる
IF ABL:1 == 0
	TIMES B , 1.00
ELSEIF ABL:1 == 1
	TIMES B , 1.10
ELSEIF ABL:1 == 2
	TIMES B , 1.20
ELSEIF ABL:1 == 3
	TIMES B , 1.30
ELSEIF ABL:1 == 4
	TIMES B , 1.40
ELSE
	TIMES B , 1.50
ENDIF

;PALAM:润滑をみる
IF PALAM:4 < PALAMLV:1
	TIMES B , 0.60
ELSEIF PALAM:4 < PALAMLV:2
	TIMES B , 0.80
ELSEIF PALAM:4 < PALAMLV:3
	TIMES B , 1.00
ELSEIF PALAM:4 < PALAMLV:4
	TIMES B , 1.20
ELSEIF PALAM:4 >= PALAMLV:4
	TIMES B , 1.40
ENDIF

;プ雷ヤーのABL:Ｃ感觉をみる
IF ABL:PLAYER:3 == 0
	TIMES B , 1.00
ELSEIF ABL:PLAYER:3 == 1
	TIMES B , 1.50
ELSEIF ABL:PLAYER:3 == 2
	TIMES B , 2.00
ELSEIF ABL:PLAYER:3 == 3
	TIMES B , 2.50
ELSEIF ABL:PLAYER:3 == 4
	TIMES B , 3.50
ELSE
	TIMES B , 5.00
ENDIF

;EXP:Ｖ经验をみる
;处女だと増える
SIF EXP:0 < EXPLV:1
	TIMES B , 1.50




IF TALENT:PLAYER:137 || TALENT:PLAYER:139 || TALENT:PLAYER:140
	;再装填処理中は射精槽は20分の1に
	SIF CFLAG:PLAYER:9 > 0
		B /= 20
	BASE:PLAYER:2 += B
ELSE
	B = B/6
	BASE:PLAYER:2 += B
ENDIF

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、气力
LOSEBASE:0 += 60
LOSEBASE:1 += 120

SOURCE:1 = 500
SOURCE:4 = 200
SOURCE:7 = 500
SOURCE:9 = 300
SOURCE:13 = 100
SOURCE:18 = 500


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;性压抑、抵抗持ちには、抑郁ソース追加
SIF TALENT:32 || TALENT:34
	SOURCE:16 += 500

;贞操重视持ちで恋慕、相亲相爱持ち以外には常に反抗のソースを追加する（做爱系のみ）
SIF TALENT:30 && (TALENT:3 == 0 && TALENT:6 == 0)
	SOURCE:18 += 1000

;处女だった場合は、痛苦のソースと反抗のソースを追加する
IF TALENT:0 == 1
	SOURCE:7 += 2000
	SOURCE:18 += 3000
ENDIF

;再生处女だった場合は、痛苦のソースと反抗のソースを追加する
IF TALENT:1 == 1
	SOURCE:7 += 500
	SOURCE:18 += 1000
ENDIF

;Vへの痛苦は先に処理しておく
;EXP:Ｖ经验をみる（处女の反感は別途処理済みなので省略）
IF EXP:0 < EXPLV:1
	TIMES SOURCE:7 , 2.50
ELSEIF EXP:0 < EXPLV:2
	TIMES SOURCE:7 , 0.80
	TIMES SOURCE:18 , 1.20
ELSEIF EXP:0 < EXPLV:3
	TIMES SOURCE:7 , 0.40
ELSEIF EXP:0 < EXPLV:4
	TIMES SOURCE:7 , 0.10
ELSEIF EXP:0 < EXPLV:5
	TIMES SOURCE:7 , 0.00
ELSEIF EXP:0 >= EXPLV:5
	TIMES SOURCE:7 , 0.00
ENDIF

;PALAM:润滑をみる
IF PALAM:4 < PALAMLV:1
	TIMES SOURCE:7 , 1.20
	TIMES SOURCE:18 , 1.50
ELSEIF PALAM:4 < PALAMLV:2
	TIMES SOURCE:7 , 0.70
	TIMES SOURCE:18 , 1.20
ELSEIF PALAM:4 < PALAMLV:3
	TIMES SOURCE:7 , 0.40
ELSEIF PALAM:4 < PALAMLV:4
	TIMES SOURCE:7 , 0.10
ELSEIF PALAM:4 >= PALAMLV:4
	TIMES SOURCE:7 , 0.10
ENDIF

;中毒による中毒充足のソースは先に処理しておく
;性成瘾を見る
IF ABL:18 == 1
	TIMES SOURCE:9 , 0.00
ELSEIF ABL:18 == 2
	TIMES SOURCE:9 , 0.30
ELSEIF ABL:18 == 3
	TIMES SOURCE:9 , 0.60
ELSEIF ABL:18 == 4
	TIMES SOURCE:9 , 0.90
ELSEIF ABL:18 == 5
	TIMES SOURCE:9 , 1.20
ELSEIF ABL:18 == 6
	TIMES SOURCE:9 , 1.50
ELSEIF ABL:18 == 7
	TIMES SOURCE:9 , 1.80
ELSEIF ABL:18 == 8
	TIMES SOURCE:9 , 2.30
ELSEIF ABL:18 == 9
	TIMES SOURCE:9 , 3.00
ELSEIF ABL:18 >= 10
	TIMES SOURCE:9 , 5.00
ENDIF


;-------------------------------------------------
;射精チェック
;-------------------------------------------------
CALL SAMEN_CHECK

;射精時の処理
IF E >= 1
	;射精先タイプの设定
	TFLAG:703 = 1
	;射精先選択の可・不可
	TFLAG:704 = 1
	;汎用的な処理はこの関数で行う
	;（射精槽の再計算や射精经验の上昇と共通部分の表示）
	CALL SAMEN_SHOOT
ENDIF

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;射精に関係なく行われる処理はここから
;膣内に破瓜の血の汚れを付加
SIF TALENT:0 == 1 || TALENT:1 == 1
	STAIN:6 |= 64
;奴隶の膣内⇔调教者のＰの汚れが移动
STAIN:6 |= STAIN:PLAYER:2
STAIN:PLAYER:2 |= STAIN:6

;-------------------------------------------------
;经验の処理
;-------------------------------------------------
EXP:0 += 1
PRINTL Ｖ经验＋１

;蔷薇经验とか加算
CALL COM_EXP(4)


;爱情经验
IF TALENT:3 == 1 && ASSIPLAY == 0
	PRINTS EXPNAME:23
	PRINTL +1
	EXP:23 += 1
ELSEIF TALENT:6 == 1 && ASSIPLAY == 0
	PRINTS EXPNAME:23
	PRINTL +2
	EXP:23 += 2
ENDIF

;依赖度ベクトル
TFLAG:33 = 1

;依赖度補正値(恋慕で1、相亲相爱で2）
IF TALENT:3 && ASSIPLAY == 0
	TFLAG:30 += 1
ELSEIF TALENT:6 && ASSIPLAY == 0
	TFLAG:30 += 2
ENDIF


RETURN 1

