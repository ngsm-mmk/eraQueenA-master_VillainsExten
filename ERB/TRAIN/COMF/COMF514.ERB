;-------------------------------------------------
;駅弁
;做爱系派生指令
;-------------------------------------------------
@COM514
PRINTL 车站便当
STR:0 = 车站便当
SELECTCOM = 514

;-------------------------------------------------
;射精槽チェック
;-------------------------------------------------
B = 1800

;ABL:调教者の技巧をみる
IF ABL:PLAYER:2 == 0
	TIMES B , 1.00
ELSEIF ABL:PLAYER:2 == 1
	TIMES B , 1.20
ELSEIF ABL:PLAYER:2 == 2
	TIMES B , 1.40
ELSEIF ABL:PLAYER:2 == 3
	TIMES B , 1.60
ELSEIF ABL:PLAYER:2 == 4
	TIMES B , 1.80
ELSE
	TIMES B , 2.00
ENDIF

;ABL:顺从をみる
IF ABL:0 == 0
	TIMES B , 0.80
ELSEIF ABL:0 == 1
	TIMES B , 0.90
ELSEIF ABL:0 == 2
	TIMES B , 1.00
ELSEIF ABL:0 == 3
	TIMES B , 1.10
ELSEIF ABL:0 == 4
	TIMES B , 1.20
ELSE
	TIMES B , 1.30
ENDIF

;ABL:欲望をみる
IF ABL:1 == 0
	TIMES B , 1.00
ELSEIF ABL:1 == 1
	TIMES B , 1.10
ELSEIF ABL:1 == 2
	TIMES B , 1.20
ELSEIF ABL:1 == 3
	TIMES B , 1.30
ELSEIF ABL:1 == 4
	TIMES B , 1.40
ELSE
	TIMES B , 1.50
ENDIF

;PALAM:润滑をみる
IF PALAM:4 < PALAMLV:1
	TIMES B , 0.60
ELSEIF PALAM:4 < PALAMLV:2
	TIMES B , 0.80
ELSEIF PALAM:4 < PALAMLV:3
	TIMES B , 1.00
ELSEIF PALAM:4 < PALAMLV:4
	TIMES B , 1.20
ELSEIF PALAM:4 >= PALAMLV:4
	TIMES B , 1.40
ENDIF

;プ雷ヤーのABL:Ｃ感觉をみる
IF ABL:PLAYER:3 == 0
	TIMES B , 1.00
ELSEIF ABL:PLAYER:3 == 1
	TIMES B , 1.50
ELSEIF ABL:PLAYER:3 == 2
	TIMES B , 2.00
ELSEIF ABL:PLAYER:3 == 3
	TIMES B , 2.50
ELSEIF ABL:PLAYER:3 == 4
	TIMES B , 3.50
ELSE
	TIMES B , 5.00
ENDIF

;再装填処理中は射精槽は20分の1に
SIF CFLAG:PLAYER:9 > 0
	B /= 20


IF TALENT:PLAYER:137 || TALENT:PLAYER:139 || TALENT:PLAYER:140
	BASE:PLAYER:2 += B
ELSE
	B = B/6
	BASE:PLAYER:2 += B
ENDIF
;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、气力
LOSEBASE:0 += 150
LOSEBASE:1 += 180

SOURCE:4 = 100
SOURCE:6 = 500
SOURCE:7 = 300
SOURCE:9 = 500
SOURCE:13 = 500
SOURCE:18 = 400

;上のほうで計算した汚れデータ
SOURCE:15 = Y*10 + 60

;前回对面立位ならＶ、对面立位肛ならＡ
IF PREVCOM:74
	SOURCE:1 = 400
	SOURCE:9 = 100
ELSEIF PREVCOM:75
	SOURCE:2 = 400
	SOURCE:7 = 100
ENDIF
;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;性压抑、抵抗持ちには、抑郁ソース追加
SIF TALENT:32 || TALENT:34
	SOURCE:16 += 300

;贞操重视持ちで恋慕、相亲相爱持ち以外には常に反抗のソースを追加する（做爱系のみ）
SIF TALENT:30 && (TALENT:3 == 0 && TALENT:6 == 0)
	SOURCE:18 += 1000
	
IF SELECTCOM:74
	;Vへの痛苦は先に処理しておく
	;EXP:Ｖ经验をみる
	IF EXP:0 < EXPLV:1
		TIMES SOURCE:7 , 2.50
	ELSEIF EXP:0 < EXPLV:2
		TIMES SOURCE:7 , 0.80
		TIMES SOURCE:18 , 1.20
	ELSEIF EXP:0 < EXPLV:3
		TIMES SOURCE:7 , 0.40
	ELSEIF EXP:0 < EXPLV:4
		TIMES SOURCE:7 , 0.10
	ELSEIF EXP:0 < EXPLV:5
		TIMES SOURCE:7 , 0.00
	ELSEIF EXP:0 >= EXPLV:5
		TIMES SOURCE:7 , 0.00
	ENDIF
ELSEIF SELECTCOM:75
	;Aへの痛苦は先に処理しておく
	;EXP:Ａ经验をみる
	IF EXP:1 < EXPLV:1
		TIMES SOURCE:7 , 3.00
		TIMES SOURCE:18 , 3.00
	ELSEIF EXP:1 < EXPLV:2
		TIMES SOURCE:7 , 1.20
		TIMES SOURCE:18 , 2.00
	ELSEIF EXP:1 < EXPLV:3
		TIMES SOURCE:7 , 0.80
		TIMES SOURCE:18 , 1.50
	ELSEIF EXP:1 < EXPLV:4
		TIMES SOURCE:7 , 0.60
		TIMES SOURCE:18 , 1.20
	ELSEIF EXP:1 < EXPLV:5
		TIMES SOURCE:7 , 0.40
	ELSEIF EXP:1 >= EXPLV:5
		TIMES SOURCE:7 , 0.20
	ENDIF
ENDIF

;PALAM:润滑をみる
IF PALAM:4 < PALAMLV:1
	TIMES SOURCE:7 , 1.20
	TIMES SOURCE:18 , 2.00
ELSEIF PALAM:4 < PALAMLV:2
	TIMES SOURCE:7 , 1.00
	TIMES SOURCE:18 , 1.50
ELSEIF PALAM:4 < PALAMLV:3
	TIMES SOURCE:7 , 0.60
ELSEIF PALAM:4 < PALAMLV:4
	TIMES SOURCE:7 , 0.30
ELSEIF PALAM:4 >= PALAMLV:4
	TIMES SOURCE:7 , 0.20
ENDIF

;中毒による中毒充足のソースは先に処理しておく
;性成瘾を見る
IF ABL:18 == 1
	TIMES SOURCE:9 , 0.00
ELSEIF ABL:18 == 2
	TIMES SOURCE:9 , 0.30
ELSEIF ABL:18 == 3
	TIMES SOURCE:9 , 0.60
ELSEIF ABL:18 == 4
	TIMES SOURCE:9 , 0.90
ELSEIF ABL:18 == 5
	TIMES SOURCE:9 , 1.20
ELSEIF ABL:18 == 6
	TIMES SOURCE:9 , 1.50
ELSEIF ABL:18 == 7
	TIMES SOURCE:9 , 1.80
ELSEIF ABL:18 == 8
	TIMES SOURCE:9 , 2.30
ELSEIF ABL:18 == 9
	TIMES SOURCE:9 , 3.00
ELSEIF ABL:18 >= 10
	TIMES SOURCE:9 , 5.00
ENDIF

;-------------------------------------------------
;射精チェック
;-------------------------------------------------
CALL SAMEN_CHECK

;射精時の処理
IF E >= 1
	;射精先タイプの设定
	;前回对面立位ならＶ、对面立位肛ならＡ
	IF SELECTCOM:74
		TFLAG:703 = 1
	ELSEIF SELECTCOM:75
		TFLAG:704 = 2
	ENDIF
	;射精先選択の可・不可
	TFLAG:704 = 1
	;汎用的な処理はこの関数で行う
	;（射精槽の再計算や射精经验の上昇と共通部分の表示）
	CALL SAMEN_SHOOT
	;指令ごとに固有の処理はこの下に書く
ENDIF

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;射精に関係なく行われる処理はここから

IF SELECTCOM:74
	;奴隶の膣内⇔调教者のＰの汚れが移动
	STAIN:6 |= STAIN:PLAYER:2
	STAIN:PLAYER:2 |= STAIN:6
ELSEIF SELECTCOM:75
	;奴隶のＡ⇔调教者のＰの汚れが移动
	STAIN:4 |= STAIN:PLAYER:2
	STAIN:PLAYER:2 |= STAIN:4
ENDIF
;-------------------------------------------------
;经验の処理
;-------------------------------------------------
IF SELECTCOM:74
	;V经验
	EXP:0 += 1
	PRINTL Ｖ经验＋１
ELSEIF SELECTCOM:75
	;A经验
	EXP:1 += 2
	PRINTL Ａ经验＋２
ENDIF
	
;性交经验
EXP:4 += 1
EXP:PLAYER:4 += 1
PRINTS EXPNAME:4
PRINTL +1

;蔷薇经验とか加算
CALL COM_EXP(4)


;爱情经验
IF TALENT:3 == 1 && ASSIPLAY == 0
	PRINTS EXPNAME:23
	PRINTL +2
	EXP:23 += 2
ELSEIF TALENT:6 == 1 && ASSIPLAY == 0
	PRINTS EXPNAME:23
	PRINTL +4
	EXP:23 += 4
ENDIF

;依赖度ベクトル
TFLAG:33 = 1

;依赖度補正値(恋慕で1、相亲相爱で2）
IF TALENT:3 && ASSIPLAY == 0
	TFLAG:30 += 1
ELSEIF TALENT:6 && ASSIPLAY == 0
	TFLAG:30 += 2
ENDIF

RETURN 1

