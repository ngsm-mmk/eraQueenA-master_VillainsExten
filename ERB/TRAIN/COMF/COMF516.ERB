;-------------------------------------------------
;打屁股肛
;做爱系派生指令
;-------------------------------------------------
@COM516
IF TFLAG:54 == 1 
	PRINTL 打屁股逆肛
	STR:0 = 打屁股逆肛
ELSE
	PRINTL 打屁股肛交
	STR:0 = 打屁股肛交
ENDIF
TFLAG:56 = SELECTCOM
SELECTCOM = 516


;丝袜びりびりフラグ
IF TEQUIP:1 == 5 || TEQUIP:1 == 6
	TFLAG:55 = 1
ENDIF

;调教对象が后庭处子だった場合の喪失フラグ
