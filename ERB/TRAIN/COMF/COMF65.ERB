;-------------------------------------------------
;乳头奸
;做爱系指令、レベル1
;-------------------------------------------------
@COM65
PRINTL 乳头奸
;-------------------------------------------------
;実行判定処理
;-------------------------------------------------
;绳子で縛られている場合は無視（绳子が無い時のみ実行判定を行う）
IF TEQUIP:19 == 0 && !TEQUIP:PLAYER:60
	A = 0
	S = 0
	;すべての命令に共通の要素を考慮
	;(顺从が高いと命令に従いやすいなど)
	CALL COM_ORDER

	;ABL:欲望
	IF ABL:1
		SIF S
			PRINT  + 
		A += ABL:1 * 3
		PRINTS ABLNAME:1
		PRINTV 'LV,ABL:1,'(,ABL:1 * 3,')
		S = 1
	ENDIF
	;ABL:Ｂ感觉
	IF ABL:6
		SIF S
			PRINT  + 
		A += ABL:6 * 2
		PRINTS ABLNAME:6
		PRINTV 'LV,ABL:6,'(,ABL:6 * 2,')
		S = 1
	ENDIF
	;ABL:性成瘾
	IF ABL:18
		SIF S
			PRINT  + 
		A += ABL:18 * 1
		PRINTS ABLNAME:18
		PRINTV 'LV,ABL:18,'(,ABL:18 * 1,')
		S = 1
	ENDIF
	;快乐刻印
	IF MARK:1
		SIF S
			PRINT  + 
		A += MARK:1 * 3
		PRINTS MARKNAME:1
		PRINTV 'LV,MARK:1,'(,MARK:1 * 3,')
		S = 1
	ENDIF
	;PALAM:润滑（の不足）
	IF PALAM:4 < PALAMLV:3
		PRINT  - 
		A -= 5
		PRINT 润滑不足
		PRINTV '(,5,')
		S = 1
	ENDIF
	;PALAM:情欲
	IF PALAM:6 < PALAMLV:1
		L = 0
	ELSEIF PALAM:6 < PALAMLV:2
		L = 1
	ELSEIF PALAM:6 < PALAMLV:3
		L = 2
	ELSEIF PALAM:6 < PALAMLV:4
		L = 3
	ELSEIF PALAM:6 < PALAMLV:5
		L = 4
	ELSE
		L = 5
	ENDIF
	IF L
		SIF S
			PRINT  + 
		A += L * 3
		PRINTS PALAMNAME:6
		PRINTV 'LV,L,'(,L * 3,')
		S = 1
	ENDIF
	;害羞
	IF TALENT:35
		PRINT  - 
		A -= 2
		PRINTS TALENTNAME:35
		PRINTV '(,2,')
		S = 1
	ENDIF
	;否定快感
	IF TALENT:71
		PRINT  - 
		A -= 5
		PRINTS TALENTNAME:71
		PRINTV '(,5,')
		S = 1
	ENDIF
	;讨厌男人（恋慕、相亲相爱で无效）
	IF TALENT:82 && TALENT:PLAYER:140 && ((TALENT:3 == 0 && TALENT:6 == 0) || PLAYER != MASTER)
		PRINT  - 
		A -= 12
		PRINTS TALENTNAME:82
		PRINTV '(,12,')
		S = 1
	ENDIF
	;恋慕
	IF TALENT:3 && ASSIPLAY == 0
		SIF S
			PRINT  + 
		A += 5
		PRINTS TALENTNAME:3
		PRINTV '(,5,')
		S = 5
	ENDIF
	;相亲相爱
	IF TALENT:6 && ASSIPLAY == 0
		SIF S
			PRINT  + 
		A += 10
		PRINTS TALENTNAME:6
		PRINTV '(,10,')
		S = 10
	ENDIF

	;调教者が扶她
	IF TALENT:PLAYER:139
		SIF S
			PRINT  + 
		A += 4
		PRINTS TALENTNAME:139
		PRINTV '(,4,')
		S = 1
	ENDIF

	;媚薬
	IF TEQUIP:26
		SIF S
			PRINT  + 
		A += 6
		PRINTS ITEMNAME:41
		PRINTV '(,6,')
		S = 1
	ENDIF

	Y = 0
	;愛液の汚れ
	SIF STAIN:PLAYER:2 & 1
		Y += 1
	;精液の汚れ
	SIF STAIN:PLAYER:2 & 4
		Y += 3
	;肛の汚れ
	SIF STAIN:PLAYER:2 & 8
		Y += 7
	SIF STAIN:PLAYER:2 & 16
		Y += 15

	SIF TALENT:61
		Y /= 3
	SIF TALENT:62
		Y *= 2

	;汚れあり
	IF Y
		PRINT  - 
		A -= Y
		;污臭钝感
		IF TALENT:61
			PRINT 有污物、
			PRINTS TALENTNAME:61
		ELSEIF TALENT:62
			PRINT 有污物、
			PRINTS TALENTNAME:62
		ELSE
			PRINT 有污物
		ENDIF
		PRINTV '(,Y,')
		S = 1
	ENDIF

	;合計を表示(15以上で実行)
	PRINT  = 
	PRINTV A

	V = 15
	SIF A < V
		PRINT  < 
	SIF A == V
		PRINT  = 
	SIF A > V
		PRINT  > 
	PRINT 实行值
	PRINTV V
	;実行不可口上/実行判定操作
	IF A < V
		CALL KOJO_DENIAL
		WAIT
		RETURN 0
	ELSE
		PRINTL
	ENDIF
ENDIF

;-------------------------------------------------
;実行決定
;-------------------------------------------------
STR:0 = 乳头奸


;-------------------------------------------------
;射精槽チェック
;-------------------------------------------------
B = 1500

;ABL:调教者の技巧をみる
IF ABL:PLAYER:2 == 0
	TIMES B , 1.00
ELSEIF ABL:PLAYER:2 == 1
	TIMES B , 1.20
ELSEIF ABL:PLAYER:2 == 2
	TIMES B , 1.40
ELSEIF ABL:PLAYER:2 == 3
	TIMES B , 1.60
ELSEIF ABL:PLAYER:2 == 4
	TIMES B , 1.80
ELSE
	TIMES B , 2.00
ENDIF

;ABL:顺从をみる
IF ABL:0 == 0
	TIMES B , 0.80
ELSEIF ABL:0 == 1
	TIMES B , 0.90
ELSEIF ABL:0 == 2
	TIMES B , 1.00
ELSEIF ABL:0 == 3
	TIMES B , 1.10
ELSEIF ABL:0 == 4
	TIMES B , 1.20
ELSE
	TIMES B , 1.30
ENDIF

;ABL:欲望をみる
IF ABL:1 == 0
	TIMES B , 1.00
ELSEIF ABL:1 == 1
	TIMES B , 1.10
ELSEIF ABL:1 == 2
	TIMES B , 1.20
ELSEIF ABL:1 == 3
	TIMES B , 1.30
ELSEIF ABL:1 == 4
	TIMES B , 1.40
ELSE
	TIMES B , 1.50
ENDIF

;PALAM:润滑をみる
IF PALAM:4 < PALAMLV:1
	TIMES B , 0.60
ELSEIF PALAM:4 < PALAMLV:2
	TIMES B , 0.70
ELSEIF PALAM:4 < PALAMLV:3
	TIMES B , 0.80
ELSEIF PALAM:4 < PALAMLV:4
	TIMES B , 0.90
ELSEIF PALAM:4 >= PALAMLV:4
	TIMES B , 1.00
ENDIF

;调教对象の胸围尺寸を見る
;爆乳
IF TALENT:112
	TIMES B , 1.50
;巨乳
ELSEIF TALENT:111
	TIMES B , 1.20
;贫乳
ELSEIF TALENT:109
	TIMES B , 0.90
;绝壁
ELSEIF TALENT:110
	TIMES B , 0.70
ENDIF

;プ雷ヤーのABL:Ｃ感觉をみる
IF ABL:PLAYER:3 == 0
	TIMES B , 1.00
ELSEIF ABL:PLAYER:3 == 1
	TIMES B , 1.50
ELSEIF ABL:PLAYER:3 == 2
	TIMES B , 2.00
ELSEIF ABL:PLAYER:3 == 3
	TIMES B , 2.50
ELSEIF ABL:PLAYER:3 == 4
	TIMES B , 3.50
ELSE
	TIMES B , 5.00
ENDIF

;再装填処理中は射精槽は20分の1に
SIF CFLAG:PLAYER:9 > 0
	B /= 20

	BASE:PLAYER:2 += B

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、气力
LOSEBASE:0 += 60
LOSEBASE:1 += 100

SOURCE:3 = 200
SOURCE:4 = 100
SOURCE:7 = 400
SOURCE:9 = 150
SOURCE:13 = 40
SOURCE:17 = 150
SOURCE:18 = 300

;上のほうで計算した汚れデータ
SOURCE:15 = Y*10 + 60

;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;性压抑、抵抗持ちには、抑郁ソース追加
SIF TALENT:32 || TALENT:34
	SOURCE:16 += 300

;中毒による中毒充足のソースは先に処理しておく
;性成瘾を見る
IF ABL:18 == 1
	TIMES SOURCE:9 , 0.00
ELSEIF ABL:18 == 2
	TIMES SOURCE:9 , 0.30
ELSEIF ABL:18 == 3
	TIMES SOURCE:9 , 0.60
ELSEIF ABL:18 == 4
	TIMES SOURCE:9 , 0.90
ELSEIF ABL:18 == 5
	TIMES SOURCE:9 , 1.20
ELSEIF ABL:18 == 6
	TIMES SOURCE:9 , 1.50
ELSEIF ABL:18 == 7
	TIMES SOURCE:9 , 1.80
ELSEIF ABL:18 == 8
	TIMES SOURCE:9 , 2.30
ELSEIF ABL:18 == 9
	TIMES SOURCE:9 , 3.00
ELSEIF ABL:18 >= 10
	TIMES SOURCE:9 , 5.00
ENDIF

;-------------------------------------------------
;射精チェック
;-------------------------------------------------
CALL SAMEN_CHECK

;射精時の処理
IF E >= 1
	;射精先タイプの设定
	TFLAG:703 = 4
	;射精先選択の可・不可
	TFLAG:704 = 1
	;汎用的な処理はこの関数で行う
	;（射精槽の再計算や射精经验の上昇と共通部分の表示）
	CALL SAMEN_SHOOT
	;指令ごとに固有の処理はこの下に書く
ENDIF

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;奴隶の口⇔调教者のＰの汚れが移动
;射精に関係なく行われる処理はここから
STAIN:0 |= STAIN:PLAYER:2
STAIN:PLAYER:2 |= STAIN:0
;奴隶のＢ⇔调教者のＰの汚れが移动
STAIN:5 |= STAIN:PLAYER:2
STAIN:PLAYER:2 |= STAIN:5

;-------------------------------------------------
;经验の処理
;-------------------------------------------------
;性交经验
EXP:4 += 1
EXP:PLAYER:4 += 1
PRINTS EXPNAME:4
PRINTL +1

;蔷薇经验とか加算
CALL COM_EXP(4)

;爱情经验
IF TALENT:3 == 1 && ASSIPLAY == 0
	PRINTS EXPNAME:23
	PRINTL +1
	EXP:23 += 2
ELSEIF TALENT:6 == 1 && ASSIPLAY == 0
	PRINTS EXPNAME:23
	PRINTL +2
	EXP:23 += 4
ENDIF

;依赖度ベクトル
TFLAG:33 = 1

;依赖度補正値(主人かつ恋慕、淫乱、服从で1。相亲相爱、娼妇、隶属で2）
IF (TALENT:3 || TALENT:4 || TALENT:5) && ASSIPLAY == 0
	TFLAG:30 += 1
ELSEIF (TALENT:6 || TALENT:7 || TALENT:8) && ASSIPLAY == 0
	TFLAG:30 += 2
ENDIF


RETURN 1


