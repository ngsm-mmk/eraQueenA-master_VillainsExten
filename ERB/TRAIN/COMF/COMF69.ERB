;-------------------------------------------------
;背面座位
;做爱系指令、レベル3
;-------------------------------------------------
@COM69
;-------------------------------------------------
;特殊派生処理
;-------------------------------------------------
;调教者が前回:助手で今回:主人か、前回:主人で今回:助手のとき
IF (ASSIPLAY && TFLAG:50 == 0) || (ASSIPLAY == 0 && TFLAG:50)
	;前回の调教が对面座位のとき二輪刺しへ
	IF PREVCOM == 68
		CALL COM_ABLE512
		SIF RESULT == 1
			JUMP COM512
	ENDIF
;肛插入命令中だと二輪刺し肛へ
IF TEQUIP:48
	CALL COM_ABLE513
	SIF RESULT == 1
		JUMP COM513
ENDIF
;前のプ雷が后背位かつ调教者の技巧3以上の場合、插入子宫口玩弄へ派生。
ELSEIF PREVCOM == 61 && ABL:PLAYER:2 >= 3
	JUMP COM507
ENDIF

;羞耻PLAY中、乱れ牡丹にランクアップ
SIF TEQUIP:30
 	JUMP COM504

PRINTL 背面座位
;-------------------------------------------------
;実行判定処理
;-------------------------------------------------
;绳子で縛られている場合は無視（绳子が無い時のみ実行判定を行う）
IF TEQUIP:19 == 0 && !TEQUIP:PLAYER:60
	A = 0
	S = 0
	;すべての命令に共通の要素を考慮
	;(顺从が高いと命令に従いやすいなど)
	CALL COM_ORDER

	;ABL:欲望
	IF ABL:1
		SIF S
			PRINT  + 
		A += ABL:1 * 3
		PRINTS ABLNAME:1
		PRINTV 'LV,ABL:1,'(,ABL:1 * 3,')
		S = 1
	ENDIF
	;ABL:Ｖ感觉
	IF ABL:4
		SIF S
			PRINT  + 
		A += ABL:4 * 2
		PRINTS ABLNAME:4
		PRINTV 'LV,ABL:4,'(,ABL:4 * 2,')
		S = 1
	ENDIF
	;ABL:性成瘾
	IF ABL:18
		SIF S
			PRINT  + 
		A += ABL:18 * 1
		PRINTS ABLNAME:18
		PRINTV 'LV,ABL:18,'(,ABL:18 * 1,')
		S = 1
	ENDIF
	;快乐刻印
	IF MARK:1
		SIF S
			PRINT  + 
		A += MARK:1 * 3
		PRINTS MARKNAME:1
		PRINTV 'LV,MARK:1,'(,MARK:1 * 3,')
		S = 1
	ENDIF
	;PALAM:润滑（の不足）
	IF PALAM:4 < PALAMLV:3
		PRINT  - 
		A -= 5
		PRINT 润滑不足
		PRINTV '(,5,')
		S = 1
	ENDIF
	;PALAM:情欲
	IF PALAM:6 < PALAMLV:1
		L = 0
	ELSEIF PALAM:6 < PALAMLV:2
		L = 1
	ELSEIF PALAM:6 < PALAMLV:3
		L = 2
	ELSEIF PALAM:6 < PALAMLV:4
		L = 3
	ELSEIF PALAM:6 < PALAMLV:5
		L = 4
	ELSE
		L = 5
	ENDIF
	IF L
		SIF S
			PRINT  + 
		A += L * 3
		PRINTS PALAMNAME:6
		PRINTV 'LV,L,'(,L * 3,')
		S = 1
	ENDIF
	;害羞
	IF TALENT:35
		PRINT  - 
		A -= 2
		PRINTS TALENTNAME:35
		PRINTV '(,2,')
		S = 1
	ENDIF
	;否定快感
	IF TALENT:71
		PRINT  - 
		A -= 5
		PRINTS TALENTNAME:71
		PRINTV '(,5,')
		S = 1
	ENDIF
	;讨厌男人（恋慕、相亲相爱で无效）
	IF TALENT:82 && TALENT:PLAYER:140 && ((TALENT:3 == 0 && TALENT:6 == 0) || PLAYER != MASTER)
		PRINT  - 
		A -= 12
		PRINTS TALENTNAME:82
		PRINTV '(,12,')
		S = 1
	ENDIF
	;恋慕
	IF TALENT:3 && ASSIPLAY == 0
		SIF S
			PRINT  + 
		A += 15
		PRINTS TALENTNAME:3
		PRINTV '(,15,')
		S = 15
	ENDIF
	;相亲相爱
	IF TALENT:6 && ASSIPLAY == 0
		SIF S
			PRINT  + 
		A += 20
		PRINTS TALENTNAME:6
		PRINTV '(,20,')
		S = 20
	ENDIF
	;处女
	IF TALENT:0
		PRINT  - 
		A -= 40
		PRINTS TALENTNAME:0
		PRINTV '(,40,')
		S = 1
	;V经验
	ELSEIF EXP:0 < EXPLV:2
		PRINT  - 
		A -= 15
		PRINTS EXPNAME:0
		PRINT 不足
		PRINTV '(,15,')
		S = 1
	ENDIF

	;调教者が扶她
	IF TALENT:PLAYER:139
		SIF S
			PRINT  + 
		A += 4
		PRINTS TALENTNAME:139
		PRINTV '(,4,')
		S = 1
	ENDIF
	;调教者が孩童尺寸
	IF TALENT:PLAYER:114
		SIF S
			PRINT  + 
		A += 2
		PRINTFORM %NAME:PLAYER%是孩童尺寸
		PRINTV '(,2,')
		S = 1
	ENDIF
	
	;调教者が短小
	IF TALENT:PLAYER:115
		SIF S
			PRINT  + 
		A += 1
		PRINTFORM %NAME:PLAYER%是短小
		PRINTV '(,1,')
		S = 1
	ENDIF
	
	;调教者が巨根
	IF TALENT:PLAYER:116
		SIF S
			PRINT  - 
		A -= 2
		PRINTFORM %NAME:PLAYER%是巨根
		PRINTV '(,2,')
		S = 1
	ENDIF
	
	;调教者が尺寸傲人
	IF TALENT:PLAYER:117
		SIF S
			PRINT  - 
		A -= 4
		PRINTFORM %NAME:PLAYER%是尺寸傲人
		PRINTV '(,4,')
		S = 1
	ENDIF

	;媚薬
	IF TEQUIP:26
		SIF S
			PRINT  + 
		A += 6
		PRINTS ITEMNAME:41
		PRINTV '(,6,')
		S = 1
	ENDIF

	Y = 0
	;愛液の汚れ
	SIF STAIN:PLAYER:2 & 1
		Y += 1
	;精液の汚れ
	SIF STAIN:PLAYER:2 & 4
		Y += 3
	;肛の汚れ
	SIF STAIN:PLAYER:2 & 8
		Y += 7
	SIF STAIN:PLAYER:2 & 16
		Y += 15

	SIF TALENT:61
		Y /= 3
	SIF TALENT:62
		Y *= 2

	;汚れあり
	IF Y
		PRINT  - 
		A -= Y
		;污臭钝感
		IF TALENT:61
			PRINT 有污物、
			PRINTS TALENTNAME:61
		ELSEIF TALENT:62
			PRINT 有污物、
			PRINTS TALENTNAME:62
		ELSE
			PRINT 有污物
		ENDIF
		PRINTV '(,Y,')
		S = 1
	ENDIF

	;合計を表示(42以上で実行)
	PRINT  = 
	PRINTV A

	V = 42
	SIF A < V
		PRINT  < 
	SIF A == V
		PRINT  = 
	SIF A > V
		PRINT  > 
	PRINT 实行值
	PRINTV V
	;実行不可口上/実行判定操作
	IF A < V
		CALL KOJO_DENIAL
		WAIT
		RETURN 0
	ELSE
		PRINTL
	ENDIF
ENDIF

;-------------------------------------------------
;実行決定
;-------------------------------------------------
STR:0 = 背面座位


;丝袜びりびりフラグ
IF TEQUIP:1 == 5 || TEQUIP:1 == 6
	TFLAG:55 = 1
ENDIF


;V经验を伴う指令のフラグ(处女膜再生对応)
TFLAG:19 = 1
SIF (TALENT:3 || TALENT:6) && ASSIPLAY == 0 && EXP:0 == 0
	TFLAG:20 = 1

;处女喪失時のテ接吻ト用処理
SIF TFLAG:60 && (PREVCOM == 60 || PREVCOM == 61 || PREVCOM == 66 || PREVCOM == 68 || PREVCOM == 69) && ((ASSIPLAY && TFLAG:50) || (ASSIPLAY == 0 && TFLAG:50 == 0)) && TFLAG:31
	TFLAG:31 = 2
;调教者が童贞だった場合の喪失フラグ
IF TALENT:PLAYER:160
	TFLAG:160 = 2
ENDIF
;-------------------------------------------------
;射精槽チェック
;-------------------------------------------------
B = 1800

;ABL:调教者の技巧をみる
IF ABL:PLAYER:2 == 0
