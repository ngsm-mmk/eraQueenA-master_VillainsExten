;-------------------------------------------------
;玩弄乳头
;爱抚系指令、レベル1
;-------------------------------------------------
@COM7
;-------------------------------------------------
;特殊派生処理
;-------------------------------------------------
;前回と今回の调教者が同じ
IF (ASSIPLAY && TFLAG:50) || (ASSIPLAY == 0 && TFLAG:50 == 0)
	;前回の调教が插入系のとき、插入中胸爱抚へ
	SIF (PREVCOM >= 60 && PREVCOM <= 77 && PREVCOM != 65) || PREVCOM == 503 || PREVCOM == 504 || PREVCOM == 506 || PREVCOM == 507 || PREVCOM == 515 || PREVCOM == 516 || PREVCOM == 517 || PREVCOM == 518 || PREVCOM == 591
		JUMP COM594
ENDIF

PRINTL 玩弄乳头
STR:0 = 玩弄乳头


;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、气力
LOSEBASE:0 += 5
LOSEBASE:1 += 30

SOURCE:3 = 100
SOURCE:4 = 30
SOURCE:13 = 10
SOURCE:15 = 10
SOURCE:18 = 10
;奴隶男の場合
SIF TALENT:140
	SOURCE:17 = 3
;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;性压抑、抵抗持ちには、抑郁ソース追加
SIF TALENT:32 || TALENT:34
	SOURCE:16 += 10

;喜欢身体接触だと情爱のソース追加
IF TALENT:65
	SOURCE:4 += 50
ENDIF

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;奴隶のＢ⇔调教者の指の汚れが移动
STAIN:5 |= STAIN:PLAYER:1
STAIN:PLAYER:1 |= STAIN:5

;-------------------------------------------------
;经验の処理
;-------------------------------------------------
;蔷薇经验とか加算
CALL COM_EXP(3)


;依赖度ベクトル
TFLAG:33 = 1


RETURN 1

