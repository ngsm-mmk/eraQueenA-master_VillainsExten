;-------------------------------------------------
;尾交
;人外限定
;-------------------------------------------------
@COM94
PRINTL 尾交
;-------------------------------------------------
;実行判定処理
;-------------------------------------------------
A = 0
S = 0

;すべての命令に共通の要素を考慮
;(顺从が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:欲望
IF ABL:1
	SIF S
		PRINT  +
	A += ABL:1 * 2
	PRINTS ABLNAME:1
	PRINTV 'LV,ABL:1,'(,ABL:1 * 2,')
	S = 1
ENDIF
;ABL:侍奉精神
IF ABL:7
	SIF S
		PRINT  +
	A += ABL:7 * 4
	PRINTS ABLNAME:7
	PRINTV 'LV,ABL:7,'(,ABL:7 * 4,')
	S = 1
ENDIF
;ABL:精液成瘾
IF ABL:16
	SIF S
		PRINT  +
	A += ABL:16 * 1
	PRINTS ABLNAME:16
	PRINTV 'LV,ABL:16,'(,ABL:16 * 1,')
	S = 1
ENDIF

IF MARK:1
	SIF S
		PRINT  +
	A += MARK:1 * 2
	PRINTS MARKNAME:1
	PRINTV 'LV,MARK:1,'(,MARK:1 * 2,')
	S = 1
ENDIF


;PALAM:情欲
IF PALAM:6 < PALAMLV:1
	L = 0
ELSEIF PALAM:6 < PALAMLV:2
	L = 1
ELSEIF PALAM:6 < PALAMLV:3
	L = 2
ELSEIF PALAM:6 < PALAMLV:4
	L = 3
ELSEIF PALAM:6 < PALAMLV:5
	L = 4
ELSE
	L = 5
ENDIF
IF L
	SIF S
		PRINT  +
	A += L * 3
	PRINTS PALAMNAME:6
	PRINTV 'LV,L,'(,L * 3,')
	S = 1
ENDIF

;害羞
IF TALENT:35
	PRINT  -
	A -= 1
	PRINTS TALENTNAME:35
	PRINTV '(,1,')
	S = 1
ENDIF
;污臭钝感
IF TALENT:61
	SIF S
		PRINT  +
	A += 1
	PRINTS TALENTNAME:61
	PRINTV '(,1,')
	S = 1
ENDIF
;污臭敏感
IF TALENT:62
	PRINT  -
	A -= 3
	PRINTS TALENTNAME:62
	PRINTV '(,3,')
	S = 1
ENDIF
;自我奉献
IF TALENT:63
	SIF S
		PRINT  +
	A += 6
	PRINTS TALENTNAME:63
	PRINTV '(,6,')
	S = 1
ENDIF
;否定快感（淫乱、娼妇で无效）
IF TALENT:71 && (TALENT:4 == 0 && TALENT:7 == 0)
	PRINT  -
	A -= 3
	PRINTS TALENTNAME:71
	PRINTV '(,3,')
	S = 1
ENDIF
;调教者が男で、同性倾向0（陷落で无效）
IF MALE(PLAYER) && ABL:同性倾向 < 1 && (!KANRAKU(TARGET) || (ASSIPLAY && KANRAKU(TARGET) != 2))
	PRINT  -
	A -= 6
	PRINTFORM 无同性倾向
	PRINTV '(,6,')
	S = 1
ENDIF

;讨厌男人（陷落で无效）
IF MALE(PLAYER) && TALENT:讨厌男人 && (!KANRAKU(TARGET) || (ASSIPLAY && KANRAKU(TARGET) != 2))
	PRINT  -
	A -= 7
	PRINTS TALENTNAME:82
	PRINTV '(,7,')
	S = 1
ENDIF
;恋慕
IF TALENT:3 && ASSIPLAY == 0
	SIF S
		PRINT  +
	A += 3
	PRINTS TALENTNAME:3
	PRINTV '(,3,')
	S = 3
ENDIF
;相亲相爱
IF TALENT:6 && ASSIPLAY == 0
	SIF S
		PRINT  +
	A += 6
	PRINTS TALENTNAME:6
	PRINTV '(,6,')
	S = 6
ENDIF

;媚薬
IF TEQUIP:26
	SIF S
		PRINT  +
	A += 6
	PRINTS ITEMNAME:41
	PRINTV '(,6,')
	S = 1
ENDIF

Y = 0
;愛液の汚れ
SIF STAIN:PLAYER:2 & 1
	Y += 1
;精液の汚れ
SIF STAIN:PLAYER:2 & 4
	Y += 3
;肛の汚れ
SIF STAIN:PLAYER:2 & 8
	Y += 7
SIF STAIN:PLAYER:2 & 16
	Y += 15

SIF TALENT:61
	Y /= 3
SIF TALENT:62
	Y *= 2

;腋扱きでは汚れの影響が少なめ
Y /= 3

;汚れあり
IF Y
	PRINT  -
	A -= Y
	;污臭钝感
	IF TALENT:61
		PRINT 有污物、
		PRINTS TALENTNAME:61
	ELSEIF TALENT:62
		PRINT 有污物、
		PRINTS TALENTNAME:62
	ELSE
		PRINT 有污物
	ENDIF
	PRINTV '(,Y,')
	S = 1
ENDIF

;合計を表示(22以上で実行)
PRINT  =
PRINTV A

V = 22
SIF A < V
	PRINT  <
SIF A == V
	PRINT  =
SIF A > V
	PRINT  >
PRINT 实行值
PRINTV V
;実行不可口上/実行判定操作
IF A < V
	CALL KOJO_DENIAL
	WAIT
	RETURN 0
ELSE
	PRINTL
ENDIF

;-------------------------------------------------
;実行決定
;-------------------------------------------------
STR:0 = 尾交

;実行者が奴隶であるフラグ
TFLAG:28 = 1

;-------------------------------------------------
;射精槽チェック
;-------------------------------------------------
B = 300

;ABL:技巧をみる
IF ABL:2 == 0
	TIMES B , 1.00
ELSEIF ABL:2 == 1
	TIMES B , 1.20
ELSEIF ABL:2 == 2
	TIMES B , 1.40
ELSEIF ABL:2 == 3
	TIMES B , 1.60
ELSEIF ABL:2 == 4
	TIMES B , 1.80
ELSE
	TIMES B , 2.00
ENDIF

;ABL:顺从をみる
IF ABL:0 == 0
	TIMES B , 0.80
ELSEIF ABL:0 == 1
	TIMES B , 0.90
ELSEIF ABL:0 == 2
	TIMES B , 1.00
ELSEIF ABL:0 == 3
	TIMES B , 1.10
ELSEIF ABL:0 == 4
	TIMES B , 1.20
ELSE
	TIMES B , 1.30
ENDIF

;ABL:侍奉精神をみる
IF ABL:7 == 0
	TIMES B , 0.50
ELSEIF ABL:7 == 1
	TIMES B , 0.80
ELSEIF ABL:7 == 2
	TIMES B , 1.20
ELSEIF ABL:7 == 3
	TIMES B , 1.50
ELSEIF ABL:7 == 4
	TIMES B , 1.80
ELSE
	TIMES B , 2.40
ENDIF

;尾巴敏感度
IF TALENT:232
	TIMES B , 1.20
ELSEIF TALENT:233
	TIMES B , 0.80
ELSE
	TIMES B , 1.00
ENDIF

;再装填処理中は射精槽は20分の1に
SIF CFLAG:PLAYER:9 > 0
	B /= 20


SIF TALENT:PLAYER:137 || TALENT:PLAYER:139 || TALENT:PLAYER:140
	BASE:PLAYER:2 += B

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、气力
LOSEBASE:0 += 30
LOSEBASE:1 += 150

SOURCE:4 = 60
SOURCE:5 = 210
SOURCE:6 = 50
SOURCE:13 = 400
SOURCE:14 = 400
SOURCE:17 = 500
SOURCE:18 = 200

IF TALENT:232
    SOURCE:23 = 120
ELSEIF TALENT:233
    SOURCE:23 = 80
ELSE
    SOURCE:23 = 100
ENDIF

;上のほうで計算した汚れデータ
SOURCE:15 = Y*10 + 60

;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;性压抑、抵抗持ちには、抑郁ソース追加
SIF TALENT:32 || TALENT:34
	SOURCE:16 += 300

;-------------------------------------------------
;射精チェック
;-------------------------------------------------
CALL SAMEN_CHECK

;射精時の処理
IF E >= 1
	;射精先タイプの设定
	TFLAG:703 = 5
	;射精先選択の可・不可
	TFLAG:704 = 1
	;汎用的な処理はこの関数で行う
	;（射精槽の再計算や射精经验の上昇と共通部分の表示）
	CALL SAMEN_SHOOT
ENDIF

;-------------------------------------------------
;经验の処理
;-------------------------------------------------
;蔷薇经验とか加算
CALL COM_EXP(7)


;依赖度ベクトル
TFLAG:33 = 1

;依赖度補正値(主人かつ恋慕、淫乱、服从で1。相亲相爱、娼妇、隶属で2）
IF (TALENT:3 || TALENT:4 || TALENT:5) && ASSIPLAY == 0
	TFLAG:30 += 1
ELSEIF (TALENT:6 || TALENT:7 || TALENT:8) && ASSIPLAY == 0
	TFLAG:30 += 2
ENDIF

TFLAG:100 = 1

RETURN 1
