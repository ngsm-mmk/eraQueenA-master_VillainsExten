@COM_ABLE52
SIF COM_FILTER:52
	RETURN 0
IF FLAG:1501 == 1
    resetcolor
ELSE
    resetcolor
    setcolor 186,161,252
ENDIF
;逆肛正常位実行判定
;女主人→男奴隶调教指令の表示調整中じゃないなら不可
SIF TFLAG:54 != 1
	RETURN 0
;做爱系指令表示制限状態だとダメ
SIF FLAG:84 & 8
	RETURN 0
;调教者が受だとだめ
SIF TALENT:PLAYER:157
	RETURN 0
;调教对象が攻だと駄目
SIF TALENT:158
	RETURN 0
;体型不合时不可
SIF TALENT:巨大体型 && !TALENT:PLAYER:巨大体型
	RETURN 0
SIF TALENT:小人体型 && !TALENT:PLAYER:小人体型
	RETURN 0
;无形体不可
SIF TALENT:无形体
	RETURN 0
SIF TALENT:马身
	RETURN 0
;Ａ扩张经验９以下で肛塞插入中はダメ
SIF EXP:53 < 10 && TEQUIP:14
	RETURN 0
;Ａ扩张经验９以下で后庭拉珠使用中は不可
SIF EXP:53 < 10 && TEQUIP:15
	RETURN 0
;Ａ扩张经验９以下で、肛门爱抚命令か肛插入命令中
SIF EXP:53 < 10 && (TEQUIP:46 || TEQUIP:48)
	RETURN 0
;普通の灌肠使用中は不可
SIF TEQUIP:21
	RETURN 0
;バルーン使用中は不可
SIF TEQUIP:22
	RETURN 0
;電極使用中は不可
SIF TEQUIP:23
	RETURN 0
;一定のＡ经验が必要
SIF EXP:1 < EXPLV:3
	RETURN 0
;小人体型持ちは、双方が调教者と调教对象ならOK
;それ以外の場合は主人が禁断的知识持ちじゃないとダメ
SIF TALENT:142 && (TALENT:MASTER:144 == 0 && TALENT:PLAYER:142 == 0)
	RETURN 0
;调教者が男性、双性、扶她、もしくは捆绑式假阴茎がないとダメ
SIF TALENT:PLAYER:137 == 0 && TALENT:PLAYER:139 == 0 && TALENT:PLAYER:140 == 0 && ITEM:PBAND == 0
	RETURN 0
;处女で、顺从4以下・百合属性or同性倾向4以下のASSIの場合正常位肛交はできない（施虐狂ならＯＫ）
IF TALENT:0 && ASSIPLAY
	SIF (ABL:ASSI:0 <= 4 || ABL:ASSI:11 <= 4 || ABL:ASSI:20 <= 4) && TALENT:ASSI:83 == 0
		RETURN 0
ENDIF
;润滑不足で、顺从3以下・百合属性or同性倾向3以下のASSIの場合正常位肛交はできない（施虐狂ならＯＫ）
IF PALAM:4 < PALAMLV:2 && ASSIPLAY
	SIF (ABL:ASSI:0 <= 3 || ABL:ASSI:11 <= 3 || ABL:ASSI:20 <= 3) && TALENT:ASSI:83 == 0
		RETURN 0
ENDIF
;着衣プ雷：手指灵巧をもっておらず、贞操带をつけている、布偶装を着ている、もしく水手裙、运动短裤ー以外の下身外衣を履いているとダメ
SIF TALENT:PLAYER:58 == 0 && (TEQUIP:2 == 5 || (TEQUIP:4 > 1 &&  TEQUIP:4 < 6) || TEQUIP:6 == 7)
		RETURN 0
;触手调教中はダメ
SIF TEQUIP:25
	RETURN 0
;浴室PLAY中は塑料地垫が無いとダメ
IF TEQUIP:31
SIF ITEM:20 == 0 && NOITEM == 0
	RETURN 0
ENDIF
RETURN 1

