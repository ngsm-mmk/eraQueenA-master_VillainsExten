;-------------------------------------------------
;主人の诞生日サーチ機能
;CALL MASTERBIRTHDAYでこの処理が呼び出せる
;当日が诞生日であればB:1に1が、一週間以内に诞生日が来るならB:1に2が代入されて返ってくる
;-------------------------------------------------
@MASTERBIRTHDAY
;入れ子を空にする
B:1 = 0

;伊兄弟
IF NO:MASTER == 1 || NO:MASTER == 2 || NO:MASTER == 29 || NO:MASTER == 30 || NO:MASTER == 85 || NO:MASTER == 86
	IF FLAG:80 == 3
		IF FLAG:81 == 17
			B:1 = 1
		ELSEIF FLAG:81 >= 10 && FLAG:81 <= 16
			B:1 = 2
		ENDIF
	ENDIF
ENDIF

;-------------------------------------------------
;胸のサイズを比較する機能
;AとBに比較したい相手の登录番号を入れてCALL
;B:1に大きかった方の登录番号、B:2にその差を返します。引き分けの時はB:1が-1になります
;登录番号は@NOなりTARGETとASSIで判定するなりして使ってください
;-------------------------------------------------
@COMPARE_BSIZE
A:1 = 0
A:2 = 0
B:1 = 0
B:2 = 0
IF TALENT:A:110
	A:1 = 0
ELSEIF TALENT:A:109
	A:1 = 1
ELSEIF TALENT:A:111
	A:1 = 3
ELSEIF TALENT:A:112
	A:1 = 4
ELSE
	A:1 = 2
ENDIF

IF TALENT:B:110
	A:2 = 0
ELSEIF TALENT:B:109
	A:2 = 1
ELSEIF TALENT:B:111
	A:2 = 3
ELSEIF TALENT:B:112
	A:2 = 4
ELSE
	A:2 = 2
ENDIF

IF A:1 > A:2
	B:1 = A
	B:2 = A:1
	B:2 -= A:2
ELSEIF A:1 < A:2
	B:1 = B
	B:2 = A:2
	B:2 -= A:2
ELSE
	B:1 = -1
	B:2 = 0
ENDIF

;-------------------------------------------------
