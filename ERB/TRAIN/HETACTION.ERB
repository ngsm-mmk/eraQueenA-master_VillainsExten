;=============================================================================
;面識機能に関する関数群
;=============================================================================
;-------------------------------------------------
;記録機能用の@EVENT関数
;-------------------------------------------------
@EVENTEND
	CALL SAVE_ACQUAINTANCE_ASSI
	PREVCOM:829 = NO:TARGET
	IF ASSI > 0
		PREVCOM:830 = NO:ASSI
	ELSE
		PREVCOM:830 = -1
	ENDIF
@EVENTTURNEND
	CALL SAVE_MEMBER

;-------------------------------------------------
;ここ一週間の助手と调教对象を記録する機能
;PREVCOM:800以降を盛大に使用します
;801 ～ 814 = ここ一週間の奴隶のキャラ番号
;815 ～ 828 = ここ一週間の助手
;829 保存用
;830 保存用
;-------------------------------------------------
@SAVE_MEMBER
;奴隶のキャラ番号を登录
I = 814
REPEAT 14
	IF I != 801
		PREVCOM:I = PREVCOM:(I-1)
	ELSE
		IF TARGET > 0
			IF PREVCOM:829 == NO:TARGET
				PREVCOM:801 = NO:TARGET
			ELSE
				PREVCOM:801 = -1
			ENDIF
		ELSE
			PREVCOM:801 = -1
		ENDIF
	ENDIF
	I -= 1
REND
;助手のキャラ番号を登录
I = 828
REPEAT 14
	IF I != 815
		PREVCOM:I = PREVCOM:(I-1)
	ELSE
		IF ASSI > 0
			IF PREVCOM:830 == NO:ASSI
				PREVCOM:815 = NO:ASSI
			ELSE
				PREVCOM:815 = -1
			ENDIF
		ELSE
			PREVCOM:815 = -1
		ENDIF
	ENDIF
	I -= 1
REND
PREVCOM:829 = -1
PREVCOM:830 = -1

;-------------------------------------------------
;日常での面識を更新する機能
;へたくしょん導入で勝手に保存されるようになります
;更新には口上側の制御が必要なので注意
;-------------------------------------------------
@SAVE_ACQUAINTANCE_DAILY_LIFE
;Bが登場しない場合は無視
IF B > 0 && B < CHARANUM
	G = (799 + NO:A)
	H = (799 + NO:B)
	CFLAG:A:H |= 1
	CFLAG:B:G |= 1
	;Cが居るならCの分も判定
	IF C > 0 && C < CHARANUM && C != B
		I = (799 + NO:C)
		CFLAG:A:I |= 1
		CFLAG:B:I |= 1
		CFLAG:C:G |= 1
		CFLAG:C:H |= 1
	ENDIF
ENDIF

;-------------------------------------------------
;助手调教での面識を更新する機能
;へたくしょん導入で勝手に保存されるようになります
;これも更新には口上側の制御が必要なので注意
;-------------------------------------------------
@SAVE_ACQUAINTANCE_ASSI
;ASSIがいない場合は無視
IF ASSI > 0
	G = (799 + NO:ASSI)
	H = (799 + NO:TARGET)
	CFLAG:ASSI:H |= 2
	CFLAG:TARGET:G |= 2
	IF ASSIPLAY
		CFLAG:ASSI:H |= 8
		CFLAG:TARGET:G |= 4
	ENDIF
ENDIF

;-------------------------------------------------
;ここ一週間の调教对象をサーチする機能
;Rに调教对象のキャラ番号を入れてCALL
;D:1に调教对象と会った日を返す。D:2に実際に会ったのが何回前かを返す
;-------------------------------------------------
@GET_MEMBER_TARGET
;调教对象サーチ
I = 814
D:1 = -1
D:2 = -1
REPEAT 14
	SIF PREVCOM:I == R
		D:1 = (I - 800)
	I -= 1
REND
IF D:1 >= 0
	D:2 = D:1
	;割って余りが出ない時は、就这样÷2で日付が出る
	IF (D:1 % 2) == 0
		D:1 /= 2
	;割って余りが出る時は、今が昼か夜かで結果が変わる
	ELSE
		;昼なら前日なので、1を足す
		IF TIME == 0
			D:1 += 1
		;夜ならその日のうちに会ってるので1を引く
		ELSE
			D:1 -= 1
		ENDIF
		D:1 /= 2
	ENDIF
ENDIF

;-------------------------------------------------
;ここ一週間の助手をサーチする機能
;Rに助手のキャラ番号を入れてCALL
;D:3に助手と会った日を、D:4に実際に会ったのが何回前か返す
;当日の助手？ 助手口上で判定すればいいと思うよ！
;-------------------------------------------------
@GET_MEMBER_ASSI
;助手サーチ
I = 828
D:3 = -1
D:4 = -1
REPEAT 14
	SIF PREVCOM:I == R
		D:3 = (I - 814)
	I -= 1
REND
IF D:3 >= 0
	D:4 = D:3
	;割って余りが出ない時は、就这样÷2で日付が出る
	IF (D:3 % 2) == 0
		D:3 /= 2
	;割って余りが出る時は、今が昼か夜かで結果が変わる
	ELSE
		;昼なら前日なので、1を足す
		IF TIME == 0
			D:3 += 1
		;夜ならその日のうちに会ってるので1を引く
		ELSE
			D:3 -= 1
		ENDIF
		D:3 /= 2
	ENDIF
ENDIF

;-------------------------------------------------
;口上主の童贞、处女、後处女を奪った相手をサーチする機能
;Rに童贞なら1、处女なら4、後处女なら16を入れてCALL
;存在すればLに奪った相手のキャラ番号が格納されます。
;-------------------------------------------------
@GET_LOSTPURE
R *= 32
L = -1
REPEAT CHARANUM
	G = (799 + NO:COUNT)
	SIF (CFLAG:G & R)
		L = NO:COUNT
REND
