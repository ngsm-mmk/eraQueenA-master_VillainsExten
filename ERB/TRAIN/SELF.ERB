							;前につっこんでた
							IF TEQUIP:13
								JUEL:1 += 400*A/2
								EXP:0 += A*2/2
							ENDIF
							;後ろにつっこんでた
							IF TEQUIP:14
								JUEL:2 += 400*A/2
								EXP:1 += A
							ENDIF
								
							JUEL:4 += A*300/2
							JUEL:5 += A*250/2
							JUEL:14 += A*300/2
							;阴茎がある、孩童尺寸で無いと射精经验も上昇
							SIF ((TALENT:137 || TALENT:139) || TALENT:140) && TALENT:114 == 0
								EXP:3  += A / 4
							;母乳体质だと喷乳经验も上昇
							SIF TALENT:149
								EXP:54 += A / 4
						ENDIF
					;屈服が3か反感が1以下　以外
					ELSE
						
						Z = 14
						;CALL KOJO_MESSAGE_EVENT
						IF !MALE(TARGET)
							PRINTFORML %CALLNAME%发出悲鸣，慌张地钻进被窝
							PRINTFORML 在被命令继续做下去后
							PRINTFORML %CALLNAME%虽因羞耻与悔恨面容扭曲，却还是只能活动起手指，发出高亢的声音。
						ELSE
							PRINTFORML %CALLNAME%受到惊吓，慌张地钻进被窝
							PRINTFORML 然而快感已被撩拨上来的股间着实煎熬，令其只能低低地哀鸣。
						ENDIF
						JUEL:6 += A*300/3
						JUEL:8 += A*300/3
						EXP:10 += A/3
						EXP:11 += A/3
						JUEL:0 += 500*A/3
						;前につっこんでた
						IF TEQUIP:13
							JUEL:1 += 400*A/3
							EXP:0 += A
						ENDIF
						;後ろにつっこんでた
						IF TEQUIP:14
							JUEL:2 += 400*A/3
							EXP:1 += A
						ENDIF
							
						JUEL:4 += A*300/3
						JUEL:5 += A*250/3
						JUEL:14 += A*300/3
						;阴茎がある、孩童尺寸で無いと射精经验も上昇
						SIF ((TALENT:137 || TALENT:139) || TALENT:140) && TALENT:114 == 0
							EXP:3  += A / 6
						;母乳体质だと喷乳经验も上昇
						SIF TALENT:149
							EXP:54 += A / 6
					ENDIF
				ENDIF
				PRINTW 　
			ENDIF
		ELSE
		;ばれなかった
			;自慰の報告　顺从+露出+受虐狂-反抗刻印*2
			B = ABL:0 + ABL:8 + ABL:9 - ( MARK:3 * 2 )
			;直率か幼稚だと言いやすい
			SIF TALENT:13 || TALENT:151
				B += 2
				
			PRINTL 　
			;報告値が10以上で反抗刻印2未満か弱点があると報告する
			IF B >= 10 && (MARK:3 < 2 || TALENT:37)
				Z = 0
				;CALL KOJO_MESSAGE_EVENT
				PRINTFORM 按照吩咐，%CALLNAME%上报自己自慰了
				PRINTV A
				PRINTFORML 回。
				JUEL:6 += A*200
			ELSE
				PRINTFORM %PLAYER_NAME()%离开房间后，%CALLNAME%好像自慰了
				PRINTV A
				PRINTFORML 回。
			ENDIF
		ENDIF
		
		PRINTL 　
		EXP:2 += A / 4
		CALL PALAMCHANGE
		PRINTFORMW
		RETURN 0
	ENDIF
ENDIF

