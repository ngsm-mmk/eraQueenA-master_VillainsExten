;-------------------------------------------------
;朝に起こるイベント
;-------------------------------------------------
@EVENT_NEWDAY_SELF
;调教对象が居ない場合は終了
SIF TARGET < 0
	RETURN 0

;朝イベント取消フラグが立ってる場合は終了
SIF CFLAG:42
	RETURN 0

;現在调教している奴隶以外も発生するようにする。

D = CHARANUM - 1
$DOREI_KETTEI
;随机で奴隶の中の１人を選択
C = RAND:D
C += 1
;イベントを起こす奴隶を決定
TARGET = C
;对象が労役中、育儿中ならやり直し
SIF CFLAG:12 > 0 || TALENT:154
	GOTO DOREI_KETTEI


;全体判定、恋慕以外の陷落素質が無い場合もしくは反抗刻印があるときは関数終了
SIF (TALENT:4 == 0 && TALENT:5 == 0 && TALENT:6 == 0 && TALENT:7 == 0 && TALENT:8 == 0) || MARK:3 > 0
	RETURN 0

;バー镇尼判定(一応后庭处子、童贞も勝手に失わないようにしてみた)
V = 0
SIF TALENT:0 || TALENT:1 || TALENT:161 || TALENT:MASTER:160
	V = 1

;朝侍奉系の発動フラグ
A = 0

;このイベントは今调教中の奴隶限定
TARGET = TFLAG:403
;基本的に主人攻の朝の３Ｐ
;助手あり、奴隶に贞操带なく、1/3の確率、主人が童贞でない、主人が受でない、２人的どちらかが攻でない
;３人阴茎无なら、ペニバン必要、誰かが受ではない
;二人が男性无でV经验20以上or男性でA经验50以上＋バックナー镇尼でない（处女やぶり防止）
;二人的ホモor百合属性1以上
;奴隶と助手の関係が良好
IF ASSI > 0 && TEQUIP:下身内衣 != 5 && RAND:3 == 0 && !TALENT:MASTER:童贞 && !TALENT:MASTER:受 && (!TALENT:攻 || !TALENT:ASSI:攻) && !TALENT:嫉妒 && !TALENT:ASSI:嫉妒
IF (PENIS(TARGET) && !TALENT:受) || (PENIS(ASSI) && !TALENT:ASSI:受) || ((PENIS(MASTER) || ITEM:PBAND) && !TALENT:MASTER:受)
IF (!TALENT:男性 && EXP:0 >= 20) || (TALENT:男性 && EXP:1 >= 50 && !TALENT:后庭处子)
IF KANRAKU(TARGET) && RELATION:(NO:ASSI) > 100 && RELATION:ASSI:(NO:TARGET) > 100
IF (!TALENT:ASSI:男性 && EXP:ASSI:0 >= 20) || (TALENT:ASSI:男性 && EXP:ASSI:1 >= 50 && !TALENT:ASSI:后庭处子)
IF ((ABL:20 >= 1 && MALE(TARGET)) || (ABL:11 >= 1 && !MALE(TARGET))) && ((ABL:ASSI:20 >= 1 && MALE(ASSI)) || (ABL:ASSI:11 >= 1 && !MALE(ASSI)))
	CALL MORNING_EVENT_8
	A = 1
ENDIF
ENDIF
ENDIF
ENDIF
ENDIF
ENDIF
TARGET = C

;朝料理判定 
IF TALENT:相亲相爱 && RAND:3 == 0
	CALL SELF_MORNING_4
ENDIF

;朝の入浴
IF ABL:侍奉精神 >= 7 && ABL:顺从 >= 5 && RAND:3 == 0
	CALL MORNING_EVENT_7
ENDIF

IF RAND:3 == 0 && A == 0
	;主人に阴茎がある
	IF (TALENT:MASTER:137 || TALENT:MASTER:139 || TALENT:MASTER:140)
		$PENIS_YES_EVENT
		R = RAND:6
		;接吻で起こされる（相亲相爱のみ）
		IF R == 0 && TALENT:6
			CALL SELF_MORNING_6
		;朝乳交判定
		ELSEIF R == 1 && ABL:7 >= 5 && !MALE(TARGET) && TALENT:110 == 0
			CALL SELF_MORNING_2
		;朝足交侍奉判定（施虐狂持ちのみ）
		ELSEIF R == 2 && ABL:7 >= 5 && TALENT:83
			CALL SELF_MORNING_5
		;朝骑乘位判定 侍奉精神７以上＋V感覚３以上or男性でA感覚３以上	;调教对象が攻、主人が受だと駄目
		ELSEIF R == 3 && ABL:侍奉精神 >= 7 && (ABL:Ｖ感觉 >= 3 || (TALENT:男性 && ABL:Ａ感觉 >= 3)) && !TALENT:158 && !TALENT:MASTER:157
			CALL SELF_MORNING_3
		;朝饮尿判定（スカ经验３０以上）
		ELSEIF R == 4 && EXP:33 >= 30
			CALL SELF_MORNING_9
		;朝口交／舔阴判定
		ELSEIF R == 5
			CALL SELF_MORNING_1
		ELSE
			GOTO PENIS_YES_EVENT
		ENDIF
	ELSE
		$PENIS_NO_EVENT
		R = RAND:4
		;接吻で起こされる（相亲相爱のみ）
		IF R == 0 && TALENT:6
			CALL SELF_MORNING_6
		;朝骑乘位判定 ペニバンあり。处女、再生处女はだめ。侍奉精神７以上＋V感覚３以上or男性で尻穴狂	;调教对象が攻、主人が受だと駄目
		ELSEIF R == 1 && ITEM:PBAND && ABL:侍奉精神 >= 7 && (ABL:Ｖ感觉 >= 3 || (TALENT:男性 && TALENT:尻穴狂)) && !TALENT:158 && !TALENT:MASTER:157
			CALL SELF_MORNING_3
		;朝饮尿判定（スカ经验３０以上）
		ELSEIF R == 2 && EXP:33 >= 30
			CALL SELF_MORNING_9
		;朝口交／舔阴判定
		ELSEIF R == 3
			CALL SELF_MORNING_1
		ELSE
			GOTO PENIS_NO_EVENT
		ENDIF
	ENDIF
ENDIF



