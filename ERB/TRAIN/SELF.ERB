A = 0
TFLAG:13 = 1
CALL KOJO_MESSAGE_EVENT

;精液成瘾
IF ABL:16 == 0
	A = 1
ELSEIF ABL:16 == 1
	A = 2
ELSEIF ABL:16 == 2
	A = 3
ELSEIF ABL:16 == 3
	A = 4
ELSEIF ABL:16 == 4
	A = 5
ELSEIF ABL:16 >= 5
	A = 6
ENDIF

;污臭钝感
SIF TALENT:61
	A += 1
;污臭敏感
SIF TALENT:62
	A -= 1
;自我奉献
SIF TALENT:63
	A += 1
;精液绝顶经验
SIF EXP:24
	A += 3	
SIF A == 0
	A = 1

;主人的竿の有無
IF TALENT:MASTER:140 || TALENT:MASTER:139 || TALENT:MASTER:137
	EXP:口交经验 += A
	PRINTFORML %EXPNAME:22%＋{A}
	;孩童尺寸で無い
	IF TALENT:MASTER:孩童尺寸== 0
		EXP:精液经验 += A/2
		PRINTFORML %EXPNAME:20%＋{A/2}
		;精液绝顶经验
		IF EXP:精饮绝顶经验
			PRINTFORML %EXPNAME:24%＋1
			EXP:精饮绝顶经验 += 1
		ENDIF
	ENDIF
ELSE
	EXP:舔阴经验 += A
	PRINTFORML %EXPNAME:25%＋{A}
	;侍奉快乐经验
	IF EXP:侍奉快乐经验 >= 50
		PRINTFORML %EXPNAME:21%＋1
		EXP:侍奉快乐经验 += 1
	ENDIF
