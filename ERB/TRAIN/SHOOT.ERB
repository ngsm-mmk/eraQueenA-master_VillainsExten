	SIF P == ASSI
		A = 103
	IF TFLAG:2 == 2 || TFLAG:6 == 2 || TFLAG:7 == 2
		IF NO:P == 0
			PRINTFORM %CALLNAME:MASTER%
		ELSE
			PRINTFORM %CALLNAME:P%
		ENDIF
		PRINTL 　阴道内大量射精
