		EXP:T:8 += 2
		;Omega
		SIF TALENT:T:164
		CFLAG:T:A += 2
	ELSEIF TFLAG:2 == 1 || TFLAG:7 == 1
		IF NO:P == 0
			PRINTFORM %CALLNAME:MASTER%
		ELSE
			PRINTFORM %CALLNAME:P%
		ENDIF
		PRINTL 　肠内射精
		PRINTFORML %CALLNAME:T%的肛射经验+１
		EXP:T:8 += 1
		;Omega
		SIF TALENT:T:164
		CFLAG:T:A += 1
	ENDIF
;避孕套をしてないかつＶ系なら中出し
ELSEIF TFLAG:703 == 1 || (TFLAG:703 == 6 && TFLAG:705 == 0)
