@SAMEN_SHOOT_CALC
;射精時の共通処理
;精液汚れの设定
STAIN:P:2 |= 4

;大量射精
IF E == 2
	SOURCE:液体 += 5000
	IF NO:P == 0
		PRINTFORM %CALLNAME:MASTER%
	ELSE
		PRINTFORM %CALLNAME:P%
	ENDIF
	PRINTL 　大量射精

	EXP:P:3 += 2
	BASE:P:2 -= EJAC*2
	SIF BASE:P:2 >= EJAC
		BASE:P:2 = EJAC-1
;通常の射精
ELSEIF E == 1
	SOURCE:液体 += 5000
	IF NO:P == 0
		PRINTFORM %CALLNAME:MASTER%
	ELSE
		PRINTFORM %CALLNAME:P%
	ENDIF
	PRINTL 　射精

	EXP:P:3 += 1
	BASE:P:2 -= EJAC
	SIF BASE:P:2 >= EJAC
		BASE:P:2 = EJAC-1
ENDIF

;射精時に中毒充足のソースを追加
;通常射精時
SIF E == 1
	A = 200
;大量射精時
SIF E == 2
	A = 500

IF E > 0
	;精液成瘾を見る
	IF ABL:16 == 1
		TIMES A , 0.00
	ELSEIF ABL:16 == 2
		TIMES A , 0.30
	ELSEIF ABL:16 == 3
		TIMES A , 0.60
	ELSEIF ABL:16 == 4
		TIMES A , 0.90
	ELSEIF ABL:16 == 5
		TIMES A , 1.20
	ELSEIF ABL:16 == 6
		TIMES A , 1.50
	ELSEIF ABL:16 == 7
		TIMES A , 1.80
	ELSEIF ABL:16 == 8
		TIMES A , 2.30
	ELSEIF ABL:16 == 9
		TIMES A , 3.00
	ELSEIF ABL:16 == 10
		TIMES A , 5.00
	ENDIF
ENDIF

;ゴム射フラグがあったら中毒充足しない
SIF CFLAG:T:106 != 9
	SOURCE:9 += A

;射精後は再装填の时间を設ける
;通常射精時
SIF E == 1
	CFLAG:P:9 = 3
;大量射精時
SIF E == 2
	CFLAG:P:9 = 5
CFLAG:P:11 -= 1

