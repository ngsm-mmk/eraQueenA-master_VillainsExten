@SAMEN_SHOOT
;ぶっかけ選択
;[0] 髪 手脚区分
;[1] 顔	手脚区分
;[2] 胸	口交・乳交区分
;[3] 腹	口交・乳交区分
;[4] 腋 口交・乳交2区分
;[5] 腿 做爱2区分
;[6] 割れ目 做爱2区分
;[7] 足　口交・乳交区分
;[8] ゴム射・中田氏

;各指令中で射精の判定に使われる変数Eを使用する。
;これまでの射精の処理をここに持たせ、各指令内で使わないようにする。
;よってここでEを初期化し、各COMFに処理を返す（E == 0ならば各指令内で射精の処理は行われない）

;射精した人物の设定
DEBUGPRINTFORML 射精角色设定
IF TFLAG:6
DEBUGPRINTFORML P = ASSI
	P = ASSI
ELSE
DEBUGPRINTFORML P = PLAYER
	P = PLAYER
ENDIF

;射精する对象の设定
DEBUGPRINTFORML 射精对象设定 SELECTCOM=={SELECTCOM} TFLAG:703=={TFLAG:703}
IF SELECTCOM == 215 && P == ASSI
DEBUGPRINTFORML T = MASTER
	T = MASTER
ELSEIF (SELECTCOM == 185 && P != ASSI) || SELECTCOM == 215
DEBUGPRINTFORML T = ASSI
	T = ASSI
ELSE
DEBUGPRINTFORML T = TARGET
	T = TARGET
ENDIF

;まずぶっかけ場所判別フラグを初期化
CFLAG:T:106 = 0
;---------------------------------------------------------
;追加部分、射精箇所選択機能をOFFにした際に避孕套を含めた通常の射精処理を行う
;また、状況的に射精箇所の選択を行えないであろう指令の場合もここで通常処理に飛ばす
;---------------------------------------------------------
$INPUT_LOOP

;竿の有無で分岐
IF TALENT:P:140 || TALENT:P:139 || TALENT:P:137
	;子供は射精できない
	IF TALENT:P:114 || TALENT:P:机器人 || TALENT:P:人偶
		CALL KARA_SHOOT_CHECK
		RETURN 0
	ELSEIF FLAG:99 == 8 || TFLAG:704 == 0
		;避孕套装备中
		IF TEQUIP:P:35 == 1
			CFLAG:T:106 = 9
		;通常射精
		ELSE
			CALL COMMAND_CHECK
			RETURN 0
		ENDIF
	ELSEIF FLAG:99 < 8
		IF FLAG:99 == 0
			CFLAG:T:106 = 1
		ELSEIF FLAG:99 == 1
			CFLAG:T:106 = 2
		ELSEIF FLAG:99 == 2
			CFLAG:T:106 = 3
		ELSEIF FLAG:99 == 3
			CFLAG:T:106 = 4
		ELSEIF FLAG:99 == 4
			CFLAG:T:106 = 5
		ELSEIF FLAG:99 == 5
			CFLAG:T:106 = 6
		ELSEIF FLAG:99 == 6
			CFLAG:T:106 = 7
		ELSEIF FLAG:99 == 7
			CFLAG:T:106 = 8
		ELSEIF FLAG:99 == 8
			;避孕套付きの場合
			IF TEQUIP:P:35 == 1
				CFLAG:T:106 = 9
			ELSE
				CALL COMMAND_CHECK
				RETURN 0
			ENDIF
		ENDIF
	ELSE
		PRINTFORMW %CALLNAME:P%的阴茎已膨胀至极限，到达了射精的临界点……
		PRINTFORMW 　
		PRINTFORMW 要在%CALLNAME:T%的哪里射精？
		PRINTL [0] 「发」
		PRINTL [1] 「脸」
		PRINTL [2] 「胸」
		PRINTL [3] 「腹」
		PRINTL [4] 「腋」
		PRINTL [5] 「腿」
		PRINTL [6] 「股间」
		PRINTL [7] 「屁股」
		PRINTL [8] 「足」
		IF TEQUIP:P:35
			PRINTL [9] 「射在避孕套里」
		ELSE
			PRINTL [9] 「直接射精」
		ENDIF

		INPUT
		IF RESULT == 0
			CFLAG:T:106 = 1
		ELSEIF RESULT == 1
			CFLAG:T:106 = 2
		ELSEIF RESULT == 2
			CFLAG:T:106 = 3
		ELSEIF RESULT == 3
			CFLAG:T:106 = 4
		ELSEIF RESULT == 4
			CFLAG:T:106 = 5
		ELSEIF RESULT == 5
			CFLAG:T:106 = 6
		ELSEIF RESULT == 6
			CFLAG:T:106 = 7
		ELSEIF RESULT == 7
			CFLAG:T:106 = 8
		ELSEIF RESULT == 8
			CFLAG:T:106 = 10
		ELSEIF RESULT == 9
			;避孕套付きの場合
			IF TEQUIP:P:35 == 1
				CFLAG:T:106 = 9
			ELSE
				CALL COMMAND_CHECK
				RETURN 0
			ENDIF
		ELSE
			GOTO INPUT_LOOP
		ENDIF
	ENDIF

ELSE
;女の場合の绝顶は子供の「空射精」と同じ処理でいい。
	CALL KARA_SHOOT_CHECK
	RETURN 0
ENDIF



