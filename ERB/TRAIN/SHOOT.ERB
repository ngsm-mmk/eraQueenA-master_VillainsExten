	ENDIF
	PRINTL 　避孕套内射精
	CALL SAMEN_SHOOT_CONDOM
;Ａ系中出しがあってもいいと思う
ELSEIF TFLAG:703 == 2 || (TFLAG:703 == 6 && TFLAG:705 == 1)
