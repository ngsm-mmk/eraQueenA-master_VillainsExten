	LOCAL = 0
	REPEAT CHARANUM
		SIF COUNT != MASTER && (CFLAG:(2000+NO:COUNT) == 5 || CFLAG:(2000+NO:COUNT) == 6)
			LOCAL = COUNT
	REND
	;居なかったら次に一番相性の高いヤツを探す　150以上ないと駄目　主人以外
	IF LOCAL == 0 
		REPEAT CHARANUM
			SIF COUNT != MASTER && RELATION:COUNT:(NO:TARGET) >= 150 && RELATION:COUNT:(NO:TARGET) >= RELATION:LOCAL:(NO:TARGET)
				LOCAL = COUNT
		REND
	ENDIF
	IF LOCAL > 0 
		PRINTL
		IF (CFLAG:(2000+NO:LOCAL) == 5 || CFLAG:(2000+NO:LOCAL) == 6)
			IF MALE(TARGET)
				PRINT 儿子
			ELSE
