        ELSEIF CFLAG:2410 == 2
            J = 23000
        ELSEIF CFLAG:2410 == 3 || CFLAG:2410 == 4 || CFLAG:2410 == 5
            J = 53000
        ELSEIF CFLAG:2410 == 6 || CFLAG:2410 == 7 || CFLAG:2410 == 8
            J = 128000
        ELSEIF CFLAG:2410 == 9 || CFLAG:2410 == 10 || CFLAG:2410 == 11
            J = 278000
        ENDIF
        M += J

