;劳役指示
@CHARA_WORKING
CALL KAIGYO
A = 0
B = 0
D = 0
E = 0
F = 0
G = 0
J = 0
REPEAT CHARANUM
	SIF COUNT == MASTER
		CONTINUE
	SIF CFLAG:COUNT:1 != 2
		CONTINUE
	SIF CFLAG:COUNT:12 > 0
		CONTINUE
	;育児部屋に移动しているキャラは除外
	C = COUNT
	CALL CHECK_CHILD_CARE
	SIF RESULT == 1
		CONTINUE
	SIF (TALENT:COUNT:3 || TALENT:COUNT:6) && BASE:COUNT:0 >= 800
		A += 1
	SIF (TALENT:COUNT:4 || TALENT:COUNT:7) && TIME == 0 && BASE:COUNT:0 >= 500
		B += 1
	IF (TALENT:COUNT:5 || TALENT:COUNT:8) && TIME == 0 && BASE:COUNT:0 >= 500
		D += 1
		SIF (TALENT:COUNT:怀孕 && CFLAG:COUNT:110 - 11 <= DAY) || TALENT:COUNT:育儿中
			D -= 1
	ENDIF
	SIF KANRAKU(COUNT) && ABL:COUNT:14 >= 2 && BASE:COUNT:0 >= 800
		E += 1
	SIF (TALENT:COUNT:4 || TALENT:COUNT:7) && BASE:COUNT:0 > 800
		F += 1
	SIF (TALENT:COUNT:5 || TALENT:COUNT:8) && TALENT:COUNT:158 == 0 && BASE:COUNT:0 >= 300
		G += 1
	SIF (TALENT:COUNT:3 || TALENT:COUNT:6 || TALENT:COUNT:4 || TALENT:COUNT:7 || TALENT:COUNT:5 || TALENT:COUNT:8) && BASE:COUNT:0 >= 500 && !TIME
		J += 1
	SIF (TALENT:COUNT:4 || TALENT:COUNT:7 || TALENT:COUNT:5 || TALENT:COUNT:8) && BASE:COUNT:0 >= 500 && TIME
		J += 1
REND
IF TARGET > 0 && TARGET != MASTER
	SIF (TALENT:3 || TALENT:6) || ABL:0 >= 2
		H = 1
ENDIF
SIF ABL:MASTER:2 > 3 && (FLAG:30 + FLAG:31 + FLAG:32) >= 3
	I = 1
DRAWLINE
PRINTL 请选择要进行的劳役
DRAWLINE
;调教者变更に对応
SIF MASTER == 0 || CFLAG:MASTER:1 == 3
	PRINTL [  0] - 拍卖
SIF A
	PRINTL [  1] - 经营售餐棚
SIF B
	PRINTL [  2] - 娼馆打工
SIF D
	PRINTL [  3] - 出租奴隶
SIF E
	PRINTL [  4] - 演唱会
SIF F
	PRINTL [  5] - 乱交LIVE
SIF G
	PRINTL [  6] - 公共便所PLAY
SIF J
	PRINTL [  7] - 画室打工
SIF FLAG:28
	PRINTL [ 10] - 让见习调教师独立
IF H
	SIF TARGET != -1
	PRINTFORML [ 50] - 让%CALLNAME:TARGET%做饭
ENDIF
SIF I
	PRINTL [ 51] - 自己做饭
DRAWLINE
PRINTL [100] - 返回
$INPUT_LOOP
INPUT
IF RESULT == 100
	RETURN 0
ELSEIF RESULT == 0
	CALL CHARA_SELL
ELSEIF RESULT == 1 && A
	CALL LUNCH_STALL
ELSEIF RESULT == 2 && B
	CALL PROSTITUTION
ELSEIF RESULT == 3 && D
	CALL RENTAL_SLAVE
ELSEIF RESULT == 4 && E
	CALL CONCERT
ELSEIF RESULT == 5 && F
	CALL PROMISCUOUS_SEX_LIVE
ELSEIF RESULT == 6 && G
	CALL PUBLIC_TOILET
ELSEIF RESULT == 7 && J
    CALL NUDE_MODEL
ELSEIF RESULT == 10 && FLAG:28
	CALL MINARAI_HITORIDACHI
ELSEIF RESULT == 50 && H
	CALL LUNCH_JUDGE
ELSEIF RESULT == 51 && I
	CALL LUNCH_SELF
ELSE
	GOTO INPUT_LOOP
ENDIF

RETURN 0

@MINARAI_HITORIDACHI
DRAWLINE
PRINTL 要让谁独立出去？
PRINTL 
FLAG:28 = 0
REPEAT CHARANUM
	IF CFLAG:COUNT:35
		FLAG:28 += 1
		IF COUNT < 10
			PRINTFORML [ {COUNT}] %CALLNAME:COUNT%
		ELSE
			PRINTFORML [{COUNT}] %CALLNAME:COUNT%
		ENDIF
	ENDIF
REND
PRINTL [999] 返回
INPUT
SIF RESULT == 999 || !CFLAG:RESULT:35
	RETURN 0
FLAG:28 -= 1
TFLAG:403 = TARGET
SIF ASSI == RESULT
	ASSI = -1
TARGET = RESULT

IF CFLAG:2404 == 1
    PRINTL
    PRINTFORML 房间里的%CALLNAME%被带出来了
    PRINTW
    CFLAG:2404 = 0
ELSEIF CFLAG:2414 == 1
    C = 0
    REPEAT CHARANUM
        SIF CFLAG:COUNT:(NO:TARGET+3000) == 1
            C = COUNT
    REND
    PRINTL
    PRINTFORML 在%CALLNAME:C%房间里的%CALLNAME%被带出来了
    PRINTW
    CFLAG:2414 = 0
    CFLAG:C:(NO:TARGET+3000) = 0
    CFLAG:C:2404 = 0
ENDIF

;イベント口上はひとまず无
PRINTFORMW %NAME%离开宅邸了
;見習いの父か母を捜す 主人以外
LOCAL = 0
REPEAT CHARANUM
	SIF COUNT != MASTER && (CFLAG:(2000+NO:COUNT) == 5 || CFLAG:(2000+NO:COUNT) == 6)
		LOCAL = COUNT
REND
;居なかったら次に一番相性の高いヤツを探す　150以上ないと駄目　主人以外
IF LOCAL == 0 
	REPEAT CHARANUM
		SIF COUNT != MASTER && RELATION:COUNT:(NO:TARGET) >= 150 && RELATION:COUNT:(NO:TARGET) >= RELATION:LOCAL:(NO:TARGET)
			LOCAL = COUNT
	REND
ENDIF
IF LOCAL > 0 
	PRINTL
	IF (CFLAG:(2000+NO:LOCAL) == 5 || CFLAG:(2000+NO:LOCAL) == 6)
		IF MALE(TARGET)
			PRINT 儿子（？）
		ELSE
			PRINT 女儿
		ENDIF
	ENDIF
	PRINTFORM %CALLNAME%离开后，
	SIF CFLAG:(2000+NO:LOCAL) != 5 && CFLAG:(2000+NO:LOCAL) != 6
		PRINTFORM 与%CALLNAME%关系很好的
	PRINTFORMW %CALLNAME:LOCAL%好像感到非常沮丧…
	SIF !TALENT:LOCAL:失意
		PRINTFORMW %CALLNAME:LOCAL%陷入了｢失意｣状态
	TALENT:LOCAL:失意 = 1
	CFLAG:LOCAL:36 += 20
ENDIF
;現在の子供の数フラグ－
SIF NO:TARGET >= 300
	FLAG:33--
DELCHARA TARGET
TARGET = -1
ASSI = -1

;奴隶拍卖の処理
@CHARA_SELL
DRAWLINE
PRINTL 要拍卖谁？
PRINTL 
REPEAT CHARANUM
	TFLAG:COUNT = 0
	SIF COUNT == MASTER
		CONTINUE
	SIF CFLAG:COUNT:1 == 3
		CONTINUE
	SIF CFLAG:COUNT:12 > 0
		CONTINUE
	;育児部屋に移动しているキャラは除外
	C = COUNT
	CALL CHECK_CHILD_CARE
	;烙印付きは売れない→売れるようにした
	;SIF TALENT:COUNT:159
	;	CONTINUE
	SIF RESULT == 1
		CONTINUE
	IF CFLAG:COUNT:1 >= 1 && CFLAG:COUNT:1 <= 3 && CFLAG:COUNT:7 == 0
		PRINTFORM [{COUNT}] %NAME:COUNT% 
		IF ASSI == COUNT
			PRINT (助手)
		ELSEIF ISASSI:COUNT == 1
			PRINT (原助手)
		ELSEIF CFLAG:COUNT:1 == 2
			PRINT (助手可)
		ENDIF
		PRINTL 
	ENDIF
REND
PRINTFORML [100] 返回

$INPUT_LOOP
INPUT

IF RESULT == 100
	RETURN 0
ELSEIF RESULT < 0 || RESULT >= CHARANUM
	GOTO INPUT_LOOP
ELSEIF CFLAG:RESULT:1 < 1 || CFLAG:RESULT:1 == 3 || CFLAG:RESULT:7 > 0 || CFLAG:RESULT:12 > 0 || RESULT == MASTER
	GOTO INPUT_LOOP
ELSE
	C = RESULT
	CALL CHECK_CHILD_CARE
	SIF RESULT == 1
		GOTO INPUT_LOOP
	;拍卖処理
	TFLAG:403 = TARGET
	TFLAG:404 = ASSI
	TARGET = C
ENDIF

