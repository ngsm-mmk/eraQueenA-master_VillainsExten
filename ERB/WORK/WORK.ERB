        IF CFLAG:2411 == 1
            J = 3000
        ELSEIF CFLAG:2411 == 2
            J = 8000
        ELSEIF CFLAG:2411 == 3 || CFLAG:2411 == 4 || CFLAG:2411 == 5
            J = 18000
        ELSEIF CFLAG:2411 == 6 || CFLAG:2411 == 7 || CFLAG:2411 == 8
            J = 43000
        ELSEIF CFLAG:2411 == 9 || CFLAG:2411 == 10 || CFLAG:2411 == 11
            J = 93000
        ENDIF
        M += J

        ;陷落售价减半
        IF KANRAKU(TARGET)
            TIMES M , 0.50
        ELSE
            TIMES M , 0.80
        ENDIF

        PRINTL

        IF M == 0
            PRINTFORMW %NAME:TARGET%个人房间里的家具没有人要
        ELSE
