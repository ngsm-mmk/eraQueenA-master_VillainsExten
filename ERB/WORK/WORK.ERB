	DELCHARA Z
	TARGET = -1
	ASSI = -1
ELSEIF RESULT == 1
	TARGET = TFLAG:403
	ASSI = TFLAG:404
	RETURN 0
ELSE
	GOTO INPUT_LOOPSALE
ENDIF


