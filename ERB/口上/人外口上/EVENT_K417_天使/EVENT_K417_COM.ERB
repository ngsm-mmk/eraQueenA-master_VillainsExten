		IF CFLAG:302 == 0
		;専用の台詞を書いて、汎用喘ぎを表示させたくない場合、台詞の最後にRETURN 0を付ける
		;RETURN 0
		;もう選んだことあるが、今回の调教では初めてこの指令を選んだ時
		ELSE
			;Ｃ系用　現在のパラで口上を分岐する
			;痛くて感じる余裕が無い&受虐狂ではない
			IF D >= 1 && C:4 <= 2 && TALENT:84 != 1 && ABL:9 < 3
			;Ｃ責めメインの時
			ELSEIF C:4 == 1 || (C:4 == 2 && PALAM_COMPARE(0) && C:0 > 0) && D == 0
				;殆どか、あまり感じていない
				IF (PALAM:0 + UP:0) < 500
				;少し感じ始めたか、感じてきている
				ELSEIF (PALAM:0 + UP:0) < 3000
				;感じている
				ELSE
				ENDIF
			;Ｃだけ绝顶寸前
			ELSEIF C:0 == 4 && C:1 < 4 && C:2 < 4 && C:3 < 4 && D == 0
			ENDIF
		ENDIF
	;今回の调教で二回目以降
	ELSEIF T:302 < 3 || FLAG:7 == 2
	ENDIF
ENDIF
